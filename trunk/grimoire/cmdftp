   VERSION=( "0.7.3" "0.7.2" )
    IGNORE="^0\.6\|^0\.5\|^0\.4"
  CATEGORY="file-transfer"
 ATTRIBUTE="console new"
    SOURCE="cmdftp/cmdftp-$VERSION"
  HOMEPAGE="http://cmdftp.sourceforge.net"
       URL="$SOURCEFORGE_URL"
  ESTIMATE="10"
     SHORT="cmdftp is a command line FTP clinet with shell-like functions."
      LONG="
cmdftp is a command line FTP client for Unix systems that features
shell-like functions, passive mode, local and remote transparent modes,
broken connection resumption, multiple and recursive file transfers,
auto-login, and more.
"
