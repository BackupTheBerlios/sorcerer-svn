   VERSION=( "1.4a12" )
  CATEGORY="network"
 ATTRIBUTE="console"
       URL="ftp://ftp.ee.lbl.gov"
  HOMEPAGE="ftp://ftp.ee.lbl.gov"
      DESC="traceroute traces the route Internet packets travel.

traceroute is a system administrators utility to trace the route ip
packets from the current system take in getting to some destination
system.
"


build() {
default_build  &&
chmod  0755  /usr/sbin/traceroute
}
