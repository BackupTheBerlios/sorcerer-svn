    stable 0.3 0.2.2 0.2.1 0.2 0.2rc1
   require libpcap
  category network security
 attribute console
    source $SOURCEFORGE_URL xprobe/xprobe2/Xprobe2%20$VERSION/xprobe2-$VERSION.tar.gz
  homepage http://www.sys-security.com/html/projects/X.html
  estimate 100
      desc 'xprobe2 is an operating system fingerprintg tool.
xprobe2 is an active operating system fingerprinting tool with
a different approach to operating system fingerprinting.
Xprobe2 rely on fuzzy signature matching, probabilistic guesses,
multiple matches simultaneously, and a signature database.'


build(){
unset CFLAGS CXXFLAGS LDFLAGS
default_build
}
