# supports gzip, bzip, bzip2, compress, lzop, zip, jar, lha, rar.

   version   stable 3.2.2 3.2.0 3.0.0
   version unstable 3.3.91 3.3.90 3.3.3
   require GConf bzip2 gst-python gzip nautilus perl--Encode perl--PathTools perl--Scalar-List-Utils zip
#  require GConf-dbus bzip2 gzip nautilus zip
  optional lha  '' '' for lha support
  optional lzop '' '' for lzop support
  category archive utility
 attribute gnome
    source $GNOME_URL sources/file-roller/${VERSION:0:3}/file-roller-$VERSION.tar.xz
      info last 20120130
      info home http://www.gnome.org
      desc 'GUI archive manager for the GNOME environment'
