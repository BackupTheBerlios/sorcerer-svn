    stable 1.15
   require zlib
  category archive
 attribute console
       url $SOURCEFORGE_URL
    source advancemame/advancecomp-$VERSION.tar.gz
    CFLAGS='-O3'
  CXXFLAGS='-O3'
   LDFLAGS='-Wl,-O1'
  homepage http://advancemame.sourceforge.net/comp-readme.html
  estimate 130
      desc 'recompression program for enhancing efficiency of .zip .gz .png and .mng files'

build_old(){
./configure          \
--prefix=/           \
--datadir=/usr/share \
--infodir=/usr/info  \
--mandir=/usr/man    \
CFLAGS="$CFLAGS"     \
CXXFLAGS="$CXXFLAGS" \
LDFLAGS="$LDFLAGS"   &&
make                 &&
prepare_install      &&
make    install
}
