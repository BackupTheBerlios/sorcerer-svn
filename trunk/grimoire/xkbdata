X_VERSION=X11R7.0
inherit xf86
   VERSION=( "${X_VERSION}-1.0.1" )
    SOURCE="data/xkbdata-$VERSION.tar.bz2"
       REQ="xkbcomp"
  ESTIMATE="134"
      DESC="xkbdata provides keyboard mappings"

build() {
 case $VERSION in
  X11R7.0-1.0.1)  sed -i 's:include \"cz(:include \"pc/cz(:'  symbols/pc/cz   ;;
 esac

 fix_sym_dir            &&
 default_build
}

