    stable 1.0.1 X11R7.0-1.0.1
  unstable 1.0.1
   require xkbcomp
  category xorg/app
 attribute X11R7 x11
#attribute deprecated
       url $XORG_URL
    source data/xkbdata-$VERSION.tar.bz2
  estimate 700
      desc 'Keyboard mappings'

build(){
case $VERSION in
 X11R7.0-1.0.1) sed -i 's:include \"cz(:include \"pc/cz(:' symbols/pc/cz ;;
         1.0.1) sed -i 's:include \"cz(:include \"pc/cz(:' symbols/pc/cz ;;
esac
default_build
}
