    stable 0.1.11 0.1.9 0.1.7
   require id3lib

    ignore '^1\.0$'
# 1.0 is an erronous report.
  category audio/editor
 attribute console
    source id3v2/id3v2-$VERSION.tar.gz
       url "$SOURCEFORGE_URL"
  homepage http://id3v2.sourceforge.net
  estimate 10
      desc 'id3v2 is a command line id3v2 tag editor.'

build(){
sed -i "s:-g:$CFLAGS -Wno-deprecated:"  Makefile
make clean
make            &&
prepare_install &&
install -vo root -g root -m 755 id3v2 /usr/bin
}
