# 1.0 is an erronous report.

    stable 0.1.12 0.1.11 0.1.9 0.1.7
   require id3lib
  category audio/editor
 attribute console
    source $SOURCEFORGE_URL id3v2/id3v2/$VERSION/id3v2-$VERSION.tar.gz
  homepage http://id3v2.sourceforge.net
      desc 'command line id3v2 tag editor'

build(){
sed -i "s:-g:$CFLAGS -Wno-deprecated:"  Makefile
make clean
make            &&
prepare_install &&
install -vo root -g root -m 755 id3v2 /usr/bin
}
