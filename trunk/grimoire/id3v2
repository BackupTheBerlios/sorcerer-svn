   VERSION=( "0.1.11" "0.1.9" "0.1.7" )
    IGNORE="^1\.0$"
# 1.0 is an erronous report.
  CATEGORY="mp3"
 ATTRIBUTE="console"
    SOURCE="id3v2/id3v2-$VERSION.tar.gz"
       URL="$SOURCEFORGE_URL"
  HOMEPAGE="http://id3v2.sourceforge.net"
       REQ="id3lib"
  ESTIMATE="11"
      DESC="id3v2 is a command line id3v2 tag editor."

build() {
sed  -i  "s:-g:$CFLAGS -Wno-deprecated:"  Makefile
make  clean
make             &&
prepare_install  &&
install  -o root  -g root  -m 755  id3v2  /usr/bin
}
