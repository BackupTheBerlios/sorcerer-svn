   VERSION=( "1.09" )
    SOURCE="/B/BR/BRYCE/${SPELL#perl--}-$VERSION.tar.gz"
  CATEGORY="perl"
 ATTRIBUTE="perl-module new"
  HOMEPAGE="http://search.cpan.org/search?dist=${SPELL#perl--}"
       URL="http://www.cpan.org/authors/id/"
       REQ=( "perl" )
  ESTIMATE="10"
      DESC="$SPELL organizes list of objects into parent/child order"

build() {
  true  |  default_pm_build
}
