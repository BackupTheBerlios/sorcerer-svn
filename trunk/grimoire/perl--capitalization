     PNAME=${SPELL#perl--}
   VERSION=( "0.03" )
  CATEGORY="perl"
 ATTRIBUTE="perl-module new"
    SOURCE="M/MI/MIYAGAWA/$PNAME-$VERSION.tar.gz"
       URL="http://www.cpan.org/authors/id/"
  HOMEPAGE="http://search.cpan.org/search?dist=$PNAME"
       REQ=( "perl perl--Devel-Symdump" )
  ESTIMATE="16"
      DESC="$SPELL - no capitalization on method names"

