   VERSION=( 0.9.12 0.9.11.2 0.9.11.1 0.9.10 )
  CATEGORY='security/authentication'
 ATTRIBUTE='console server'
    SOURCE="prelude-manager-$VERSION.tar.gz"
  HOMEPAGE='http://www.prelude-ids.org'
       URL='http://www.prelude-ids.org/download/releases/'
      VURL='http://www.prelude-ids.org/spip.php?rubrique6'
       REQ=( 'init-functions libprelude'
             'libpreludedb:::for database plugin'
             'libxml2:::for XML plugin'
             'tcp_wrappers:::for connection control by tcp_wrappers' )
  ESTIMATE='1000'
      DESC='prelude-manager provides a server that receives events emited by sensors.'


build_old(){
 default_build &&
 if   ! [ -d /etc/prelude/profile/prelude-manager ]
 then prelude-admin add prelude-manager --uid 0 --gid 0
 fi
}
