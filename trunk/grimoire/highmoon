with info    good 20130703
with info    last 20060325
with version stable 1.2.4
with base    SDL_image
with role    game
with trait   x11
with source  stable http://highmoon.gerdsmeier.net/highmoon-$VERSION.tar.gz
with info    home http://highmoon.gerdsmeier.net/
with info    cite 'spaceship gravity scorched earth type dueling game'

build(){
 make &&
 make install INSTALLBIN="$DESTDIR"/usr/bin \
              INSTALLPATH="$DESTDIR"/usr/share/games/HighMoon
}
