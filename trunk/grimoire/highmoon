   VERSION=( "1.1.2" "1.1.1" "1.1" )
  CATEGORY="game"
 ATTRIBUTE="x11 new"
    SOURCE="highmoon-$VERSION.tar.gz"
       URL="http://highmoon.gerdsmeier.net"
  HOMEPAGE="http://highmoon.gerdsmeier.net"
       REQ="SDL_image"
  ESTIMATE="12"
      DESC="highmoon is a spaceship gravity scorched earth type dueling game."

build() {
make             &&
prepare_install  &&
make    install  INSTALLBIN=/usr/games  \
                 INSTALLPATH=/usr/share/games/HighMoon
}
