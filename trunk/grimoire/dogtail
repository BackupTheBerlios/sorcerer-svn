    stable 0.4.3
   require Python
  category development
 attribute gnome
    source sources/dogtail/${VERSION:0:3}/dogtail-$VERSION.tar.bz2
  homepage http://www.gnome.org
       url "$GNOME_URL"
      opts --enable-static=no
  estimate 100
      desc 'dogtail provides a GUI test tool and automation framework
that uses Accessibility (a11y) technologies to communicate
with desktop applications.'

build(){
 prepare_install &&
 python setup.py install --prefix=/usr
}
