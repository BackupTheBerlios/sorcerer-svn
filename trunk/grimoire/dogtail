   version stable 0.4.3
   require Python
  category development
 attribute gnome
    source $GNOME_URL sources/dogtail/${VERSION:0:3}/dogtail-$VERSION.tar.bz2
  homepage http://www.gnome.org
      opts --enable-static=no
      desc 'GUI test tool and automation framework
that uses Accessibility (a11y) technologies
