   version stable 0.6.14
   require openjade
  category documentation
 attribute console
 attribute broke
    source ftp://sources.redhat.com/pub/docbook-tools/new-trials/SOURCES/docbook-utils-$VERSION.tar.gz
  homepage http://sources.redhat.com/docbook-tools/
      desc 'conversion of SGML to other formats'

build(){
 case $VERSION in
  0.6.14) sed -i 's:jade :openjade :' doc/HTML/Makefile.in ;;
 esac
 default_build
}
