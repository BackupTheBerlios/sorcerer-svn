   VERSION=( "0.6.14" )
  CATEGORY="documentation"
 ATTRIBUTE="console broken"
    SOURCE="docbook-utils-$VERSION.tar.gz"
       URL="ftp://sources.redhat.com/pub/docbook-tools/new-trials/SOURCES/"
  HOMEPAGE="http://sources.redhat.com/docbook-tools/"
       REQ="openjade"
  ESTIMATE="5"
      DESC="docbook-utils provides conversion of SGML to other formats."

build()  {
  case  $VERSION  in
    0.6.14)  sed  -i  "s:jade :openjade :"  doc/HTML/Makefile.in  ;;
  esac
  default_build
}
