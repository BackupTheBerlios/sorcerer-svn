   VERSION=( "2.13-pre7" "2.12r" )
    IGNORE="pre"
  CATEGORY="utility"
 ATTRIBUTE="console"
    SOURCE="pub/linux/utils/util-linux/testing/util-linux-$VERSION.tar.bz2"
       URL="$KERNEL_URL"
  HOMEPAGE="ftp://ftp.win.tue.nl/pub/linux-local/utils/util-linux/"
       REQ=""
  ESTIMATE="140"
      DESC="schedutils provides programs for adjusting process IO performance."

build()  {
  ./configure            \
  --build=$BUILD         \
  --prefix=/             \
  --sysconfdir=/etc      \
  --localstatedir=/var   \
  $OPTS                  &&
  cd      schedutils     &&
  make                   &&
  prepare_install        &&
  make    install
}
