   VERSION=( 1.3.5 1.3.4 1.3.2 1.3.0 1.2.7 1.2.6 1.2.4 )
    IGNORE='data\|osd\|PRE\|RC\|rc'
  CATEGORY='network/chat'
 ATTRIBUTE='client'
    SOURCE="licq/licq-$VERSION.tar.bz2"
       URL="$SOURCEFORGE_URL"
  HOMEPAGE='http://www.licq.org'
       REQ=( 'gpgme' 'openssl:::for secure socket connections' )
  ESTIMATE='700'
      DESC='licq is a featured icq client for Unix systems.
licq includes all the basic features of ICQ, like sending/receiving
messages, chat, file transfer, contact list with pixmaps and user
status, basic/extended user info, adding/editing users from within
the GUI, user history, user groups, and new user registration.'


build(){
 case $SPELL in
  licq-*) cd licq-$VERSION/plugins/${SPELL:5} ;;
 esac
 export PATH="/usr/bin/qt3:$PATH"
 default_build
}


case $SPELL in
 licq-*) DIR="none";  DESC="$SPELL provides a plugin for licq."
esac
