    stable 1.0.0b3
  category disk/cd
 attribute console
    source $SOURCEFORGE_URL linux-udf/udftools/$VERSION/udftools-$VERSION.tar.gz
  homepage http://sourceforge.net/projects/linux-udf/
  estimate 580
      desc 'udftools provides tools for packet writing to CD and DVDs.'

build(){
 case $VERSION in
  1.0.0b3) patch -p1 < $SCRIPT_DIR/udftools-1.0.0b3.patch ;;
 esac
 default_build
}
