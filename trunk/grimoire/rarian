    stable 0.8.0 0.6.0
   require libxslt

    ignore '^0\.9\.'
  category documentation
 attribute gnome
    source sources/rarian/${VERSION:0:3}/rarian-$VERSION.tar.bz2
  homepage http://www.gnome.org
       url "$GNOME_URL"
      opts --enable-static=no
   protect /var/log/scrollkeeper.log
