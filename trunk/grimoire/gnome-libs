    stable 1.4.2
   require ORBit db esound imlib libXpm rarian
  category development
 attribute gnome library
       url $GNOME_URL
    source sources/gnome-libs/1.4/gnome-libs-$VERSION.tar.bz2
  homepage http://www.gnome.org
   use_gcc gcc-v3.4
      opts --disable-gtk-doc
  estimate 2800
      desc 'gnome-libs provides libraries for gnome 1.4 programs.'

post_install(){ echo libglade-v0 | pipe_queue $CAST_QUEUE com; }
