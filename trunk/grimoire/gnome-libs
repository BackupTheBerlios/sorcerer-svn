  1.4.2)  USE_GCC='gcc-v3.4'  ;;
esac
       REQ='ORBit db esound imlib libXpm rarian'
      OPTS='--disable-gtk-doc'
  ESTIMATE='2800'
      DESC='gnome-libs provides libraries for gnome 1.4 programs.'

post_install(){ echo libglade-v0 | pipe_queue $CAST_QUEUE com; }
