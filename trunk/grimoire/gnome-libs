   version stable 1.4.2
   require ORBit db esound imlib libXpm rarian
      opts --disable-gtk-doc
  category development
 attribute gnome library
 attribute broke
    source $GNOME_URL sources/gnome-libs/1.4/gnome-libs-$VERSION.tar.bz2
  homepage http://www.gnome.org
   use_gcc gcc-v3.4
      desc 'libraries for gnome 1.4 programs'

post_install(){ echo libglade-v0 | pipe_queue $CAST_QUEUE; }
