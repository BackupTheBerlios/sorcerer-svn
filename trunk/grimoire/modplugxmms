   VERSION=( "2.05" "2.04" "2.0" )
  CATEGORY="audio"
 ATTRIBUTE="library x11"
    SOURCE="modplug-xmms/modplugxmms-$VERSION"
       URL="$SOURCEFORGE_URL"
  HOMEPAGE="http://modplug-xmms.sourceforge.net"
       REQ="automake libmodplug unrar xmms"
  ESTIMATE="291"
      DESC="modplugxmms is a ModPlug Plugin for XMMS v2.0.

modplugxmms enables xmms to play Amiga mod files.
"


build() {
aclocal
automake  -acf
autoconf
default_build
}
