# 0.10.4 and 0.10.5 require developmental versions of glibmm

    stable 0.10.8 0.10.7.3 0.10.7
   require glibmm gstreamer gst-plugins-base libxml++
      opts --enable-static=no
  category audio/library video/library
 attribute gnome library
    source $GNOME_URL sources/gstreamermm/${VERSION:0:4}/gstreamermm-$VERSION.tar.bz2
      home http://gstreamer.freedesktop.org/
      docs http://gstreamer.freedesktop.org/documentation/
#     mail http://gstreamer.freedesktop.org/lists/
  estimate 2000
      desc 'c++ wrapper for gstreamer C library'
