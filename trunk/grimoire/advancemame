# sdlmame probably supports a newer version of mame than advancemame

    stable 0.106.1
   require SDL
    CFLAGS='-O3 -fomit-frame-pointer'
   LDFLAGS='-Wl,-O1'
  category game development/emulator
 attribute x11
    source $SOURCEFORGE_URL advancemame/advancemame/$VERSION/advancemame-$VERSION.tar.gz
  homepage http://advancemame.sourceforge.net
  estimate 2400
      desc 'provides a port for the mame, multi arcade machine emulator'
