    stable 0.11.0 0.10.0 0.9.1 0.8.1
   require libogg
      opts --enable-static=no
  category audio/codec audio/library
 attribute library makej multilib-concurrent
    source http://downloads.xiph.org/releases/celt/celt-$VERSION.tar.gz
      home http://www.celt-codec.org/
      docs http://www.celt-codec.org/docs/
  estimate 600
      desc 'compression algorithm for audio'

build(){
 case $VERSION in
  0.11.0) sed -i 's:$OGG_LIBS -logg:-logg:' configure ;;
 esac
 default_build
}
