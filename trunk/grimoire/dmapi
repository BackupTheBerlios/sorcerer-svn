   VERSION=( "2.0.8" "2.0.5" "2.0.2" )
  CATEGORY="file-system"
 ATTRIBUTE="console"
    SOURCE="dmapi-$VERSION.src.tar.gz"
       URL="ftp://oss.sgi.com/projects/xfs/download/latest/cmd_tars"
  HOMEPAGE="http://oss.sgi.com/projects/xfs"
       REQ="xfsprogs"
  EPROVIDE="xfs,dmapi"
  ESTIMATE="108"
      DESC="dmapi provides the Data Management API for XFS Filesystems.

dmapi provides the data management API (DMAPI) for XFS Filesystem support.
"


build()  {

  ./configure  --prefix=/usr    \
               $OPTS            &&
  make  "DEBUG="                \
        "LDFLAGS=$LDFLAGS"      &&
  prepare_install               &&
  make    install  install-dev

}
