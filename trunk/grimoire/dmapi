    stable 2.2.8-1 2.2.8-1 2.2.5-1
   require xfsprogs
  category disk/file-system
 attribute console
#   source dmapi-$VERSION.src.tar.gz
    source dmapi_$VERSION.tar.gz
       url ftp://oss.sgi.com/projects/xfs/cmd_tars/
  homepage http://oss.sgi.com/projects/xfs
  estimate 200
      desc 'dmapi provides the Data Management API for XFS Filesystems.'

build(){
 export OPTIMIZER="$CFLAGS"
 unset              CFLAGS
 export DEBUG=-DNDEBUG

 make  configure &&
 make            &&
 prepare_install &&
 make    install install-dev
#make    install install-lib install-dev
}
