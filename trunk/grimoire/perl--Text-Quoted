     PNAME=${SPELL#perl--}
   VERSION=( "1.8" )
  CATEGORY="perl"
 ATTRIBUTE="perl-module new"
    SOURCE="J/JE/JESSE/$PNAME-$VERSION.tar.gz"
       URL="http://www.cpan.org/authors/id/"
  HOMEPAGE="http://search.cpan.org/search?dist=$PNAME"
       REQ=( "perl perl--Text-Autoformat" )
  ESTIMATE="15"
      DESC="$SPELL module extracts the structure of a quoted mail message"

