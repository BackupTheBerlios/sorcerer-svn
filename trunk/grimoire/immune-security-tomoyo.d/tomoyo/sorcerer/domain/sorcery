<kernel> /domain/limited
use_profile 3

allow_read /\{\*\}/\*
allow_ioctl /\{\*\}/\* 0-0xFFFFFFFF
allow_ioctl anon_inode:inotify 0x541B
allow_ioctl pipe:[\*] 0-0xFFFFFFFF
allow_ioctl socket:[family=2:type=2:protocol=17] 0x541B
allow_ioctl socket:[family=1:type=1:protocol=0] 0x541B
allow_ioctl socket:[family=1:type=2:protocol=0] 0x8933
allow_ioctl socket:[family=1:type=1:protocol=0] 0x5421
allow_execute /domain/freedom
allow_execute /media/root/domain/freedom
allow_read/write /sys/kernel/security/tomoyo/.process_status
allow_read/write /media/root/sys/kernel/security/tomoyo/.process_status
allow_read/write @DEV-NOMEM
allow_chgrp @DEV-PTS 0-0xFFFFFFFF
allow_chown @DEV-PTS 0-0xFFFFFFFF
allow_chmod @DEV-PTS 0-0x0FFF
allow_chgrp @DEV-TTY 0-0xFFFFFFFF
allow_chown @DEV-TTY 0-0xFFFFFFFF
allow_chmod @DEV-TTY 0-0x0FFF
allow_execute @EXE
allow_execute @TMP
allow_create @KBD 0-0x0FFF
allow_read/write @KBD
allow_unlink @KBD
allow_chgrp @LOCK-RUN 0-0xFFFFFFFF
allow_chmod @LOCK-RUN 0-0x0FFF
allow_chown @LOCK-RUN 0-0xFFFFFFFF
allow_create @LOCK-RUN 0-0x0FFF
allow_mkdir @LOCK-RUN 0-0x0FFF
allow_mkfifo @LOCK-RUN 0-0x0FFF
allow_mksock @LOCK-RUN 0-0x0FFF
allow_read/write @LOCK-RUN
allow_rename @LOCK-RUN @LOCK-RUN
allow_rmdir @LOCK-RUN
allow_symlink @LOCK-RUN
allow_truncate @LOCK-RUN
allow_unlink @LOCK-RUN
allow_create @LOGS 0-0x0FFF
allow_read/write @LOGS
allow_rename @LOGS @LOGS
allow_read/write @MTRR
allow_read/write @PROC-OOM
allow_truncate @PROC-OOM
allow_read/write @SYS-DEVICES
allow_chgrp @SYS-FS 0-0xFFFFFFFF
allow_chown @SYS-FS 0-0xFFFFFFFF
allow_create @SYS-FS 0-0x0FFF
allow_mkdir @SYS-FS 0-0x0FFF
allow_read/write @SYS-FS
allow_rmdir @SYS-FS
allow_truncate @SYS-FS
allow_create @SYSTEM-CONFIG-PRINTER 0-0x0FFF
allow_chgrp @TMP 0-0xFFFFFFFF
allow_chown @TMP 0-0xFFFFFFFF
allow_chmod @TMP 0-0x0FFF
allow_create @TMP 0-0x0FFF
allow_link @TMP @TMP
allow_mkdir @TMP 0-0x0FFF
allow_mkfifo @TMP 0-0x0FFF
allow_mksock @TMP 0-0x0FFF
allow_read/write @TMP
allow_rename @TMP @TMP
allow_rmdir @TMP
allow_symlink @TMP
allow_truncate @TMP
allow_unlink @TMP
allow_chgrp @USR-SRC 0-0xFFFFFFFF
allow_chmod @USR-SRC 0-0x0FFF
allow_chown @USR-SRC 0-0xFFFFFFFF
allow_create @USR-SRC 0-0x0FFF
allow_execute @USR-SRC
allow_link @USR-SRC @USR-SRC
allow_mkdir @USR-SRC 0-0x0FFF
allow_mkfifo @USR-SRC 0-0x0FFF
allow_mksock @USR-SRC 0-0x0FFF
allow_read/write @USR-SRC
allow_rename @USR-SRC @USR-SRC
allow_rmdir @USR-SRC
allow_symlink @USR-SRC
allow_truncate @USR-SRC
allow_unlink @USR-SRC
allow_chgrp @VAR 0-0xFFFFFFFF
allow_chmod @VAR 0-0x0FFF
allow_chown @VAR 0-0xFFFFFFFF
allow_create @VAR 0-0x0FFF
allow_link @VAR @VAR
allow_mkdir @VAR 0-0x0FFF
allow_mksock @VAR 0-0x0FFF
allow_read/write @VAR
allow_rename @VAR @VAR
allow_rmdir @VAR
allow_symlink @VAR
allow_truncate @VAR
allow_unlink @VAR
allow_chroot @VAR-EMPTY

