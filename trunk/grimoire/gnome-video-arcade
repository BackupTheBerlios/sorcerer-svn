   version stable 0.7.1
   require gstreamer libunique-v1 libwnck mame sqlite
  category development
 attribute gnome new
    source $GNOME_URL sources/gnome-video-arcade/${VERSION:0:3}/gnome-video-arcade-$VERSION.tar.bz2
  homepage http://www.gnome.org
  estimate 600
      desc 'front-end program for MAME, multi arcade machine emulator'

