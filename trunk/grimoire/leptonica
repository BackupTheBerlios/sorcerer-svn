with version stable 1.69 1.68
with base    giflib libpng tiff
with also    --disable-static
case $VERSION in
 1.69)
with also    --without-giflib ;;
 1.68)
with also    --without-libpng ;;
esac
with trait   library makej multilib-concurrent
with role    graphic/scan
with source  http://leptonica.googlecode.com/files/leptonica-$VERSION.tar.gz
with info    good 20120716
with info    last 20120709
with info    home http://code.google.com/p/leptonica
with info    home http://www.leptonica.com
with info    vurl http://code.google.com/p/leptonica/downloads/list
with info    cite 'image processing library'
