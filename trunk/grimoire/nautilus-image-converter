   version stable 0.3.0
   require nautilus
  category utility/file-manager
 attribute gnome
    source $GNOME_URL sources/nautilus-image-converter/${VERSION:0:3}/nautilus-image-converter-$VERSION.tar.bz2
  homepage http://www.gnome.org/projects/nautilus-image-converter
      desc 'nautilus-image-converter is a file manager for the GNOME desktop.
It also can be used to draw your desktop.'
