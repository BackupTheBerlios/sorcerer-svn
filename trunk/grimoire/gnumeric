   VERSION=( 1.8.3 1.8.2 1.8.1 )
    IGNORE='^1\.9\.'
  CATEGORY='utility/spreadsheet'
 ATTRIBUTE='gnome'
    SOURCE="sources/gnumeric/${VERSION%.*}/gnumeric-$VERSION.tar.bz2"
  HOMEPAGE='http://www.gnome.org/projects/gnumeric'
       URL="$GNOME_URL"
       REQ=( 'gal2 goffice gtksourceview rarian'
             'libgnomedb:::for accessing databases'
             'webfonts:::for editing excel spreadsheads created on windoze' )
# May have optional requirement on psiconv
  ESTIMATE='6500'
      DESC='gnumeric is a spreadsheet program.
gnumeric is intended to be a drop in
replacement for commercial spreadsheets.
Gnumeric will import your existing Excel,
1-2-3, Applix, Sylk, XBase and Oleo files.'
