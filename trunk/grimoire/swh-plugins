# installs in /usr/lib/ladspa

   version stable 0.4.14
   require fftw ladspa_sdk
  category audio/library
 attribute library makej new
 attribute multilib-fail
    source http://plugin.org.uk/releases/$VERSION/swh-plugins-$VERSION.tar.gz
  homepage http://plugin.org.uk/
      desc 'audio plugins for the LADSPA plugin system'

build(){
 export CFLAGS+=' -fPIC'
 default_build
}
