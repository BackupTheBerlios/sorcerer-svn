#     OPTS='--prefix=/opt/kde3 --with-qt-dir=/opt/kde3'
      OPTS='--prefix=/opt/kde3 --with-qt-dir=/opt/kde3 --disable-debug --enable-final'
  ESTIAMTE='146'
      DESC='ksambaplugin is a samba configuration plugin for KDE
It consists of a KControl Center Module for configuring nearly every
option on a SAMBA server and a Konqueror properties dialog plugin for
quick sharing of single directories.'

build(){
cd  ksambaplugin-$VERSION
default_build
}
