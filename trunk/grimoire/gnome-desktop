   version stable 3.2.0 3.0.2 3.0.0 2.91.93 2.32.1 2.32.0
   require gtk+-v3 startup-notification
      opts --enable-static=no --enable-gtk-doc=no --enable-gtk-doc-html=no --enable-gtk-doc-pdf=no
  category window-manager
 attribute gnome library makej multilib-concurrent
    source $GNOME_URL sources/gnome-desktop/${VERSION:0:3}/gnome-desktop-$VERSION.tar.xz
      info last 20111006
  homepage http://www.gnome.org
 conflicts libgnome libgnomemm libgnomeui libgnomeuimm libgnomeui libbonoboui libbonobouimm
      desc 'API lib for gnome'

build(){
 sed -i 's:hardcode_into_libs=yes:hardcode_into_libs=no:' configure
 default_build
}
