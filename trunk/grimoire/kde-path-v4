   VERSION=( 4.0.0 )
  CATEGORY='profile'
 ATTRIBUTE='kde4'
  HOMEPAGE='http://www.kde.org/'
  ESTIMATE='5'
      DESC='kde-path-v4 exports a PATH to where KDE is installed'

build(){
 prepare_install
 mkdir   -vp                              /etc/bash.d/interactive
 install -vm 0755 $SCRIPT_DIR/PATH-KDE-v3 /etc/bash.d/interactive
}
