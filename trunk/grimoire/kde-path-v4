    stable 4.0.0
  category profile
 attribute kde4
  homepage http://www.kde.org/
  estimate 5
      desc 'kde-path-v4 exports a PATH to where KDE is installed'

build(){
 prepare_install
 mkdir   -vp                              /etc/bash.d/interactive
 install -vm 0755 $SCRIPT_DIR/PATH-KDE-v4 /etc/bash.d/interactive
}
