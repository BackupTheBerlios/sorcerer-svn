# Requirement on avahi creates recursive requirement loop

# optional requirement on libgnomeui causes recursive requirement
# Appears to always use the internal gd library since it requires
# gd 2.0.34 which was not released.

    stable 2.20.2 2.20.0 2.18 2.16.1 2.16
   require Python gd libXaw ruby tk
  category graphic/editor
 attribute x11
 attribute broke
    source http://www.graphviz.org/pub/graphviz/ARCHIVE/graphviz-$VERSION.tar.gz
  homepage http://www.research.att.com/sw/tools/graphviz/
      opts --enable-static=no
case $VERSION in
 2.20.2) opts --enable-tcl=no ;;
esac
  estimate 4500
      desc 'graphviz is a set of graph drawing tools and libraries.'

build(){
 case $VERSION in
  2.18) sed -i 's:tclpkg::' Makefile.in ;;
 esac
 default_build
}
