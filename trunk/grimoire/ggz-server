inherit ggz
unset  REQ
    SOURCE="ggz-server-$VERSION.tar.gz"
case  $VERSION  in
  0.0.13)  USE_GCC="gcc-v4.0"  ;;
esac
       REQ=( "avahi ggz-grubby ggz-utils"
             "sqlite:::for sqlite database support" )
      OPTS="$OPTS --with-database=db4"
  ESTIMATE="1480"
      DESC="ggz-server provides servers for GGZ"

build()  {
  case  $VERSION  in
    0.0.9)  sed    -i   "s:create_4002:create:"  configure  ;;
  esac
  default_build
}
