   inherit ggz
   require avahi ggz-grubby ggz-utils
  optional sqlite '' '' for sqlite database support
      opts --with-database=db4
  estimate 1400
      desc 'ggz-server provides servers for GGZ'

build(){
 case $VERSION in
  0.0.14.1) sed -i '1i#include <cstring>' $( find . -name \*.cpp ) ;;
 esac
 default_build
}
