   version stable 0.2.3
   require cups glib
      opts --enable-static=no
  category development
 attribute gnome library makej multilib-concurrent
    source $GNOME_URL sources/libgnomecups/${VERSION:0:3}/libgnomecups-$VERSION.tar.bz2
  homepage http://www.gnome.org
      desc 'printing library to use cups from gnome'
