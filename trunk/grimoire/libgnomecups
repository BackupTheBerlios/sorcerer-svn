    stable 0.2.3 0.2.2 0.2.1 0.2.0
   require cups glib
  category development
 attribute gnome library
    source $GNOME_URL sources/libgnomecups/${VERSION:0:3}/libgnomecups-$VERSION.tar.bz2
  homepage http://www.gnome.org
      opts --enable-static=no
  estimate 400
      desc 'libgnomecups is a printing library to use cups from gnome.'
