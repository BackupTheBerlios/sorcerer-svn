# Disable introspection since introspection is broke with clutter

    stable 1.3.6 1.3.4 1.3.2 1.2.0
   require clutter gst-plugins-base
case $VERSION in
 1.3.4)
      opts --enable-introspection=no ;;
 1.3.2)
      opts --enable-introspection=no ;;
esac

  category audio/cd
 attribute gnome
    source http://source.clutter-project.org/sources/clutter-gst/${VERSION:0:3}/clutter-gst-$VERSION.tar.bz2
#   source $GNOME_URL sources/clutter-gst/${VERSION:0:3}/clutter-gst-$VERSION.tar.bz2
      info last  20110305
      info home  http://clutter-project.org/
      info docs  http://www.clutter-project.org/documentation
  estimate 200
      desc 'library for creating fast visually rich animated GUIs'
