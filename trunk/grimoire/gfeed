   require gnet gtk+
    ignore 'pre'
  category network/rss
 attribute server x11
       url $SOURCEFORGE_URL
    source gfeed/gfeed-$VERSION.tar.gz
  homepage http://www.gfeed.org
  estimate 50
      desc 'gfeed is an RSS feed reader for Linux using gtk+.
While highly customizable it is not an aggregator.'
