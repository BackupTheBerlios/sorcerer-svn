     PNAME=${SPELL#perl--}
   VERSION=( "0.55" )
  CATEGORY="perl"
 ATTRIBUTE="perl-module new"
    SOURCE="A/AU/AUDREYT/$PNAME-$VERSION.tar.gz"
       URL="http://www.cpan.org/authors/id/"
  HOMEPAGE="http://search.cpan.org/search?dist=$PNAME"
       REQ="perl gnupg perl--Digest-SHA"
  ESTIMATE="30"
      DESC="$SPELL performs module signature file manipulation"

