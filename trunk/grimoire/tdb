    stable 1.0.4
    ignore '^1\.0\.[5-6]\|f\|1\.0[3-5]\|class'
# 1.0.6 has compile errors
  category db
 attribute library new
       url $SOURCEFORGE_URL
    source tdb/tdb-$VERSION.tar.gz
  homepage http://sourceforge.net/projects/tdb
  estimate 100
 freshmeat tbs
      desc 'tdb is a trival database library.
It is very much like gdbm or db.'
