   version stable 2.6.3 2.6.2 2.6.1
   require gtk+
  optional Python '' '' for libglade-convert program
      opts --enable-static=no
  category development
 attribute gnome library makej multilib-concurrent
    source $GNOME_URL sources/libglade/${VERSION:0:3}/libglade-$VERSION.tar.bz2
  homepage http://www.gnome.org
  estimate 1200
      desc 'library for loading glade files at runtime using gtk+'
