    stable 0.9
   require Python

  category development
 attribute gnome
    source sources/strongwind/${VERSION:0:3}/strongwind-$VERSION.tar.bz2
       url "$GNOME_URL"
  homepage http://www.gnome.org
  estimate 30
      desc 'strongwind is a GUI for automated testing.'

build(){
prepare_install &&
python setup.py install
}
