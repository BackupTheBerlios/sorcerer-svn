     PNAME=${SPELL#perl--}
   VERSION=( "2.04" )
  CATEGORY="perl"
 ATTRIBUTE="perl-module new"
    SOURCE="S/SB/SBURKE/$PNAME-$VERSION.tar.gz"
       URL="http://www.cpan.org/authors/id/"
  HOMEPAGE="http://search.cpan.org/search?dist=$PNAME"
       REQ="perl--HTML-Tree perl--Font-AFM"
  ESTIMATE="30"
      DESC="$SPELL formats HTML as plaintext PostScript or RTF
HTML::FormatPS
HTML::FormatRTF
HTML::FormatText
HTML::Formatter"
