   version   stable 3.4.2
   version unstable 3.5.90
   require WebKit xz
      opts --enable-static=no
  category documentation
 attribute gnome library makej multilib-concurrent
    source   stable $GNOME_URL sources/yelp/${VERSION:0:3}/yelp-$VERSION.tar.xz
    source unstable $GNOME_URL sources/yelp/${VERSION:0:3}/yelp-$VERSION.tar.xz
      info last 20120517
      info home http://www.gnome.org
      desc 'gnome help browser'

build(){
 sed -i 's:hardcode_into_libs=yes:hardcode_into_libs=no:' configure
 default_build
}
