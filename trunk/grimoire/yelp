with version   stable 3.6.2 3.6.1 3.6.0
with base    WebKit xz
with also    --enable-static=no
with role    documentation
with trait   gnome library makej multilib-concurrent
with source    stable $GNOME_URL sources/yelp/${VERSION:0:3}/yelp-$VERSION.tar.xz
with source  unstable $GNOME_URL sources/yelp/${VERSION:0:3}/yelp-$VERSION.tar.xz
with info    last 20121117
with info    home http://www.gnome.org
with info    cite 'gnome help browser'

build(){
 sed -i 's:hardcode_into_libs=yes:hardcode_into_libs=no:' configure
 default_build
}
