 make install-hobbies $DP
 make install-household $DP
 make install-medical $DP
 make install-military $DP
 make install-naturalforces $DP
 make install-people $DP
 make install-plants $DP
 make install-seasonal $DP
 make install-space $DP
 make install-sports $DP
 make install-symbols $DP
 make install-town $DP
 make install-vehicles $DP
}
