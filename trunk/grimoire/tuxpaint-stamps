stable 2008.06.30 2008.03.01 2007.11.21 2007.07.01 2006.10.21 2005.11.25 2004-10-03 2003.12.23
inherit tuxpaint
IGNORE='noarch\|win\|beos'
ESTIMATE='50'
  /usr/local/share/tuxpaint/stamps

(Run each 'make install' with 'DATA_PREFIX=path' to override.)

build(){
 local DPDATA_PREFIX=/usr/share/tuxpaint/stamps

 make
 prepare_install
 mkdir -vp /usr/share/icons/hicolor/16x16/apps
 make install-animals $DP
 make install-clothes $DP
 make install-food $DP
 make install-hobbies $DP
 make install-household $DP
 make install-medical $DP
 make install-military $DP
 make install-naturalforces $DP
 make install-people $DP
 make install-plants $DP
 make install-seasonal $DP
 make install-space $DP
 make install-sports $DP
 make install-symbols $DP
 make install-town $DP
 make install-vehicles $DP
}
