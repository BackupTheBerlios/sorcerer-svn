   version stable 2.13.2 2.13.1 2.12.2
   version unstable 2.13.91 2.13.90 2.13.3
   require brasero clutter-gtk exiv2
  optional libgphoto2 '' --disable-gphoto2 for digital camera library
      opts --enable-static=no
  category graphic/viewer
