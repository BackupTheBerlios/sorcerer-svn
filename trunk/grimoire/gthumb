    source sources/gthumb/${VERSION%.*}/gthumb-$VERSION.tar.bz2
       url "$GNOME_URL"
  homepage http://www.gnome.org
      opts --enable-static=no
  estimate 3000
      desc 'gthumb provides an image viewer and browser utility for GNOME'
