    stable 20080321
  category documentation
 attribute archive_off data
  homepage http://sorcerer.silverice.org/
  estimate 10
      desc 'sorcery-documentation provides documentation or Sorcerer'

build(){
 prepare_install &&
 mkdir   -pm 0755 /usr/doc/sorcery &&
 install  -m 0644 $SCRIPT_DIR/sorcery-documentation /usr/doc/sorcery
}
