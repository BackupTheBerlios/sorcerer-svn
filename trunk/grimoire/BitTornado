    stable 0.3.17 0.3.7
   require Python
#optional wxPython '' '' for GUI client
# wxPython currently broke
    ignore '0\.3\.18'
# 0.3.18 is experimental
  category network/file-share
 attribute console python-module new
    source BitTornado-$VERSION.tar.gz
       url http://download2.bittornado.com/download/
  homepage http://bittornado.com/
  estimate 230
      desc 'BitTornado is a file transfer program.'


build(){
prepare_install &&
python setup.py install
}
