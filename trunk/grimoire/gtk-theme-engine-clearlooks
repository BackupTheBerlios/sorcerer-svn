   version stable 2.91.5
   require gtk+-v3
  category graphic
 attribute gnome library makej multilib-concurrent
    source $GNOME_URL sources/gtk-theme-engine-clearlooks/${VERSION:0:4}/gtk-theme-engine-clearlooks-$VERSION.tar.bz2
      info home http://www.gnome.org
      desc 'engines for use with GTK'

build(){
 sed -i 's:hardcode_into_libs=yes:hardcode_into_libs=no:' configure
 default_build
}
