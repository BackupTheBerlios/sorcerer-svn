   version stable 2.22.3
   require libgnomeui
  optional gnome-doc-utils '' '' for documentation generation
      opts --prefix=/opt/gucharmap/2.23
  category font/editor
 attribute gnome library
 attribute multilib-fail
    source $GNOME_URL sources/gucharmap/${VERSION:0:4}/gucharmap-$VERSION.tar.bz2
      info home  http://live.gnome.org/Gucharmap
      desc 'installs older version of gucharmap'
