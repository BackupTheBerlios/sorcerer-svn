   version stable 1.2 1.1.1
  category graphic/utility
 attribute library makej
    source $GNU_URL libxmi/libxmi-$VERSION.tar.gz
  homepage http://www.gnu.org/software/libxmi/
      desc 'GNU libxmi is a C/C++ function library for rasterizing 2-D vector graphics.
It can draw 2-D graphical primitives, including wide polygonal lines
and circular and elliptical arcs, into a user-supplied matrix of pixels.
Sophisticated line styles, such as multicolored dashing patterns, can be specified.
There is also support for filling and texturing polygons.'

build(){
 libtoolize --force &&
 aclocal    --force &&
 autoheader --force &&
 automake   --force-missing --add-missing --copy &&
 autoconf &&
 default_build
}
