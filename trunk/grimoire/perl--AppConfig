   VERSION=( "1.56" )
    SOURCE="/A/AB/ABW/${SPELL#perl--}-$VERSION.tar.gz"
  CATEGORY="perl"
 ATTRIBUTE="perl-module new"
  HOMEPAGE="http://search.cpan.org/search?dist=${SPELL#perl--}"
       URL="http://www.cpan.org/authors/id/"
       REQ=( "perl" )
  ESTIMATE="22"
      DESC="$SPELL are perl modules for reading configuration files
and parsing command line arguments."

build() {
  true  |  default_pm_build
}
