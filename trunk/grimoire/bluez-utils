   VERSION=( "2.25" )
  CATEGORY="mobile"
 ATTRIBUTE="library new"
    SOURCE="download/bluez-utils-$VERSION.tar.gz"
       URL="bluez.sf.net"
  HOMEPAGE="http://www.bluez.org/"
       REQ=( "bluez-libs libusb"
             "openobex:--enable-obex:--without-openobex:for openobex support" 
             "fuse:--enable-fuse:-without-fuse:for FUSE support" 
             "alsa-lib:--enable-alsa:--without-alsa:for ALSA support" 
             "cups:--enable-cups:--disable-cups:for Cups printing support" )
  ESTIMATE="174"
      DESC="bluez-utils provides official Linux Bluetooth utilities."
