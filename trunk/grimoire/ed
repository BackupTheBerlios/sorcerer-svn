# Unstable version might only be available on alpha.gnu.org ftp server?
# For version 1.2 a .tar.gz and tar.lz were provided, but not .tar.bz2
    stable 1.2 1.1 1.0 0.9 0.8 0.7 0.6
    secure 1.0
  unstable 1.1-rc1
  category utility/editor
 attribute console
       url $GNU_URL
    source ed/ed-$VERSION.tar.gz
  homepage http://www.gnu.org/software/ed/ed.html
      desc 'ed is a line-oriented text editor.'

build_old(){
./configure     \
--build=$BUILD  \
--prefix=/usr   \
$OPTS           &&
make            &&
prepare_install &&
make    install
}
