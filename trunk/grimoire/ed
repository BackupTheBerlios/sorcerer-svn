   VERSION=( "0.2" )
    IGNORE="b"
  CATEGORY="editor"
 ATTRIBUTE="console"
    SOURCE="ed/ed-$VERSION.tar.gz"
       URL="$GNU_URL"
  HOMEPAGE="http://www.gnu.org/software/ed/ed.html"
  ESTIMATE="64"
      DESC="ed is a line-oriented text editor."


build()  {
./configure  --build=$BUILD  \
             --prefix=/usr   \
             $OPTS           &&
make                         &&
prepare_install              &&
make    install
}
