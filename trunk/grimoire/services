version_year_month 12
  category network
 attribute client server stale
    source http://sorcerer.silverice.org/download/iana/services-$VERSION.tar.xz
      info home http://www.iana.org/
   protect /etc/services
      desc 'translates a tarball of port numbers originally from IANA into to /etc/services.d/IANA'

build(){
 mkdir -pvm 755 $DESTDIR/etc/services.d/
 [ -s services ] &&
 mv   services $DESTDIR/etc/services.d/IANA
}
