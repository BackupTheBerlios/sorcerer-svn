version_year_month 12
  category network
 attribute client server stale
    source http://sorcerer.silverice.org/download/iana/services-$VERSION.tar.bz2
  homepage http://www.iana.org/
   protect /etc/services
   exclude /etc/services.old
      desc 'services translates tarball of port numbers by IANA into to /etc/services'

build(){
 mkdir -p      $SCRIPT_DIR/services.d/
 [ -s services ] &&
 mv   services $SCRIPT_DIR/services.d/IANA &&
 prepare_install
}


current(){ [ -f /etc/services.d/IANA ]; }
