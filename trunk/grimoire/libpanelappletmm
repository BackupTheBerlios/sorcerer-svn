# multilib-fail because gnome-panel is multilib-fail

   version stable 2.26.0 2.22.0
   require gconfmm gnome-panel libgnomemm
  category window-manager
 attribute gnome library
 attribute multilib-fail
    source $GNOME_URL sources/libpanelappletmm/${VERSION:0:4}/libpanelappletmm-$VERSION.tar.bz2
  homepage http://www.gnome.org
  estimate 100
      desc 'libpanelappletmm provides a C++ wrappers for gnome-panel.'
