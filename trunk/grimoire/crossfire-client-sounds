    stable 1.50.0 1.12.0 1.11.0 1.10.0
  category game
 attribute client data x11
    source $SOURCEFORGE_URL crossfire/crossfire-client/$VERSION/crossfire-client-sounds-$VERSION.tar.gz
  homepage http://crossfire.real-time.com
      desc 'audio files for crossfire client'

build(){
rm -fr debian
prepare_install &&
mkdir -p   /usr/share/crossfire-client/sound &&
cp    -a * /usr/share/crossfire-client/sound
}
