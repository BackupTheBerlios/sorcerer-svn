    stable 1.50.0 1.12.0
  category game
 attribute client data x11
    source $SOURCEFORGE_URL crossfire/crossfire-client/$VERSION/crossfire-client-sounds-$VERSION.tar.gz
      info home  http://crossfire.real-time.com/
      desc 'audio files for crossfire client'

build(){
 rm -fr debian
 mkdir -p   $DESTDIR/usr/share/crossfire-client/sound &&
 cp    -a * $DESTDIR/usr/share/crossfire-client/sound
}
