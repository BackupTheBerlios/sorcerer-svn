inherit crossfire
unset  REQ
 ATTRIBUTE='data x11'
    IGNORE='\arch\|maps\|client'
    SOURCE="crossfire/crossfire-client-sounds-$VERSION.tar.gz"
  ESTIMATE='30'
      DESC='crossfire-client-sounds contains data files for crossfire client.'

build(){
 rm -fr debian
 data_install /usr/share/crossfire-client/sound
}
