inherit crossfire
unset  REQ
   VERSION=( "1.9.0" "1.4.0" )
 ATTRIBUTE="data x11"
    IGNORE="\arch\|maps\|client"
    SOURCE="crossfire/crossfire-client-sounds-$VERSION.tar.gz"
  ESTIMATE="5"
      DESC="crossfire-client-sounds contains data files for crossfire's client."


build()  {  data_install  /usr/share/crossfire-client/sound;  }
