    stable 0.3.5
   require gtk+ libexif
   category graphic
  attribute library
        url $SOURCEFORGE_URL
     source libexif/libexif-gtk-$VERSION.tar.bz2
   homepage http://www.sourceforge.net/projects/libexif
   estimate 500
       desc 'libexif-gtk is something.
The software authors included an empty README
and no documentation directory.'

build(){
 case $VERSION in
  0.3.5) export CFLAGS+=' -I/usr/include/libexif' ;;
 esac
 default_build
}
