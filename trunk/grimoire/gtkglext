  stable 1.2.0 1.1.0 1.0.6
    IGNORE='mm\|x11\|-'
  CATEGORY='graphic'
 ATTRIBUTE='library x11'
    SOURCE="gtkglext/gtkglext-$VERSION.tar.bz2"
       URL="$SOURCEFORGE_URL"
  HOMEPAGE='http://gtkglext.sourceforge.net'
       REQ='gtk+'
      OPTS='--enable-static=no'
  ESTIMATE='184'
      DESC='gtkglext is an OpenGL extention to gtk+'

build(){
 case $VERSION in
  1.0.6) export LDFLAGS+=' -lpangox-1.0' ;;
 esac
 default_build
}
