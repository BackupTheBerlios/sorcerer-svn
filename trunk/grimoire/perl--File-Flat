     PNAME=${SPELL#perl--}
   VERSION=( "0.96" )
  CATEGORY="perl"
 ATTRIBUTE="perl-module new"
    SOURCE="A/AD/ADAMK/$PNAME-$VERSION.tar.gz"
       URL="http://www.cpan.org/authors/id/"
  HOMEPAGE="http://search.cpan.org/search?dist=$PNAME"
       REQ=( "perl perl--File-NCopy perl--File-Remove perl--prefork" )
  ESTIMATE="15"
      DESC="$SPELL module implements a flat filesystem"

