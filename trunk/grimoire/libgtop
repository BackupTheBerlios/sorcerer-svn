   VERSION=( "2.14.1" "2.14.0" "2.12.2" "2.12.0" )
    IGNORE="^2\.15\."
  CATEGORY="development"
 ATTRIBUTE="gnome library"
    SOURCE="sources/libgtop/${VERSION%.*}/libgtop-$VERSION.tar.bz2"
  HOMEPAGE="http://www.home-of-linux.org/gnome/libgtop"
       URL="$GNOME_URL"
       REQ="glib popt"
  ESTIMATE="309"
      DESC="libgtop is a library for reporting system information.
libgtop is a library that fetches information about the running
system such as cpu and memory usage, active processes etc.
On Linux systems, these information are taken directly from the
/proc filesystem while on other systems a server is used to read
those information from /dev/kmem or similar."
