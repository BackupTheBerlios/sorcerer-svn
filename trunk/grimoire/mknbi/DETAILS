  VERSION="1.2"
 CATEGORY="bootloader"
ATTRIBUTE="console"
   SOURCE="mknbi/mknbi-$VERSION"
      URL="$SOURCEFORGE_URL"
 HOMEPAGE="http://mknbi.sourceforge.net"
    SHORT="mknbi creates images for network booting via etherboot"
cat << EOF
mknbi creates images for network booting via etherboot.
These are used to boot a diskless x86 PC over protocols 
like bootp/arp/dhcp and tftp.
EOF
