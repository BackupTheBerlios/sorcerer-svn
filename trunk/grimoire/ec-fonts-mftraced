   VERSION=( "1.0.12" "1.0.11" "1.0.10" "1.0.8" )
  CATEGORY="font"
 ATTRIBUTE="console"
    SOURCE="ec-fonts-mftraced-$VERSION.tar.gz"
       URL="http://lilypond.org/download/fonts/"
  HOMEPAGE="http://lilypond.org/"
       REQ="potrace mftrace teTeX"
  ESTIMATE="3677"
      DESC="ec-fonts-mftraced provides some fonts for lilypond"

build() {
make        all  &&
prepare_install  &&
make    install  &&
mktexlsr
}
