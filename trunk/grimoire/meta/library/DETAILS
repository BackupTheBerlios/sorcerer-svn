# The official library spell is in the library grimoire.
# This is an emergency spell to use if the library
# grimoire is not installed.
  VERSION=(  "$(  date  -u  +%Y%m%d%H  )"  )
  CATEGORY="grimoire"
 ATTRIBUTE="grimoire"
    SOURCE="library.tar.bz2"
       URL="http://sorcerer.wox.org/download"
  HOMEPAGE="http://sorcerer.wox.org"
  disable  scavenge
   ARCHIVE="off"
     SHORT="library is the library of grimoires."
cat << EOF
library is the library of grimoires.
EOF
