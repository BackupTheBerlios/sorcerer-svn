if  !  [  -d  /usr/gcc-2.95.3  ];  then  echo  "gcc2"  >>  $INSTALL_QUEUE;  fi
if  !  [  -d  /usr/gcc-3.2     ];  then  echo  "gcc"   >>  $INSTALL_QUEUE;  rm  -f  $PROVIDE_LOGS/gcc;  fi

ifsi()  {
  if    spell_installed  ${1}2
  then  echo  "0"  >  $VERSION_LOGS/$1
        echo  "$1" >  $PROVIDE_LOGS/${1}2
  fi
}

ifsi  gtk+
ifsi  gtkmm
ifsi  glib
ifsi  GConf
ifsi  gtk-engines
ifsi  libglade
ifsi  gnome-vfs
ifsi  libgda
ifsi  gnome-db
ifsi  control-center
ifsi  eel
ifsi  freetype
ifsi  librsvg
ifsi  libsigc++
ifsi  pygtk


(

  cd  $INSTALL_LOGS
  grep  "/usr/lib/libstdc++.la"                \
        $( find /lib  /usr/lib -name \*.la  )  |
  cut  -d :  -f1                               |
  while  read     FILE
  do     grep  "^$FILE$"  *
  done            |
  cut  -d :  -f1  |
  sort            |
  uniq            >>  $INSTALL_QUEUE

)
