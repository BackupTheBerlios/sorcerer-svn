    stable 20091107
 conflicts teTeX-texmf
   require texlive
  category documentation
 attribute archive_off data solo
    source ftp://tug.org/historic/systems/texlive/${VERSION:0:4}/texlive-$VERSION-texmf.tar.xz
  homepage http://www.tug.org/texlive/
      desc 'data for texlive'

build(){
prepare_install &&
cp    -Rv *    /usr/
}

post_install(){
default_post_install
echo mktexlsr | pipe_queue $CAST_QUEUE com
}
