    stable 438 436 429
   require ncurses
  category utility
 attribute console solo
    source http://www.greenwoodsoftware.com/less/less-$VERSION.tar.gz
      vurl http://www.greenwoodsoftware.com/less/download.html
  homepage http://www.greenwoodsoftware.com/less/
  estimate 150
      desc 'paginator that allows backward and forward movement'

build(){
default_build &&
mkdir -p $DESTDIR/bin &&
mv $DESTDIR/usr/bin/less $DESTDIR/bin
}
