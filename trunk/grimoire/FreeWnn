    stable 1.1.1-a021 1.10
  category input
 attribute console library server new
       url 'http://keihanna.dl.sourceforge.jp/
            http://kyushu-u.dl.sourceforge.jp/
            http://osdn.dl.sourceforge.jp/'
    source freewnn/17724/FreeWnn-$VERSION.tar.bz2
  homepage http://www.freewnn.org/
  estimate 3600
      desc 'FreeWnn provides a a kana to kanji translation system.'

build(){
mkdir    -p -m 755 /var/empty
groupadd -K GID_MIN=256 -K GID_MAX=511    wnn                   2>/dev/null
useradd  -K UID_MIN=256 -K UID_MAX=511 -g wnn -d /var/empty wnn 2>/dev/null
default_build
}
