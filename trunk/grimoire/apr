    stable 1.3.9 1.3.8 1.3.7 1.3.6 1.3.5 1.3.3
    secure 1.3.8
   require e2fsprogs
      opts --enable-static=no
  category network/web
 attribute console server
       url $APACHE_URL
    source apr/apr-$VERSION.tar.bz2
  homepage http://apr.apache.org
  estimate 700
      desc 'apache portable runtime'

build_old(){
default_build &&
ln -sv apr-1-config /usr/bin/apr-config
ln -sv apr-1.pc /usr/lib/pkgconfig/apr.pc
}
