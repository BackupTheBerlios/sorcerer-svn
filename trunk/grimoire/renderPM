 ATTRIBUTE='library new python-module'
    SOURCE="renderPM-$VERSION.tgz"
       URL='http://www.reportlab.org/daily/'
  HOMEPAGE='http://www.reportlab.org/rl_toolkit.html'
       REQ=''
  ESTIMATE='5'
      DESC='renderPM is a C extension for ReportLab,
available by subversion at URL
http://www.reportlab.co.uk/svn/public/reportlab/trunk/rl_addons/renderPM'

disable  sdelta

build(){
cd      renderPM  &&
prepare_install   &&
python  setup.py  install
}
