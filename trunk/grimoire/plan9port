   VERSION=( "20071105" "20071013" "20071010" "20070917" )
  CATEGORY="window-manager"
 ATTRIBUTE="x11 broken new"
# It does not install properly.
# Probably that is a boon.
# Some of the programs that it creates
# have the same names as those installed by other
# software such as coreutils.
    SOURCE="plan9port-$VERSION.tgz"
       URL="http://swtch.com/plan9port/"
      VURL="http://swtch.com/cgi-bin/info.cgi?file=/plan9port/plan9port.tgz"
  HOMEPAGE="http://swtch.com/plan9port/"
       REQ=""
  ESTIMATE="90"
      DESC="plan9port is a port of many plan 9 programs from plan 9 to unix."

build()  {
  ./INSTALL  -b    &&
  prepare_install  &&
  ./INSTALL  -c
}
