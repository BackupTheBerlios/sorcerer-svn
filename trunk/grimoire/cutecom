   VERSION=( 0.14.1 0.14.0 0.13.2 0.13.1 0.13.0 0.12.0 )
  CATEGORY='network'
 ATTRIBUTE='kde'
    SOURCE="cutecom-$VERSION.tar.gz"
       URL='http://cutecom.sourceforge.net/'
  HOMEPAGE='http://cutecom.sourceforge.net/'
       REQ='qt-x11-free'
  ESTIMATE='20'
      DESC="cutecom is a graphical serial terminal."

build(){
qmake
make QTDIR=/usr  &&
prepare_install  &&
        install  -m 0755  cutecom  /usr/bin/
}
