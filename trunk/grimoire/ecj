   version stable 4.5
  category development/compiler
 attribute console
    source ftp://sourceware.org/pub/java/ecj-$VERSION.jar
      info last 20100106
      desc 'eclipse java compiler for gcc gcj'

build(){
 mkdir  -pvm 755         $DESTDIR/usr/share/java
 install -vm 644 $SOURCE $DESTDIR/usr/share/java/ecj.jar
}
