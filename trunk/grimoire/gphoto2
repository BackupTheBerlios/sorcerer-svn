   VERSION=( "2.4.0" "2.3.1" "2.1.6" "2.1.5" "2.1.4" "2.1.3" "2.1.2" "2.1.1" )
    IGNORE="pre\|rc"
# gphoto-suite provides libgphoto2 and gphoto2 in a single tarball.
  CATEGORY="video/digital-camera"
 ATTRIBUTE="console"
    SOURCE="gphoto/gphoto2-$VERSION.tar.bz2"
       URL="$SOURCEFORGE_URL"
  HOMEPAGE="http://gphoto.sourceforge.net"
       REQ="libgphoto2 popt"
# gphoto2 can optionally require aalib
 FRESHMEAT="gphoto"
  ESTIMATE="350"
      DESC="gphoto2 provides digital camera applications.
gphoto2 is a command line application which enables you to transfer
digital photos from almost any serial-port or USB digital camera.
It uses the programming interface provided by libgphoto2."
