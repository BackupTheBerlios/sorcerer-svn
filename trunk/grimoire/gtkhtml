    stable 3.18.3 3.18.2 3.18.1 3.18.0
   require gail gal2 libsoup
    ignore '^3\.19\.\|^3\.23\.'
  category network/web
 attribute gnome library
       url $GNOME_URL
    source sources/gtkhtml/${VERSION:0:4}/gtkhtml-$VERSION.tar.bz2
  homepage http://www.gnome.org
      opts --enable-static=no
# gtkhtml can be compiled without soup, but evolution requires soup.
  estimate 1000
      desc 'gtkhtml is a light weight html rendering/editing library.'
