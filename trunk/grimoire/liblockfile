    stable 1.08-2
  category library
 attribute library
    source http://ftp.us.debian.org/debian/pool/main/libl/liblockfile/liblockfile_${VERSION%-*}.orig.tar.gz
    source http://ftp.us.debian.org/debian/pool/main/libl/liblockfile/liblockfile_${VERSION}.diff.gz
  homepage http://packages.debian.org/unstable/liblockfile1
      opts --enable-shared
  estimate 50
      desc 'liblockfile is a shared library with NFS-safe locking functions. 
It includes the command-line utility dotlockfile. 
library enables userspace use of kernel asynchronous I/O system calls'

build(){
gzip -cd ${SOURCE[1]} | patch -p1 &&
default_build
}
