   version stable 1.10.3
   require keyutils
      opts --enable-shared=yes  --enable-static=no
  category security/authentication security/encryption
 attribute console library makej
    source http://web.mit.edu/kerberos/dist/krb5/${VERSION:0:4}/krb5-$VERSION-signed.tar
      info last 20120116
      info home http://web.mit.edu/kerberos/
      desc 'Kerberos 5 from mit'

build(){
 if [  -f krb5-$VERSION.tar.gz ]; then
  tar -xf krb5-$VERSION.tar.gz
  cd      krb5-$VERSION/src
 fi
 default_build
}
