    stable 1.0.5
   require gnome-libs GConf-v1.0 gnome-mime-data
  optional openssl    '' '' for secure socket layer support
  optional cdparanoia '' '' for browsing CDs

    ignore '^2\.\|mm\|^1\.9\.\|^1\.1\|monikers'
  category development
 attribute gnome library
    source sources/gnome-vfs/1.0/gnome-vfs-$VERSION.tar.bz2
  homepage http://www.gnome.org
       url "$GNOME_URL"
  estimate 600
      desc 'gnome-vfs-v1.0 provides the Gnome Virtual File System.'

build(){
 sed -i "s:GnomeVFSProcessResult:GnomeVFSProcessRunResult:" libgnomevfs/gnome-vfs-private-utils.c
 sed -i "s:GnomeVFSProcessRunResult:GnomeVFSProcessResult:" libgnomevfs/gnome-vfs-process.c
 default_build
}
