   VERSION=( "1.0.5" )
    IGNORE="^2\.\|mm\|^1\.9\.\|^1\.1"
  CATEGORY="development"
 ATTRIBUTE="gnome library"
    SOURCE="sources/gnome-vfs/1.0/gnome-vfs-$VERSION.tar.bz2"
  HOMEPAGE="http://www.gnome.org"
       URL="$GNOME_URL"
       REQ=( "gnome-libs oaf GConf-v1.0 gnome-mime-data"
             "openssl:::for secure socket layer support"
             "cdparanoia:::for browsing CDs" )
  ESTIMATE="653"
      DESC="gnome-vfs-v1.0 provides the Gnome Virtual File System."

build()  {
  sed  -i  "s:GnomeVFSProcessResult:GnomeVFSProcessRunResult:"  \
       libgnomevfs/gnome-vfs-private-utils.c
  sed  -i  "s:GnomeVFSProcessRunResult:GnomeVFSProcessResult:"  \
       libgnomevfs/gnome-vfs-process.c
  default_build
}
