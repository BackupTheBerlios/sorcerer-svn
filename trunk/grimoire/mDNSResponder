    stable 107.1
  category network
 attribute console server deprecated
# kdelibs should be able to use avahi instead of mDNSResponder
    source mDNSResponder-$VERSION.tar.gz
       url http://helios.et.put.poznan.pl/~jstachow/pub/
  homepage http://helios.et.put.poznan.pl/~jstachow/pub/
  estimate 40
      desc 'mDNSResponder provides Apple mDNSResponder'

build(){
 sed -i 's:InstalledLib InstalledStartup:InstalledLib:' mDNSPosix/Makefile
 cd mDNSPosix             &&
 make            os=linux &&
 prepare_install          &&
 make    install os=linux
}
