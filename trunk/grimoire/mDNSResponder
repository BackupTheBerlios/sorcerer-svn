# kdelibs should be able to use avahi instead of mDNSResponder

   version stable 107.1
  category network
 attribute console server deprecated
    source http://helios.et.put.poznan.pl/~jstachow/pub/mDNSResponder-$VERSION.tar.gz
  homepage http://helios.et.put.poznan.pl/~jstachow/pub/
      desc 'Apple mDNSResponder'

build(){
sed -i 's:InstalledLib InstalledStartup:InstalledLib:' mDNSPosix/Makefile
cd mDNSPosix  &&
make os=linux &&
make os=linux install DSTROOT=$DESTDIR
}
