   VERSION=( "1.1.9" "1.1.8" "1.1.7" "1.1.6" "1.1.2" "1.1.0" )
  CATEGORY="disk/file-system"
 ATTRIBUTE="kernel"
    SOURCE="cvsfs/cvsfs-$VERSION.tar.gz"
       URL="$SOURCEFORGE_URL"
  HOMEPAGE="http://sourceforge.net/projects/cvsfs"
       REQ="cvs"
  disable  archive
  disable  necropolis
      DESC="cvsfs mounts a CVS project to a directory like a filesystem.
cvsfs is an attempt to let a user mount a CVS project like any filesystem.
It allows you to navigate and browse through a project tree.
No preparations are required on the CVS server side."


build() {
export  PATH="${PATH/\/usr\/ccache:/}"
default_build
}
