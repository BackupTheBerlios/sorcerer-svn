# Does aspell-dict-no replace aspell-dict-nb?
with info    last 20000101

with version stable 0.50.1-0
with base    aspell
with role    utility/editor
with trait   data
with source  $GNU_URL aspell/dict/nb/aspell-nb-$VERSION.tar.bz2
with info    cite 'dictionary for use with aspell'
build(){ ./configure && make install "DESTDIR=$DESTDIR"; }
