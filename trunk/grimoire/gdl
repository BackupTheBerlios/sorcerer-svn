   version stable 3.2.0 3.0.0 2.91.92 2.30.1 2.30.0
   require gtk+-v3 xcb-util
  category development
 attribute gnome library
    source $GNOME_URL sources/gdl/${VERSION:0:3}/gdl-$VERSION.tar.bz2
  homepage http://www.gnome.org
      desc 'Gnome Devtool Libraries'

build(){
 sed -i 's:hardcode_into_libs=yes:hardcode_into_libs=no:' configure
 default_build
}
