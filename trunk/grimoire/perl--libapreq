     PNAME=${SPELL#perl--}
   VERSION=( "1.33" )
  CATEGORY="perl"
 ATTRIBUTE="perl-module new"
    SOURCE="S/ST/STAS/$PNAME-$VERSION.tar.gz"
       URL="http://www.cpan.org/authors/id/"
  HOMEPAGE="http://search.cpan.org/search?dist=$PNAME"
       REQ=( "perl mod_perl perl--Apache-Test" )
  ESTIMATE="15"
      DESC="$SPELL provides Apache request C library
for manipulating client request data via the Apache API
with Perl and C. Functionality includes:
 - parsing of application/x-www-form-urlencoded data
 - parsing of multipart/form-data 
 - parsing of HTTP Cookies
"
