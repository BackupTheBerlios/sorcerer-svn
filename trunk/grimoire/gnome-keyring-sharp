   VERSION=( svn )
  CATEGORY='development'
 ATTRIBUTE='gnome'
    SOURCE='gnome-keyring-sharp.tar.gz'
       URL='http://anonsvn.mono-project.com/viewcvs/trunk/'
  HOMEPAGE='http://www.gnome.org'
#   SOURCE="sources/gnome-keyring-sharp/${VERSION:0:4}/gnome-keyring-sharp-$VERSION.tar.bz2"
#      URL="$GNOME_URL"
       REQ='gtk+ hal'
case $VERSION in
 svn) REQ+=' automake' ;;
esac
  ESTIMATE='60'
      DESC='gnome-keyring-sharp implements gnome-keyring in C#.'

build(){
 if ! [ -f configure ]; then
  aclocal
  automake -a
  autoconf
 fi
 default_build
}
