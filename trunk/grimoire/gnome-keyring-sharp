    stable svn
   require gtk+ hal

  category development
 attribute gnome
    source gnome-keyring-sharp.tar.gz
       url http://anonsvn.mono-project.com/viewcvs/trunk/
  homepage http://www.gnome.org
case $VERSION in
 svn) require automake ;;
esac
  estimate 60
      desc 'gnome-keyring-sharp implements gnome-keyring in C#.'

build(){
 if ! [ -f configure ]; then
  aclocal
  automake -a
  autoconf
 fi
 default_build
}
