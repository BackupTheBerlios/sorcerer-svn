with version   stable 3.6.1 3.6.0 3.4.0
with version unstable 3.7.90
with base    djvulibre gobject-introspection libgnome-keyring libgxps libspectre nautilus poppler
with elect   djvulibre       '' '' for displaying djvu documents
with elect   libgnomeprintui '' '' for printing documents
with also    --enable-static=no --enable-introspection=yes
with role    utility/documentation
with trait   gnome library makej multilib-concurrent
with source    stable $GNOME_URL sources/evince/${VERSION:0:3}/evince-$VERSION.tar.xz
with source  unstable $GNOME_URL sources/evince/${VERSION:0:3}/evince-$VERSION.tar.xz
with info    last 20121021
with info    home http://www.gnome.org
with info    cite 'document viewer for PDF and postscript files'

build(){
 sed -i 's:hardcode_into_libs=yes:hardcode_into_libs=no:' configure
 default_build
}
