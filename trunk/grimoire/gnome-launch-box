# multilib-fail because evolution-data-server is multilib-fail
   version stable 0.4
   require gnome-desktop
      opts --enable-static=no
  optional evolution-data-server
  category development
 attribute gnome library makej
 attribute multilib-fail
    source $GNOME_URL sources/gnome-launch-box/${VERSION:0:3}/gnome-launch-box-$VERSION.tar.bz2
  homepage http://www.gnome.org
  estimate 300
      desc 'analyzes text input to predict and select suitable program to run'
