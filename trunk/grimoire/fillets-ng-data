    stable 1.0.0 0.9.2
  category game
 attribute x11
    source $SOURCEFORGE_URL fillets/Fish%20Fillets%20-%20Next%20Generation/$VERSION/fillets-ng-data-$VERSION.tar.gz
      home http://fillets.sourceforge.net/
      docs http://fillets.sourceforge.net/manual.php
      desc 'data for fillets-ng game'

build(){
 mkdir -pvm 755 $DESTDIR/usr/share/games/fillets-ng &&
 cp    -rv *    $DESTDIR/usr/share/games/fillets-ng
}
