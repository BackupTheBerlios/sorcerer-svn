with version stable 1.0.0 0.9.2
with role    game
with trait   x11
with source  $SOURCEFORGE_URL fillets/Fish%20Fillets%20-%20Next%20Generation/$VERSION/fillets-ng-data-$VERSION.tar.gz
with info    last 20110912
with info    good 20120919
with info    home http://fillets.sourceforge.net/
with info    docs http://fillets.sourceforge.net/manual.php
with info    cite 'data for fillets-ng game'

build(){
 mkdir -pvm 755 $DESTDIR/usr/share/games/fillets-ng &&
 cp    -rv *    $DESTDIR/usr/share/games/fillets-ng
}
