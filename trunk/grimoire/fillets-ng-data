inherit fillets-ng
unset REQ VERSION
  stable 0.8.0 0.7.4 0.7.1 0.7.0 0.6.0 0.5.1 0.5.0 0.4.1 0.4.0
  ESTIMATE='7000'
    SOURCE="fillets/fillets-ng-data-$VERSION.tar.gz"
      DESC='fillets-ng-data provides data for fillets-ng game.'

build(){
prepare_install &&
mkdir -p    /usr/share/games/fillets-ng &&
cp    -rv * /usr/share/games/fillets-ng
}
