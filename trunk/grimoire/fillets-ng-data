    stable 0.9.2
  category game
 attribute x11
    source $SOURCEFORGE_URL fillets/Fish%20Fillets%20-%20Next%20Generation/$VERSION/fillets-ng-data-$VERSION.tar.gz
  homepage http://fillets.sourceforge.net
      desc 'data for fillets-ng game'

build(){
prepare_install &&
mkdir -p    /usr/share/games/fillets-ng &&
cp    -rv * /usr/share/games/fillets-ng
}
