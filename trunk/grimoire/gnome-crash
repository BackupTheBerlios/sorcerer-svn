   VERSION=( "0.0.5" )
  CATEGORY="development"
 ATTRIBUTE="broken depreciated gnome"
#ATTRIBUTE="gnome1 x11 broken depreciated"
    SOURCE="sources/gnome-crash/0.0/gnome-crash-$VERSION"
  HOMEPAGE="http://www.gnome.org"
       URL="$GNOME_URL"
       REQ="gnome-libs"
      DESC="gnome-crash extracts debugging information from core file of crashed applications.

This is a small tool to use after a core dump.
It can extract debugging information from a 
core file or crashed application (via gnome_segv).
"


build() {
export    CFLAGS="$CFLAGS   -I/usr/include/gdk-pixbuf-1.0"
export  CXXFLAGS="$CXXFLAGS -I/usr/include/gdk-pixbuf-1.0"
default_build
}
