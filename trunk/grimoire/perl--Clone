     PNAME=${SPELL#perl--}
   VERSION=( "0.20" )
  CATEGORY="perl"
 ATTRIBUTE="perl-module new"
    SOURCE="R/RD/RDF/$PNAME-$VERSION.tar.gz"
       URL="http://www.cpan.org/authors/id/"
  HOMEPAGE="http://search.cpan.org/search?dist=$PNAME"
       REQ=( "perl" )
  ESTIMATE="30"
      DESC="$SPELL module recursively copies Perl datatypes"

