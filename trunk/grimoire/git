   VERSION=( "1.4.2" )
  CATEGORY="development"
 ATTRIBUTE="console new"
    SOURCE="git-$VERSION.tar.bz2"
  HOMEPAGE="http://www.kernel.org/pub/software/scm/git/"
       URL="${HOMEPAGE}/"
  ESTIMATE="6"
      DESC="git is a fast distributed revision control system
with an unusually rich command set that provides both
high-level operations and full access to internals."

build() {
  autoconf			&&
  ./configure  prefix=/usr	&&
#  make  all doc			&&
  make  all			&&
  prepare_install		&&
  make  install
#  make  install install-doc
}
