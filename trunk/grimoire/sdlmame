   VERSION=( 0125u1 )
  CATEGORY='game development/emulator'
 ATTRIBUTE='x11'
#ATTRIBUTE='x11 broken'
    SOURCE="sdlmame$VERSION.zip"
       URL='http://rbelmont.mameworld.info/'
      VURL='http://rbelmont.mameworld.info/?page_id=163'
  HOMEPAGE='http://rbelmont.mameworld.info/?page_id=163'
       REQ=( '' )
case $VERSION in
 0125u1) USE_GCC=gcc-v4.2 ;;
esac
  ESTIMATE='2400'
      DESC='sdlmame is the xmame multi arcade machine emulator using SDL'

build(){
make -f makefile &&
prepare_install  &&
make -f makefile PREFIX=/usr/bin/
}
