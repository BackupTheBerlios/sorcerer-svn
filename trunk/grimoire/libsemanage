    stable 1.10.9 1.10.5 1.10.3 1.10.0
   require Python libselinux
  category security
 attribute console library
    source http://userspace.selinuxproject.org/releases/20080909/stable/libsemanage-${VERSION}.tar.gz
  homepage http://userspace.selinuxproject.org
  estimate 100
      desc 'libsemanage provides a library and interface for selinux management.'

build(){
make "CFLAGS=$CFLAGS" &&
prepare_install &&
make    install
}
