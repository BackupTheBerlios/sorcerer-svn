   VERSION=( "1.2" )
  CATEGORY="security"
 ATTRIBUTE="console library new"
    SOURCE="libsemanage-$VERSION.tgz"
  HOMEPAGE="http://www.nsa.gov/selinux"
       URL="http://www.nsa.gov/selinux/archives"
       REQ="libselinux"
  ESTIMATE="20"
      DESC="libsemanage provides a library and interface for selinux management."

build()  {
  make  "CFLAGS=$CFLAGS"  &&
  prepare_install  &&
  make    install
}
