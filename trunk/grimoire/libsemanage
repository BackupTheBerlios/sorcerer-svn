    stable 1.10.9 1.10.5 1.10.3 1.10.0
   require Python libselinux
  category security
 attribute console library
    source http://www.nsa.gov/selinux/archives/libsemanage-$VERSION.tgz
  homepage http://www.nsa.gov/selinux/
      vurl http://www.nsa.gov/selinux/code/download-stable.cfm
  estimate 100
      desc 'libsemanage provides a library and interface for selinux management.'

build(){
make "CFLAGS=$CFLAGS" &&
prepare_install &&
make    install
}
