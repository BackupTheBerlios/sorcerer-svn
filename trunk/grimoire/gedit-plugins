    source $GNOME_URL sources/gedit-plugins/${VERSION:0:4}/gedit-plugins-$VERSION.tar.bz2
      info home  http://gedit.sourceforge.net/
      info docs  http://live.gnome.org/Gedit
  estimate 600
      desc 'plugins for gedit, the gnome text editor'
