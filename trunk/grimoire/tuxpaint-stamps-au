  stable 0.0.7 0-0-6 0.0.5 src-0.0.4 0.0.3
inherit tuxpaint
    IGNORE='a\|-'
    SOURCE="tuxpaint-au/stamps-au-$VERSION.tar.bz2"
  HOMEPAGE='https://sourceforge.net/projects/tuxpaint-au'
       URL="$SOURCEFORGE_URL"
 FRESHMEAT=tuxpaint-au

build(){
 chown -R root:root *
 find . -type f | xargs --max-lines=64 chmod 644
 find . -type d | xargs --max-lines=64 chmod 755
 prepare_install &&
 mkdir -p   /usr/share/tuxpaint/stamps &&
 cp    -a * /usr/share/tuxpaint/stamps
}
