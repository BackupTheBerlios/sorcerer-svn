with version   stable 3.4.1.1 3.4.0 3.2.2
with version unstable 3.7.1
with base    glib gobject-introspection gst-python libffi pycairo
with also    --disable-docs
with role    development
with trait   gnome library python-module
with source    stable $GNOME_URL sources/pygobject/${VERSION:0:3}/pygobject-$VERSION.tar.xz
with source  unstable $GNOME_URL sources/pygobject/${VERSION:0:3}/pygobject-$VERSION.tar.xz
with info    last 20121021
with info    home http://www.pygtk.org/
with info    cite 'python bindings for gobject'

build(){
 case $VERSION in
  3.2.0) sed -i 's:1\.31\.20:0.10.8:g' configure ;;
  3.0.4) sed -i 's:1\.29\.0:0.10.8:g' configure ;;
 esac
 default_build
}

