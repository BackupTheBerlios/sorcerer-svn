    stable 6.0
  category font
 attribute data
       url $SOURCEFORGE_URL
    source ghostscript/ghostscript-fonts-other-$VERSION.tar.gz
  homepage http://www.ghostscript.com
      desc "ghostscript-fonts-other provides fonts for ghostscript."

build(){
prepare_install &&
mkdir   -p        /usr/share/ghostscript/fonts &&
install -vm 644 * /usr/share/ghostscript/fonts
}
