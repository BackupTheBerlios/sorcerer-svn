# multilib-fail because libproxy is also multilib-fail

   version stable 3.15 3.13
   require libproxy
      opts --disable-static
  category development
 attribute library makej multilib-fail
#attribute library makej multilib-concurrent
    source ftp://ftp.infradead.org/pub/openconnect/openconnect-$VERSION.tar.gz
      info last 20111125
      info home http://www.infradead.org/openconnect
      desc "client for Cicso's AnyConnect SSL VPN"

build(){ default_build && rm -f $DESTDIR/usr/lib{,64}/*.a; }
