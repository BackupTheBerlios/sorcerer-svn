   VERSION=( 1.3.3.11 1.3.3.10 1.3.4 )
# 1.3.3.11 is stable?
# 1.3.3.x is stable branch?
# The website does not state 1.3.4 is developmental.
# However, sourceforge indicates that 1.3.4 is developmental
    IGNORE='cvs'
  CATEGORY='utility/editor'
 ATTRIBUTE='kde new'
    SOURCE="scribus/scribus-$VERSION.tar.bz2"
  HOMEPAGE='http://www.scribus.net/'
       URL="$SOURCEFORGE_URL"
       REQ=( 'Python-v2.4 libart_lgpl qt-x11-free ghostscript'
             'cups:::for integration with Common Unix Printing System'
             'lcms:::for color management engine' )
  ESTIMATE='3500'
      DESC='scribus is a Desktop Publishing Program.'

build(){
 export PATH="/usr/bin/qt3:$PATH"
 default_build
}
