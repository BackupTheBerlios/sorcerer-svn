   VERSION=( 0.9.8.2 0.9.8 0.9.7 0.9.6.4 0.9.6.3 0.9.6.2 0.9.6.1 0.9.5.1a )
  CATEGORY='development/interpreter'
 ATTRIBUTE='console python-module'
    SOURCE="Pyrex-$VERSION.tar.gz"
       URL='http://www.cosc.canterbury.ac.nz/~greg/python/Pyrex/'
  HOMEPAGE='http://www.cosc.canterbury.ac.nz/~greg/python/Pyrex/'
       REQ='Python'
  ESTIMATE='100'
      DESC='Pyrex is langauge for writing python extention modules.'

build(){

case $VERSION in
 0.9.6.3) sed -i 's:#\(from Pyrex.Compiler.Version\):\1:
                  s:\(execfile(distutils.util\):#\1:' setup.py ;;
esac

[  -f  setup.py ] &&
prepare_install   &&
python setup.py install
}
