   VERSION=( "2.8.3" "2.8.2" "2.8.1" "2.8.0" )
    IGNORE="^2\.9\."
  CATEGORY="development/binding"
 ATTRIBUTE="library x11"
    SOURCE="sources/gtkmm/${VERSION%.*}/gtkmm-$VERSION.tar.bz2"
       URL="$GNOME_URL"
  HOMEPAGE="http://gtkmm.sourceforge.net"
       REQ="glibmm gtk+"
  ESTIMATE="1585"
      DESC="gtkmm is a C++ interface for the popular GUI library gtk+."

build()  {
  if    [  -d  gtkmm-$VERSION  ]
  then  cd     gtkmm-$VERSION
  fi
  default_build
}
