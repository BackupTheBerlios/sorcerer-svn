# replaces qtpfsgui
# Seems to be unable to locate installed libraw, wonder why.

   version   stable 2.3.0
   inherit kde-v4
   require qt-everywhere-opensource exiv2 fftwf openexr
  category digital-camera graphic
 attribute kde4 x11
 attribute broke
    source $SOURCEFORGE_URL qtpfsgui/luminance/$VERSION/luminance-hdr-$VERSION.tar.bz2
     info last 20120701
     info home http://qtpfsgui.sourceforge.net
conflicts qtpfsgui
     desc 'a GUI for HDR imaging workflow'

build_old(){
qmake PREFIX=/usr &&
make &&
make install INSTALL_ROOT=$DESTDIR
}
