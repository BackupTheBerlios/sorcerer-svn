   VERSION=( 3.3.3 3.3.2 2.1.1 2.0 1.5.1 1.5 1.4.1 1.4 1.2.1 )
  CATEGORY='security'
 ATTRIBUTE='console'
    SOURCE="setools-$VERSION/setools-$VERSION.tar.bz2"
      VURL='http://oss.tresys.com/projects/setools/wiki/download'
       URL='http://oss.tresys.com/projects/setools/chrome/site/dists/'
