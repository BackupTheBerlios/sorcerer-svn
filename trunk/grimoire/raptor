# Do not update separately from kde-v4 updates

   version stable 1.4.21 1.4.20 1.4.19
   require curl
      opts --enable-static=no
  category development
 attribute library makej multilib-concurrent
    source http://download.librdf.org/source/raptor-$VERSION.tar.gz
  homepage http://librdf.org/
      desc 'library for parsing and serializing RDF syntaxes'

build(){
 case $VERSION in
  1.4.21) sed -i '\,curl/types\.h,d' src/raptor_internal.h ;;
 esac
 default_build
}
