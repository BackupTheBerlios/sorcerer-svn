# libgnomeprint might be obsolete software.

   version stable 2.18.8
   require libart_lgpl pango
      opts --disable-gtk-doc --enable-static=no --without-cups
  category development
 attribute gnome library makej multilib-concurrent
    source $GNOME_URL sources/libgnomeprint/${VERSION:0:4}/libgnomeprint-$VERSION.tar.bz2
      info good 20120721
      info last 20100928
      info home http://www.gnome.org
      desc 'printing library'

build(){
 case $VERSION in
  2.18.8) sed -i '1i#include <stdio.h>' libgnomeprint/modules/cups/gnome-print-cups-transport.c ;;
 esac
 default_build
}
