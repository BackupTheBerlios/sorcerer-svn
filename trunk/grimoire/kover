   VERSION=( 2.9.6 2.9.5 2.9.3 2.9.1 )
  CATEGORY='disk/cd'
 ATTRIBUTE='kde3'
    SOURCE="kover-$VERSION.tar.gz"
       URL='http://lisas.de/kover/'
  HOMEPAGE='http://lisas.de/kover/'
       REQ='kdelibs-v3'
  ESTIMATE='133'
      DESC='kover is an easy to use WYSIWYG CD cover printer with CDDB support.'

build(){
 sed -i "\,#include <linux/cdrom.h>,i#undef __STRICT_ANSI__\
#include <linux/types.h>" kover/{cddb_fill.cc,cdrom.h}
 default_build
}
