inherit crossfire
unset  REQ
 ATTRIBUTE="data x11"
    IGNORE="\arch\|maps\|client"
    SOURCE="crossfire/crossfire-${VERSION}.maps.tar.gz"
  EPROVIDE="crossfire-maps-big"
  ESTIMATE="5"
      DESC="crossfire-maps contains data files for crossfire."

build()  {  data_install  /usr/share/crossfire/maps;  }
