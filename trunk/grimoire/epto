   VERSION=( 0.8.16 0.8.15 0.8.14 0.8.13 0.8.12 0.8.11 0.8.10 )
  CATEGORY='utility/shell'
 ATTRIBUTE='console'
    SOURCE="epto/epto-$VERSION.tar.bz2"
  HOMEPAGE='http://sourceforge.net/projects/epto'
       URL="$SOURCEFORGE_URL"
  ESTIMATE='9'
      DESC="epto is a for industrial strength shell script programming.
It features convenient error handling, tracing, 
logging, option handling, documentation template, 
process level transaction safety (sort of), and much more.
It takes less than five minutes of learning to start using it."


build()  {
  sed  -i  "s:rm -f:true:"  Makefile.makesrc
  default_build
}
