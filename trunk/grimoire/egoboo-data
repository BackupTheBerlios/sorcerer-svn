  stable 2.6.1
  CATEGORY='game'
 ATTRIBUTE='data'
    SOURCE="egoboo/egoboo-data-$VERSION.tar.gz"
       URL="$SOURCEFORGE_URL"
  HOMEPAGE='http://egoboo.sourceforge.net'
  ESTIMATE='100'
      DESC='egoboo-data is graphical nethack style game.'

build(){
 find . -type d | xargs --max-lines=2048 chmod 755
 find . -type f | xargs --max-lines=2048 chmod 644
 prepare_install &&
 mkdir -vpm 755 /usr/share/egoboo &&
 cp -av * /usr/share/egoboo
}
