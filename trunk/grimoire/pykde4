   inherit kde-v4
   require PyQt4 kdelibs-v4
  category development
 attribute kde4 library makej multilib-concurrent python-module setarch
    source   stable $KDE_URL   stable/$VERSION/src/$SPELL-$VERSION.tar.xz
    source unstable $KDE_URL unstable/$VERSION/src/$SPELL-$VERSION.tar.xz
      desc 'python wrapper for kde'

