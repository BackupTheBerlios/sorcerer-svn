    stable 3130 3120 3110 3100 393
  category graphic
 attribute library
    source $SOURCEFORGE_URL freeimage/Source%20Distribution/${VERSION:0:1}.${VERSION:1:2}.${VERSION:3:1}/FreeImage$VERSION.zip
  homepage http://freeimage.sourceforge.net
  estimate 1000
      desc 'an image loading codecs library'

build(){
make &&
make            -f Makefile.fip &&
prepare_install &&
make    install &&
make    install -f Makefile.fip
}

