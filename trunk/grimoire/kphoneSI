  stable 1.0.2.1
  CATEGORY='network/chat network/telephone'
 ATTRIBUTE='kde3 new broke'
    SOURCE="kphone/kphoneSI_$VERSION.tar.gz"
       URL="$SOURCEFORGE_URL"
  HOMEPAGE='http://sourceforge.net/project/showfiles.php?group_id=47742'
       REQ=( 'qt-x11-free'
             'alsa-lib:::for output using alsa' )
      OPTS='--prefix=/opt/kde3 --with-qt-dir=/opt/kde3 --disable-debug --enable-final'
  ESTIMATE='480'
      DESC='kphoneSI is a SIP (Session Initiation Protocol) user agent
With which you can initiate VoIP (Voice over IP) connections,
send Instant Messages, subscribe your friends presence information
and start other applications of your choice.'

build(){
#export PATH+=:/opt/kde3/bin
 export QTDIR=/opt/kde3
 autoconf &&
 default_build
}
