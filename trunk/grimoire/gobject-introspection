    stable 0.9.12 0.9.10 0.9.6
   require automake cairo fontconfig freetype glib libffi libxml2
      opts --enable-static=no
  category development
 attribute gnome library multilib-fail
 attribute broke
    source $GNOME_URL sources/gobject-introspection/${VERSION:0:3}/gobject-introspection-$VERSION.tar.bz2
  homepage http://www.gnome.org
 conflicts gir-repository
  estimate 550
      desc 'document API'

current(){ echo gobject-introspection | pipe_queue $DISPEL_QUEUE; }
