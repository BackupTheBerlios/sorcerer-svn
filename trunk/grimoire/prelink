# bad idea to use this since it modifies md5ums of installed files.

    stable 20060213
   require libelf
  category disk utility
 attribute console
 attribute broke
    source http://people.redhat.com/jakub/prelink/prelink-$VERSION.tar.bz2
  homepage http://people.redhat.com/jakub/prelink/
   exclude /etc/prelink.conf
   exclude /etc/prelink.cache
  estimate 600
      desc 'prelink modifies ELFs to expedite runtime linking.'

build(){
default_build &&
touch /etc/prelink.conf
}
