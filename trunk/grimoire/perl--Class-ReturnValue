     PNAME=${SPELL#perl--}
   VERSION=( "0.53" )
  CATEGORY="perl"
 ATTRIBUTE="perl-module new"
    SOURCE="J/JE/JESSE/$PNAME-$VERSION.tar.gz"
       URL="http://www.cpan.org/authors/id/"
  HOMEPAGE="http://search.cpan.org/search?dist=$PNAME"
       REQ=( "perl perl--Devel-StackTrace" )
  ESTIMATE="15"
      DESC="$SPELL is a return-value object 
that lets you treat it as as a boolean, array or object"

