if  [  "$SPELL"  ==  "matchbox"  ];  then
   VERSION=( "1.1" )
       REQ=( "matchbox-common  matchbox-window-manager"
             "matchbox-keyboard:::for on screen keyboard"
             "matchbox-nest:::for running at a particuliar sub resolution"
             "matchbox-panel-manager:::for panel and applet support"
             "matchbox-tests:::for an additional theme"
             "matchbox-themes-extra:::for extra themes"
             "mb-desktop-xine:::for viewing videos" )
else
    SOURCE="$SPELL-$VERSION.tar.bz2"
fi
  CATEGORY="window-manager"
 ATTRIBUTE="x11 new"
       URL="http://projects.o-hand.com/matchbox/sources/$SPELL/$VERSION/"
  HOMEPAGE="http://projects.o-hand.com/matchbox/"
  ESTIMATE="5"
      DESC="matchbox is a window manager for embedded platforms.
Please read matchbox's homepage for a detailed description."
