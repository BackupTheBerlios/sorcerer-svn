   VERSION=( "0.5.3" )
  CATEGORY="monitor utility"
 ATTRIBUTE="console"
    SOURCE="915resolution-$VERSION.tar.gz"
       URL="http://www.geocities.com/stomljen/"
  HOMEPAGE="http://www.geocities.com/stomljen/"
  ESTIMATE="49"
      DESC="$SPELL changes the resolution of a VBIOS mode Intel chipsets
$SPELL changes the resolution of an available VBIOS mode for the
Intel 800 and 900 series graphic chipset. 915resolution supports 
the 845G, 855GM, 865G, 915G, 915GM, 945G, and 945GM chipsets.
It has been created in order to obtain the 1400x1050 resolution on 
laptops with these graphic chipsets."

build(){
 make			&&
 prepare_install	&&
 make install
}
