#  VERSION=( "2.1.13" "2.0.12" "2.0.11" "2.0.10" "2.0.9" "2.0.8" "2.0.7" "2.0.6" )
   VERSION=( "2.0.12" "2.0.11" "2.0.10" "2.0.9" "2.0.8" "2.0.7" "2.0.6" )
# libfwbuilder 2.1.13 has compile error
    IGNORE="^200.....\|floppyfw\|i386"
  CATEGORY="network"
 ATTRIBUTE="x11"
    SOURCE="fwbuilder/fwbuilder-$VERSION.tar.gz"
       URL="$SOURCEFORGE_URL"
  HOMEPAGE="http://www.fwbuilder.org"
       REQ="autoconf automake gettext libfwbuilder libtool"
  ESTIMATE="568"
      DESC="fwbuilder is a GUI for building firewalls using iptables."

build() {
  export  QTDIR=/usr
  UIQ="-I/usr/include/qt"
  ./autogen.sh           &&
  ./configure            \
  --build=$BUILD         \
  --prefix=/usr          \
  --sysconfdir=/etc      \
  --localstatedir=/var   \
  $OPTS                  &&
  make  CXX="g++         \
        $UIQ $CXXFLAGS"  &&
  prepare_install        &&
  make    install
}
