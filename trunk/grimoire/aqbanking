# Probably not used anymore.
# aqbanking-kde3 is probably used instead

    stable 3.0.1 3.0.0 2.3.2 2.2.9 2.2.8 2.2.7 2.2.6 2.2.5 2.2.4 2.2.3 2.2.1 2.2.0
 case $VERSION in
  2.3*) require libchipcard3 libgail-gnome libofx ;;
     *) require libchipcard  libgail-gnome libofx ;;
 esac
  category utility/finance
 attribute library
       url $SOURCEFORGE_URL
    source aqbanking/aqbanking-$VERSION.tar.gz
  homepage http://www.aquamaniac.de/aqbanking/index.html
  estimate 3300
      desc 'aqbanking provides an interface to online banking libraries.'

build(){
 ./configure          \
 --prefix=/usr        \
 --sysconfdir=/etc    \
 --localstatedir=/var \
 --with-frontends='cbanking g2banking' \
 $OPTS           &&
 make            &&
 prepare_install &&
 make    install
}
