    stable 0.7.8 0.7.7 0.7.6
  category video
 attribute library
    source http://dl.matroska.org/downloads/libebml/libebml-$VERSION.tar.bz2
  homepage http://dl.matroska.org/downloads/libebml/
      opts --enable-static=no
  estimate 100
      desc 'libebml is a C++ libary to parse EBML files.'

build(){
cd      make/linux
make        all prefix=/usr &&
prepare_install             &&
make    install prefix=/usr
}
