   version stable 3.2.0 3.0.0.1 3.0.0 2.91.94 2.32.1 2.32.0.2
   require dconf evolution-data-server gnome-desktop gnome-menus librsvg libwnck telepathy-glib
#  require dconf evolution-data-server gnome-desktop gnome-menus librsvg libwnck telepathy-glib
      opts --enable-static=no
  category window-manager
# multilib-fail while evolution-data-server is multilib-fail
 attribute gnome library makej multilib-fail
#attribute gnome library makej multilib-concurrent
    source $GNOME_URL sources/gnome-panel/${VERSION:0:3}/gnome-panel-$VERSION.tar.xz
      info last 20111006
  homepage http://www.gnome.org
      desc 'the gnome2 panel'

build(){
 sed -i 's:hardcode_into_libs=yes:hardcode_into_libs=no:' configure
 default_build
}
