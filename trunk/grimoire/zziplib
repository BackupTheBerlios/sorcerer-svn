   version stable 0.13.61 0.13.60 0.13.59
   require docbook-xml-4.1.2 gst-python libpaper zlib
      opts --enable-static=no
  category archive
 attribute console library makej multilib-concurrent
    source $SOURCEFORGE_URL zziplib/zziplib13/$VERSION/zziplib-$VERSION.tar.bz2
      info last 20120312
      info home http://zziplib.sourceforge.net
      desc 'library for extracting data from a single zip file
zziplib library is intentionally lightweight, 
it offers the ability to easily extract data 
from files archived in a single zip file.'

build(){
 case $VERSION in
  0.13.60) sed -i '/-export-dynamic/d' configure ;;
 esac
 default_build
}
