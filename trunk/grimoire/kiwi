    stable 1.9.22 1.9.21 1.9.20 1.9.19 1.9.18
   require pygtk

  category graphic
 attribute gnome python-module
    source sources/kiwi/${VERSION:0:3}/kiwi-$VERSION.tar.bz2
  homepage 
       url "$GNOME_URL"
  estimate 600
      desc 'kiwi provides a framework for developing graphical applications in Python'

build(){
 prepare_install &&
 python setup.py install
}
