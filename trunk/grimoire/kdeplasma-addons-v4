   inherit kde-v4
   require kdepimlibs-v4 kdebase-workspace-v4 qimageblitz
  optional scim
  category graphic
      desc 'Additional applets and themes'

build(){
 CFLAGS="${CFLAGS//-mno-mmx/}"
 CFLAGS="${CFLAGS//-mno-sse2/}"
 CFLAGS="${CFLAGS//-mno-sse/}"

 CXXFLAGS="${CXXFLAGS//-mno-mmx/}"
 CXXFLAGS="${CXXFLAGS//-mno-sse2/}"
 CXXFLAGS="${CXXFLAGS//-mno-sse/}"
 kde_build
}
