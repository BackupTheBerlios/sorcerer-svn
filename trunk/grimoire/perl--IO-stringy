     PNAME=${SPELL#perl--}
   VERSION=( "2.110" )
  CATEGORY="perl"
 ATTRIBUTE="perl-module new"
    SOURCE="D/DS/DSKOLL/$PNAME-$VERSION.tar.gz"
       URL="http://www.cpan.org/authors/id/"
