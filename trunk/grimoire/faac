   version stable 1.28 1.26
   require automake libtool
  optional faad2 '' '' for MP4 support
      opts --enable-static=no
  category audio/codec audio/library
 attribute console library makej multilib-dual
#attribute multilib-concurrent
    source $SOURCEFORGE_URL faac/faac-src/faac-$VERSION/faac-$VERSION.tar.gz
      info home  http://www.audiocoding.com/
  estimate 200
      desc 'implementation of MPEG-2 and NBC/MPEG-4
faac is an implementation of a part of one or more MPEG-2 NBC/MPEG-4 
Audio tools as specified by the MPEG-2 NBC/MPEG-4 Audio standard.'
