   VERSION=( "1.24" "1.23.5" )
# 1.24.1 does not exist at Fri Nov 12 06:39:08 UTC 2004 as freshmeat reports.
    IGNORE="faac\|2001\|x86"
  CATEGORY="audio"
 ATTRIBUTE="console"
    SOURCE="faac/faac-$VERSION.tar.gz"
       URL="$SOURCEFORGE_URL"
  HOMEPAGE="http://www.audiocoding.com"
       REQ="automake libtool"
  ESTIMATE="106"
      DESC="faac is an implementation of MPEG-2 and NBC/MPEG-4.
faac is an implementation of a part of one or more MPEG-2 NBC/MPEG-4 
Audio tools as specified by the MPEG-2 NBC/MPEG-4 Audio standard."


build() {
aclocal      -I .                   &&
autoheader                          &&
libtoolize    --automake            &&
automake      --add-missing         &&
autoconf                            &&
default_build
}
