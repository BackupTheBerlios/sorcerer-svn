   VERSION=( "0.9" )
  CATEGORY="utility"
 ATTRIBUTE="console"
    SOURCE="urlview-$VERSION.tar.gz"
  HOMEPAGE="ftp://ftp.mutt.org/mutt/contrib/"
       URL="ftp://ftp.mutt.org/mutt/contrib/"
       REQ="rx"
      DESC="urlview, finds URLs in a text file and passes them to a viewer(lynx w3m ...)
urlview is a curses program for extracting URLs from text files and
displaying a menu from which you can select a specific URL to view
using your favorite browser program.
Requires: working regcomp() or GNU rx."

build() {
default_build  &&
cp  url_handler.sh  /usr/bin
}
