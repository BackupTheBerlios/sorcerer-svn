# Disable introspection since introspection is broke with clutter

    stable 0.8.1 0.8.0
case $VERSION in
 0.8.0)
      opts --enable-introspection=no ;;
esac
   require clutter-gtk libsoup sqlite
  category development
 attribute gnome library
    source $GNOME_URL sources/libchamplain/${VERSION:0:3}/libchamplain-$VERSION.tar.bz2
  homepage http://www.gnome.org
  estimate 1000
      desc 'clutter based widget to display rich, eye-candy and interactive maps'
