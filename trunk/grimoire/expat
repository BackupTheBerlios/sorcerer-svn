# Version 2.1.0 installed improperly using cmake
# Double check names of installed libraries after casting.

   version stable 2.1.0 2.0.1
      opts --enable-static=no
  category development/interpreter
 attribute library makej multilib-concurrent
    source $SOURCEFORGE_URL expat/expat/$VERSION/expat-$VERSION.tar.gz
      info last 20120326
      info home http://expat.sourceforge.net/
      info mail http://mail.libexpat.org/mailman/listinfo/
      desc 'fast non-validating stream XML parsing library'

build(){
 case $VERSION in
  2.1.*) rm -f CMakeLists.txt ;;
 esac
 default_build
}
