inherit poppler
     REQ='gtk+ qt-x11-opensource'
    OPTS='--prefix=/opt/kde4 --enable-poppler-qt4 --enable-zlib --disable-poppler-qt'
ESTIMATE='12500'
    DESC='poppler-qt-x11-opensource is a PDF rendering library with qt-x11-opensource support.'

build(){
 export PATH+=:/opt/qt4/bin
 default_build
}
