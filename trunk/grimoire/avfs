   VERSION=( "0.9.6" )
  CATEGORY="archive utility"
 ATTRIBUTE="console new"
    SOURCE="avf/avfs-$VERSION.tar.gz"
       URL="$SOURCEFORGE_URL"
# Creates library by default
#            ":--enable-library:--disable-library:to compile avfs as a library"
# Docs say preload only works on Solaris
#            ":--enable-preload:--disable-preload:for preloaded library and server"
       REQ=( ""
             "coda:--enable-avfscoda:--disable-avfscoda:for coda interface" )
  HOMEPAGE="http://avf.sourceforge.net"
  ESTIMATE="84"
      DESC="avfs enables looking into archived and compressed files."
