with version   stable 3.8.1 3.8.0 3.6.2
with version unstable 3.8.1
with info    last 20130417
trait broke
with base    clutter-gtk evince gjs libzapojit tracker
case $VERSION in
 3.8.1)
with also    --enable-introspection=no ;;
 3.8.0)
with also    --enable-introspection=no ;;
 3.6.0)
with also    --enable-introspection=no ;;
esac
with role    development
with trait   gnome library makej multilib-concurrent
with source    stable $GNOME_URL sources/gnome-documents/${VERSION:0:3}/gnome-documents-$VERSION.tar.xz
with source  unstable $GNOME_URL sources/gnome-documents/${VERSION:0:3}/gnome-documents-$VERSION.tar.xz
with info    home http://live.gnome.org/Design/Apps/Documents
with info    cite 'document manager application for GNOME'
