# xmms plugin is not optional
# 1.1.0 has compile error
    stable 1.2.2 1.2.1 1.1.0
   require gpgme jasper libXtst libgtkhtml libmcrypt libnotify libv4l openssl xmms
  optional gtkspell
  optional pulseaudio
  category network/chat
 attribute x11
    source $SOURCEFORGE_URL gyachi/gyachi/$VERSION/gyachi-$VERSION.tar.gz
  homepage http://gyachi.sourceforge.net/
  estimate 1300
      desc 'yahoo instant messenger chat client
gyachi is a fork of gyach which provides webcam and voice support,
environments, and an integrated profile browser.'

build(){
 if   [ -x autogen.sh ]
 then    ./autogen.sh
 fi
 default_build
}
