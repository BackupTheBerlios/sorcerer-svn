# stable        1.1.0 1.0.5 1.0.4

    stable 1.1.61 1.1.60 1.1.59 1.1.55 1.1.52 1.1.48 1.1.46 1.1.26 1.1.0 1.0.5 1.0.4
   require gpgme jasper libgtkhtml libmcrypt libnotify xmms
  optional gtkspell
  optional pulseaudio
  category network/chat
 attribute x11
#attribute broke
       url $SOURCEFORGE_URL
    source gyachi/gyachi-$VERSION.tar.gz
  homepage http://gyachi.sourceforge.net/
  estimate 1200
      desc 'gyachi is a yahoo instant messenger chat client.
gyachi is a fork of gyach which provides webcam and voice support,
environments, and an integrated profile browser.'

build(){
 if   [ -x autogen.sh ]
 then    ./autogen.sh
 fi
 default_build
}
