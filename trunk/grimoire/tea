# Update sqlite when updating tea

   version stable 3071400 3071300 3071201
      opts --disable-rpath --enable-shared=yes --enable-static=no
   require sqlite tcl
  category db
 attribute library lto makej multilib-concurrent
    source http://www.sqlite.org/sqlite-autoconf-$VERSION.tar.gz
      info vurl http://www.sqlite.org/download.html
      info last 20120903
      info home http://www.sqlite.org/
      desc 'tcl extension architecture or sqlite for tcl'

build(){
 cd tea
 default_build
}
