   VERSION=( "231" "23" )
      SAFE=( "231" )
    IGNORE="un\|roxy"
  CATEGORY="archive"
 ATTRIBUTE="console"
    SOURCE="zip${VERSION}.tar.gz"
       URL="http://www.mirrorservice.org/sites/ftp.info-zip.org/pub/infozip/src"
#       URL="http://ftp.info-zip.org/pub/infozip/src"
#   SOURCE="infozip/zip${VERSION}.tar.gz"
#      URL="$SOURCEFORGE_URL"
  HOMEPAGE="http://www.info-zip.org/pub/infozip/Zip.html"
  ESTIMATE="80"
      DESC="zip is a PKWARE compatible compressor.
Info-ZIP's purpose is to provide free, portable,
high-quality versions of the Zip and UnZip
compressor-archiver utilities that are compatible
with the DOS-based PKZIP by PKWARE, Inc."


build() {
make  -f unix/Makefile  generic  &&
prepare_install  &&
make  -f unix/Makefile  install  prefix=/usr INSTALL=install
}
