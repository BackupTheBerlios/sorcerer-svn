SF="darkplacesenginesource$VERSION.zip"
CF="OPTIM_RELEASE=$CFLAGS"
UG="/usr/games"
DP="darkplaces"

if    [  -f  $SF  ];  then
  unzip      $SF

  cd    darkplaces                     &&
  make  glx-release  "$CF"             &&
  make  ded-release  "$CF"             &&
  prepare_install                      &&
  install  -m 755  $DP-glx        $UG  &&
  install  -m 755  $DP-dedicated  $UG
fi
