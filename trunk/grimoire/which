   version stable 2.20 2.19 2.17 2.16 2.15 2.14 2.13
      opts --bindir=/bin
  category utility
 attribute archive_off console solo
    source $GNU_URL which/which-$VERSION.tar.gz
      desc 'which searches the path to find executables.'

current(){
 case $VERSION in
  2.20) [[ -f /bin/which ]] ;;
 esac
}
