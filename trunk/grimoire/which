   version stable 2.20
      opts --bindir=/bin
  category utility
 attribute archive_off console solo
    source $GNU_URL which/which-$VERSION.tar.gz
      info good 20120713
      info last 20080806
      info home http://carlo17.home.xs4all.nl/which/
      desc 'which searches the path to find executables.'

current(){
 case $VERSION in
  2.20) [[ -f /bin/which ]] ;;
 esac
}
