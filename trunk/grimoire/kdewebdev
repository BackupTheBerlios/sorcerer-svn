inherit kde
case  $VERSION  in
  3.4.0)
  SOURCE[1]="security_patches/post-3.4.0-kdewebdev-kommander.diff"
  ;;
esac
  CATEGORY="network/web"
       REQ=( "kdelibs" )
#            "arts:--with-arts:--without-arts:for sound support" )
  ESTIMATE="5850"
  EPROVIDE="quanta"
      DESC="kdewebdev provides web development applications.
kdewebdev contains:
 Quanta Plus:      a web page development tool
 Kommander:        a GUI script builder and executor tool.
 KFileReplace:     a powerful search and replace in multiple files
 KXSLDbg:          an XSL debugger
 KImageMapEditor:  an image map editor
 KLinkStatus:      a link validity checker"

build()  {
  case  $VERSION  in
    3.4.0)  patch  -p0  <  $SOURCE_CACHE/kdewebdev/$VERSION/${SOURCE[1]##*/} ;;
  esac
  kde_build
}
  
current()  {
  case $VERSION  in
    3.4.0) grep  -qx  "patching file kommander/executor/instance.cpp" \
           $COMPILE_LOGS/kdebase ;;
  esac
}

