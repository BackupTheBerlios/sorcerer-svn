# 2.7.7 is developmental
  stable 2.6.1
    IGNORE='data\|^2\.7\.7'
  CATEGORY='game'
 ATTRIBUTE='x11'
    SOURCE="egoboo/egoboo-$VERSION.tar.gz"
       URL="$SOURCEFORGE_URL"
  HOMEPAGE='http://egoboo.sourceforge.net'
       REQ='MesaLib SDL_mixer SDL_ttf egoboo-data'
  ESTIMATE='170'
      DESC='egoboo is graphical nethack style game.'

build(){
 cd game
 make -f Makefile.unix &&
 prepare_install &&
 make -f Makefile.unix install
}
