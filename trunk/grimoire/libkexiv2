   VERSION=( 0.1.6 0.1.5 0.1.2 0.1.1 0.1.0 )
  CATEGORY='graphic kde-extragear'
 ATTRIBUTE='kde library'
     SOURCE="kipi/libkexiv2-$VERSION.tar.bz2"
        URL="$SOURCEFORGE_URL"
  HOMEPAGE='http://extragear.kde.org/apps/kipi'
  ESTIMATE='600'
       REQ='exiv2 kdelibs-v3'
      DESC='libkexiv2 is a kde wrapper around exiv2'

build(){
 export KDEDIR=/opt/kde3
 export QTDIR=/opt/kde3
 default_build
}
