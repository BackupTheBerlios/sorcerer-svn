#!/bin/bash

### BEGIN INIT INFO
# Provides: pts
# Required-Start: $local_fs
# Should-Start: udev
# Required-Stop: 
# Default-Start: 1 2 3 4 5
# Default-Stop:
# Short-Description: devpts mounts devpts on /dev/pts
### END INIT INFO

. /lib/lsb/init-functions

NAME='/dev/pts'
OPTS='-t devpts devpts'

parse "$@"
