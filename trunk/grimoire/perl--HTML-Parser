   VERSION=( "3.55" "3.54" "3.51" "3.50" "3.48" "3.46" "3.45" "3.42" "3.40" "3.36" )
  CATEGORY="perl"
 ATTRIBUTE="perl-module new"
    SOURCE="G/GA/GAAS/${SPELL#perl--}-$VERSION.tar.gz"
       URL="http://www.cpan.org/authors/id/"
  HOMEPAGE="http://search.cpan.org/search?dist=HTML-Parser"
       REQ="perl perl--HTML-Tagset"
  ESTIMATE="40"
      DESC="$SPELL provides modules that parse and extract information from HTML documents"

build() {
  yes  |  default_pm_build
}
