   VERSION=( "0.6.0" "0.4.0" )
  CATEGORY="development ruby"
 ATTRIBUTE="library"
    SOURCE="ruby-zlib-$VERSION.tar.gz"
       URL="http://www.blue.sky.or.jp/atelier/ruby"
  HOMEPAGE="http://www.blue.sky.or.jp/atelier/ruby"
       REQ="ruby zlib"
  ESTIMATE="5"
      DESC="ruby-zlib provides a compression library for Ruby"

build() {
prepare_install   &&
ruby  extconf.rb  &&
make              &&
make    install
}
