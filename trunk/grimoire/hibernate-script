   VERSION=( "1.91" "1.12" "1.05" "1.03" "0.98" "0.97" "0.94" )
  CATEGORY="utility"
 ATTRIBUTE="console"
    SOURCE="hibernate-script-$VERSION.tar.gz"
       URL="http://www.suspend2.net/downloads/all"
  HOMEPAGE="http://www.suspend2.net"
  EPROVIDE="suspend-script"
  ESTIMATE="5"
      DESC="$SPELL provides hibernate script for Software Suspend 2"

build() {
  prepare_install	&&
  yes  |  PREFIX=/usr ./install.sh
  echo "$?" > /dev/null
}
