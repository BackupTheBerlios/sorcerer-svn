 ATTRIBUTE="console"
    SOURCE="hibernate-script-$VERSION.tar.gz"
       URL="http://www.suspend2.net/downloads/all"
  HOMEPAGE="http://www.suspend2.net"
  EPROVIDE="suspend-script"
