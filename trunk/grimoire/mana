   VERSION=( 0.2.1 )
  CATEGORY='input'
 ATTRIBUTE='console'
    SOURCE="shinji/20514/mana-$VERSION.tar.bz2"
       URL="http://keihanna.dl.sourceforge.jp/
            http://kyushu-u.dl.sourceforge.jp/
            http://osdn.dl.sourceforge.jp/"
  HOMEPAGE='http://sourceforge.jp/projects/shinji/'
       REQ='findlib gdbm'
  ESTIMATE='500'
      DESC="mana is a system for Japanese input.
It converts Hiragana text to Kana Kanji mixed text."

build()  {
  case  $VERSION  in
    0.2.1)  unset    CFLAGS
            unset  CXXFLAGS
            unset   LDFLAGS  ;;
  esac
  default_build
}
