   VERSION=( "1.1.0" "1.0.8" "1.0.7" "1.0.6" "1.0.5" "1.0.4beta" "1.0.3beta" "0.9.14beta" )
    IGNORE="qt-wizard\|beta"
  CATEGORY="finance"
 ATTRIBUTE="library"
    SOURCE="aqhbci/aqhbci-$VERSION.tar.gz"
       URL="$SOURCEFORGE_URL"
       REQ="aqbanking"
  HOMEPAGE="http://sourceforge.net/projects/aqhbci"
  ESTIMATE="307"
      DESC="aqhbci provides a library for the German HomeBanking Computer Interface.
It includes some tutorials for testing the library.
Its current capabilities include complete key management
on disc (RDH) and/or chip-card (DDV), the ability to perform
single customer credit transfers, and the ability to get balance reports."
