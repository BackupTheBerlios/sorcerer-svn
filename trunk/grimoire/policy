   VERSION=( "1.20" "1.18" "1.16" "1.14" "1.12" "1.10" )
  CATEGORY="security"
 ATTRIBUTE="console"
    SOURCE="policy-$VERSION.tgz"
  HOMEPAGE="http://www.nsa.gov/selinux"
       URL="http://www.nsa.gov/selinux/archives"
       REQ="checkpolicy m4"
  ESTIMATE="5"
      DESC="policy contains the example policy configuration.
And also the flask definition files
and the application configuration files."


build()  {
# #  sed  -i  "s/MLS=n/MLS=y/"  Makefile  &&
# make     policy  &&
