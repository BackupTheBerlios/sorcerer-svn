#!/bin/bash

### BEGIN INIT INFO
# Required-Start: dev
# Default-Start: S
# Short-Description: dev-cp copies /lib/udev/dev/* to /dev/
### END INIT INFO

. /lib/lsb/init-functions

start(){
 if   log_warning_msg "cp -a /lib/udev/dev/* /dev"; cp -a /lib/udev/dev/* /dev
 then log_success_msg "/lib/udev/dev/* copied"
 else log_failure_msg "/lib/udev/dev/* not copied"
 fi
 }

stop(){ :; }

parse "$@"
