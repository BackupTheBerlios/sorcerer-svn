  stable 1.9.1
  CATEGORY='digital-camera graphic'
 ATTRIBUTE="kde4 x11 utility new"
     SOURCE="qtpfsgui/qtpfsgui-$VERSION.tar.gz"
       URL="$SOURCEFORGE_URL"
  HOMEPAGE="http://qtpfsgui.sourceforge.net"
       REQ=( 'qt-x11-opensource exiv2 fftwf openexr' )
  ESTIMATE='170'
      DESC='qtpfsgui is a GUI for HDR imaging workflow'

build(){
 /opt/kde4/bin/qmake PREFIX=/usr &&
 make &&
 prepare_install &&
 make    install
}
