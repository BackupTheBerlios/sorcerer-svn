    stable 2.68
  category development
 attribute data
      home 'http://www.gnu.org/software/autoconf/manual/autoconf.html#Site-Defaults'
      desc 'installs a /usr/config.site with useful ./configure defaults'

build(){
 mkdir   -pvm 755 "$DESTDIR/usr"
 install  -vm 644  $SCRIPT_DIR/config.site "$DESTDIR/usr"
}
