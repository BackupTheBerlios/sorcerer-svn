    stable 1.6
   require openssl tcl
  category security/encryption
 attribute library
    source $SOURCEFORGE_URL tls/tls/$VERSION/tls$VERSION-src.tar.gz
  homepage http://tls.sourceforge.net/
  estimate 2700
      desc 'transport layer security extensions for TCL'

build(){
 sed -i '/RPATH=/d' configure
 default_build
}
