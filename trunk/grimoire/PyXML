   VERSION=( 0.8.4 0.8.3 0.8.2 0.8.1 0.8 0.7.1 0.7 )
  CATEGORY='development/interpreter/python'
 ATTRIBUTE='library'
    SOURCE="pyxml/PyXML-$VERSION.tar.gz"
       URL="$SOURCEFORGE_URL"
  HOMEPAGE='http://PyXML.sourceforge.net'
       REQ='Python'
  ESTIMATE='239'
      DESC="PyXML is an XML parsing library.
PyXML is a fast, non-validating, stream-oriented XML parsing library."

build() {
python  setup.py  build    &&
prepare_install            &&
python  setup.py  install
}
