inherit  teTeX
  unset  REQ
    SOURCE="systems/unix/teTeX/current/distrib/tetex-texmf-$VERSION.tar.gz"
disable  archive
disable  necropolis
  EPROVIDE="teTeX,texmf"
  ESTIMATE="5"
      DESC="teTeX-texmf provides data for teTeX to use."


build()  {
  prepare_install                  &&
  mkdir  -p      /usr/share/texmf  &&
  cp     -Rv  *  /usr/share/texmf/
}
