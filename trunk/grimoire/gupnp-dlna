with version   stable 0.10.0 0.6.6 0.6.1
with version unstable 0.10.0
with base    gst-plugins-base-0.10
with also    --enable-static=no
case $VERSION in
 0.10.0)
with also    --enable-introspection=no ;;
 0.6.6)
with also    --enable-introspection=no ;;
esac
with role    video/dvd video/player
with trait   library makej multilib-concurrent
with source    stable $GNOME_URL sources/gupnp-dlna/${VERSION:0:4}/gupnp-dlna-$VERSION.tar.xz
with source  unstable $GNOME_URL sources/gupnp-dlna/${VERSION:0:4}/gupnp-dlna-$VERSION.tar.xz
with info    last 20130101
