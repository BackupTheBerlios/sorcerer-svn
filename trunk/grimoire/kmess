   version stable 2.0.6.1 2.0.6
   require kdebase-v4 qca-ossl
      opts -DCMAKE_INSTALL_PREFIX:PATH=/opt/kde4
      opts -DCMAKE_INSTALL_RPATH:PATH=/opt/kde4/lib:/opt/kde4/lib64
      opts -DCMAKE_SKIP_BUILD_RPATH:BOOL=TRUE
      opts -DCMAKE_BUILD_WITH_INSTALL_RPATH:BOOL=TRUE
      opts -DCMAKE_INSTALL_RPATH_USE_LINK_PATH:BOOL=TRUE
      opts -DCMAKE_BUILD_TYPE=Release
  category network/chat
 attribute client kde4
    source $SOURCEFORGE_URL kmess/Latest%20versions/$VERSION/kmess-$VERSION.tar.bz2
#   source $SOURCEFORGE_URL kmess/Latest%20versions/$VERSION/kmess-$VERSION.tar.gz
      info last  20110225
      info home  http://kmess.sourceforge.net/
      info forum http://kmess.org/board/
  estimate 600
      desc 'kde MSN Messenger'

build(){
 local  RATH=$PATH
 local  PATH=/opt/kde4/bin:$RATH
 export PATH
 default_build
}
