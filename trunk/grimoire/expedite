build(){
 link_fix(){
  if   [[ $HOSTTYPE == x86_64 ]] && ! [[ $CFLAGS =~ -m32 ]]
  then export LDFLAGS+=' -L/usr/lib64'
  fi
 }

 case $VERSION in
  1.1.0) link_fix ;;
 esac
 default_build
}
