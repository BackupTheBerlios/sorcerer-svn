   VERSION=( "412" )
  CATEGORY="documentation"
 ATTRIBUTE="data"
    SOURCE="docbookx$VERSION.tgz"
       URL="http://scrollkeeper.sourceforge.net/data/"
  HOMEPAGE="http://www.oasis-open.org/"
  EPROVIDE="ISO-sgml-xml"
       REQ="libxml2 unzip"
  ESTIMATE="5"
      DESC="docbook-dtd provides XML/SGML standards."


build()  {
  prepare_install  &&
  make    install
}
