   VERSION=( "0.4.4" "0.4.3" "0.4.2" "0.4.1" "0.4.0" )
    IGNORE="0\.4\.5"
# 0.4.5 is very odd and may be missing header files.
  CATEGORY="disk"
 ATTRIBUTE="kde depreciated"
    SOURCE="qtparted/qtparted-$VERSION.tar.bz2"
  HOMEPAGE="http://qtparted.sourceforge.net"
       URL="$SOURCEFORGE_URL"
       REQ=( "parted qt-x11-free"
             "progsreiserfs:::for modifying reiserfs partitions"
             "xfsprogs::--disable-xfs:for modifying XFS"
             "ntfsprogs::--disable-ntfs:for modifying NTFS"
             "jfsutils::--disable-jfs:for modifying IBM's JFS" )
  ESTIMATE="208"
      DESC="qtparted is a partition magic clone.
It uses libparted for raw disk access,
so you need parted installed to use it.
The goal of QTParted is to fully wrap
all function of parted and create a
user friendly PM clone for Linux."

