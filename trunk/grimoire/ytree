   VERSION=( "1.84" "1.80" "1.79" "1.78" "1.76" )
  CATEGORY="utility/file-manager"
 ATTRIBUTE="console"
    SOURCE="ytree-$VERSION.tar.gz"
  HOMEPAGE="http://www.han.de/~werner/ytree.html"
       URL="http://www.han.de/~werner"
       REQ="ncurses readline"
  ESTIMATE="61"
      DESC="ytree is a console file manager similar to DOS-XTREE(tm)."

build()  {
  make  "ADD_CFLAGS=$CFLAGS"  &&
  prepare_install  &&
  make    install
}
