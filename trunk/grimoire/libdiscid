   VERSION=( "0.1.1" )
    IGNORE="test\|pre"
  CATEGORY="audio/library"
 ATTRIBUTE="library"
    SOURCE="libdiscid-$VERSION.tar.gz"
       URL="http://users.musicbrainz.org/~matt/"
      VURL="http://musicbrainz.org/doc/libdiscid"
  HOMEPAGE="http://musicbrainz.org/doc/libdiscid"
       REQ="pkgconfig"
  ESTIMATE="260"
      DESC="libdiscid is a library for creating musicbrainz discIDs from audio CDs."

build_old()  {
  cmake  -DCMAKE_INSTALL_PREFIX:PATH=/usr .  &&
   make          &&
prepare_install  &&
   make_install
}
