   VERSION=( "0.3.16" "0.3.15" "0.3.14" "0.3.13" "0.3.12" "0.3.11" )
  CATEGORY="log"
 ATTRIBUTE="library"
    SOURCE="libol-$VERSION.tar.gz"
       URL="http://www.balabit.hu/downloads/libol/${VERSION:0:3}"
  HOMEPAGE="http://www.balabit.hu"
  ESTIMATE="71"
      DESC="libol is a library for the syslog-ng logging daemon"

build() {
sed  -i  "s/-O2/$CFLAGS/"  configure
default_build
}
