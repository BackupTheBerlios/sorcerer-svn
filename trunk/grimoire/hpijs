# does not compile with gcc 4.4.2
   version stable 2.1.4
   require ghostscript
  category printer
 attribute x11
    source $SOURCEFORGE_URL hpinkjet/hpijs/$VERSION/hpijs-$VERSION.tar.gz
#     info home  http://hpinkjet.sourceforge.net/
      info home  http://hplipopensource.com/hplip-web/index.html
  estimate 100
 freshmeat hpinkjet
      desc 'Hewlett Packard drivers for HP Inkjet printers
hpijs provides HP open source printer drivers for their inkjet printers.
For further information on getting your HP inkjet working under linux,
see http://www.linuxprinting.org.'
