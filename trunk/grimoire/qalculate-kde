   version stable 0.9.7
   require libqalculate
      opts --with-qt-dir=/opt/kde3
 attribute broke
    source $SOURCEFORGE_URL qalculate/qalculate-kde/qalculate-kde-$VERSION/qalculate-kde-$VERSION.tar.gz
      info home http://qalculate.sourceforge.net/
