    stable 3.2
  category documentation
 attribute console
    source $SOURCEFORGE_URL polyglotman/polyglotman/$VERSION/rman-$VERSION.tar.gz
  homepage http://polyglotman.sourceforge.net
      desc 'translates manual pages to other formats'

build()  {
[ -f Makefile ] &&
make install BINDIR=$DESTDIR/usr/bin MANDIR=$DESTDIR/usr/man CFLAGS="$CFLAGS"
}
