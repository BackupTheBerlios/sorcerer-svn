    stable 3.2
  category documentation
 attribute console new
       url $SOURCEFORGE_URL
    source polyglotman/rman-$VERSION.tar.gz
  homepage http://polyglotman.sourceforge.net
      desc 'rman translates manual pages to other formats.'

build()  {
[ -f Makefile ] &&
prepare_install &&
make    install BINDIR=/usr/bin MANDIR=/usr/man CFLAGS="$CFLAGS"
}
