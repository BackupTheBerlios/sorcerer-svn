# 3.10.2 requires rarian

   version   stable 3.12.0 3.10.2 3.10.0 3.7.1 3.6.7
   version unstable 3.12.0 3.11.0
   require gtk+-v3 rarian
      opts --disable-static
  category graphic
 attribute gnome library makej multilib-concurrent
    source $GNOME_URL sources/glade/${VERSION:0:4}/glade-$VERSION.tar.xz
      info last 20120327
      info home http://glade.gnome.org/
      desc 'GUI builder for GTK+ and GNOME'

build(){
 sed -i 's:-L${py_prefix}/lib::' configure
 default_build
}
