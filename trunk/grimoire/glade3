   require libgnomeui rarian
  category graphic
 attribute gnome library makej multilib-concurrent
    source $GNOME_URL sources/glade3/${VERSION:0:3}/glade3-$VERSION.tar.bz2
      info home  http://glade.gnome.org/
      desc 'GUI builder for GTK+ and GNOME'

