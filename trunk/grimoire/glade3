   version stable 3.10.2 3.10.0 3.7.1 3.6.7
   require gtk+-v3 rarian
  category graphic
 attribute gnome library makej multilib-concurrent
    source $GNOME_URL sources/glade/${VERSION:0:4}/glade-$VERSION.tar.bz2
      info last 20111020
      info home  http://glade.gnome.org/
      desc 'GUI builder for GTK+ and GNOME'

build(){
 sed -i 's:-L${py_prefix}/lib::' configure
