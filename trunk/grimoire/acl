 FRESHMEAT="none"
      DESC="acl provides access control list for XFS and ext3"

build() {
build_old()  {
  ./configure  --prefix=/usr  &&
  make  "DEBUG="              \
        "LDFLAGS=$LDFLAGS"    &&
  prepare_install             &&
  make    install  install-lib  install-dev
}


  export  OPTIMIZER=-O
  export  DEBUG=-DNDEBUG

  make  configure  &&
  make             &&
  prepare_install  &&
  make    install  install-lib  install-dev
}
