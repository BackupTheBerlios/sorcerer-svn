   VERSION=( "2.2.15" "2.1.1" "2.0.19" "2.0.9" )
  CATEGORY="file-system"
 ATTRIBUTE="console"
    SOURCE="acl-$VERSION.src.tar.gz"
       URL="ftp://oss.sgi.com/projects/xfs/download/latest/cmd_tars"
  HOMEPAGE="http://oss.sgi.com/projects/xfs"
       REQ="attr"
  EPROVIDE="xfs,acl"
  ESTIMATE="223"
     SHORT="acl provides access control list for XFS and ext3"
stream << EOF
acl provides access control list for XFS and ext3
EOF


build()  {

  ./configure  --prefix=/usr  &&
  make  "DEBUG="              \
        "LDFLAGS=$LDFLAGS"    &&
  prepare_install             &&
  make    install  install-lib  install-dev

}
