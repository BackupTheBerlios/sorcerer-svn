   VERSION=( "2.3.5" "2.3.4" "2.3.3" "2.3.2" "2.3.1" "2.3.0" )
      SAFE=( "2.3.5" "2.3.4" )
    IGNORE="ming"
  CATEGORY="font/library"
 ATTRIBUTE="library bc"
    SOURCE="freetype/freetype-$VERSION.tar.bz2"
       URL="$SOURCEFORGE_URL"
       REQ="zlib"
  HOMEPAGE="http://freetype.sourceforge.net"
  ESTIMATE="450"
      DESC="freetype is a free, quality, portable font engine."

build()  {
  default_build  &&
  cp     -av  include/freetype  /usr/include
}
