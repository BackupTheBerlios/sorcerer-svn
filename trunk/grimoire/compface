   SOURCE="compface-$VERSION.tar.gz"
      URL='http://ftp.xemacs.org/pub/xemacs/aux/'
 HOMEPAGE='http://ftp.xemacs.org/pub/xemacs/aux/'
      REQ='autoconf'
