# This should be of the same version as bzr plus a second dot and a digit
   VERSION=( "0.91.0" "0.18.0" )
  CATEGORY="development/interpreter/python"
 ATTRIBUTE="python-module new"
    SOURCE="bzr-gtk-$VERSION.tar.gz"
       URL="http://phanatic.hu/bzr-gtk/"
  HOMEPAGE="http://bazaar-vcs.org/bzr-gtk"
       REQ="Python bzr"
  ESTIMATE="246"
      DESC="bzr-gtk is a plugin for bzr providing GUI"

build() {
  prepare_install  &&
  python  setup.py  install
}
