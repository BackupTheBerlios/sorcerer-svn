   VERSION=( "2.0.3" "2.0.2" "2.0.1" )
    IGNORE="^2-1\.1"
  CATEGORY="development"
 ATTRIBUTE="gnome library x11"
    SOURCE="2.0.1/sources/libgnome/libgnome-$VERSION"
  HOMEPAGE="http://www.gnome.org"
       URL="$GNOME_URL"
       REQ="GConf2 db esound gnome-vfs2 gtk+2 libbonobo libxml2 libxslt"
 CONFLICTS="gnome-core"
 DISTROWAT="GNOME"
  GNOMEFTP="libgnome"
     SHORT="libgnome provides the libraries essential to most gnome programs."
cat << EOF
These are the GNOME2 libraries, the foundation for most of the
application of the GNOME2 project.
EOF
