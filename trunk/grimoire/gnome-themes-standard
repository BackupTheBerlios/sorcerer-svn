   version stable 3.0.1 3.0.0
   version unstable 3.1.3 3.1.2.1
   require gnome-icon-theme gnome-icon-theme-symbolic gtk-engines
  category development
 attribute data gnome
    source $GNOME_URL sources/gnome-themes-standard/${VERSION:0:3}/gnome-themes-standard-$VERSION.tar.bz2
      info last  20110501
  homepage http://www.gnome.org
      desc 'themes for gnome'

build(){
 sed -i 's:hardcode_into_libs=yes:hardcode_into_libs=no:' configure
 default_build
}
