   version   stable 3.2.1 3.2.0.2 3.0.1
   version unstable 3.3.5.1 3.3.4
   require gnome-icon-theme gnome-icon-theme-symbolic gtk-engines librsvg
  category development
 attribute data gnome
    source $GNOME_URL sources/gnome-themes-standard/${VERSION:0:3}/gnome-themes-standard-$VERSION.tar.xz
      info last 20120131
      info home http://www.gnome.org
      desc 'themes for gnome'

build(){
 sed -i 's:hardcode_into_libs=yes:hardcode_into_libs=no:' configure
 default_build
}
