    stable 24.2
   require Python

  category development
 attribute python-module
    source numpy/Numeric-$VERSION.tar.gz
  homepage http://numpy.sourceforge.net/
       url "$SOURCEFORGE_URL"
  estimate 150
      desc 'Numeric adds a fast and sophisticated array facility to Python.'

build(){
 [ -f setup.py ] &&
 prepare_install &&
 python setup.py install
}
