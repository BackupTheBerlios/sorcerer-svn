    stable 4.17
  category utility
 attribute console new
    source tpctl/tpctl_$VERSION.tar.gz
       url "$SOURCEFORGE_URL"
  homepage http://tpctl.sourceforge.net/
  estimate 60
      desc 'tpctl provides tools for thinkpad configuration.'

build(){
 make        all &&
 prepare_install &&
 make    install
}
