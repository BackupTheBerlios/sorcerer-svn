    stable 3.5.8
  category profile
 attribute kde3
  homepage http://www.kde.org/
  estimate 5
      desc 'kde-path-v3 exports a PATH to where KDE is installed'

build(){
 prepare_install
 mkdir   -vp                              /etc/bash.d/interactive
 install -vm 0755 $SCRIPT_DIR/PATH-KDE-v3 /etc/bash.d/interactive
}
