   VERSION=( 3.5.8 )
  CATEGORY='profile'
 ATTRIBUTE='kde3'
  HOMEPAGE='http://www.kde.org/'
  ESTIMATE='5'
      DESC='kde-path-v3 exports a PATH to where KDE is installed'

build(){
 prepare_install
 mkdir   -vp                              /etc/bash.d/noninteractive
 install -vm 0755 $SCRIPT_DIR/PATH-KDE-v3 /etc/bash.d/noninteractive
}
