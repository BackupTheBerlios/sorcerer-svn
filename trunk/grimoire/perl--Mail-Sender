   VERSION=( "0.8.10" )
    SOURCE="/J/JE/JENDA/${SPELL#perl--}-$VERSION.tar.gz"
  CATEGORY="perl"
 ATTRIBUTE="perl-module new"
  HOMEPAGE="http://search.cpan.org/search?dist=${SPELL#perl--}"
       URL="http://www.cpan.org/authors/id/"
       REQ=( "perl" )
  ESTIMATE="19"
      DESC="$SPELL module for sending mails with attachements
through an SMTP server"

build() {
  true  |  default_pm_build
}
