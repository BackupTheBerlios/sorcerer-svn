# Has non standard installation procedure.
# Also meld has no description.
# Does one have to mind meld with softare authors these days
# in order to discover what they created?

    stable 1.2
  category development
 attribute gnome library
 attribute broke
       url $GNOME_URL
    source sources/meld/${VERSION:0:3}/meld-$VERSION.tar.bz2
  homepage http://www.gnome.org
      opts --enable-static=no
  estimate 7000
      desc 'meld'
