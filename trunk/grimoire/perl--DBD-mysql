     PNAME=${SPELL#perl--}
   VERSION=( "3.0007" )
  CATEGORY="perl"
 ATTRIBUTE="perl-module new"
    SOURCE="C/CA/CAPTTOFU/$PNAME-$VERSION.tar.gz"
       URL="http://www.cpan.org/authors/id/"
