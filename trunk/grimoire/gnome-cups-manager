# For a laugh read the README

   version stable 0.33
   require libgnomecups libgnomeui
  category printer
 attribute gnome
    source $GNOME_URL sources/gnome-cups-manager/${VERSION:0:4}/gnome-cups-manager-$VERSION.tar.bz2
      info home http://www.gnome.org
      opts --enable-static=no
      desc 'program for configuring cups'
