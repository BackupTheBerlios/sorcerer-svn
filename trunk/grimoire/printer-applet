# README lists as requirement
# hal-cups-utils:  https://fedorahosted.org/hal-cups-utils/
# however hal is deprecated so is this an actual requirement?
# compiles fine without it.

   inherit kde-v4
   require PyQt4 gst-python kde-runtime system-config-printer
  category printer
    source   stable $KDE_URL   stable/$VERSION/src/${SPELL%-v4}-$VERSION.tar.bz2
    source unstable $KDE_URL unstable/$VERSION/src/${SPELL%-v4}-$VERSION.tar.bz2
      desc 'system tray utility for displaying queued print jobs'
