   version stable 5.4.0
   require zlib
      opts --with-zlib --enable-static=no
  category network/web
 attribute library lto multilib-dual
    source http://www.w3.org/Library/Distribution/w3c-libwww-$VERSION.tgz
      info home http://www.w3.org/
      info good 20120711
      info last 20020612
 freshmeat libwww
      desc 'general-purpose Web API provided by W3C'

build(){
 strip_destdir(){
  local  LC_ALL
  export LC_ALL=C

  find $DESTDIR -type f     |
  xargs --max-lines=64 file |
  grep ', not stripped'     |
  cut -d : -f1              |
  xargs --max-lines=64 strip --strip-unneeded
 }

 default_build &&
 strip_destdir
}
