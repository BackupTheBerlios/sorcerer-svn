# for SGI Indigo2 and Octane for X11R7
# Version 0.2.0 may be broken or require header files that
# are not available on x86 and x86_64

    stable 0.2.0
  unstable 0.2.0
  require xorg-server
 category xorg/driver
attribute x11 X11R7 library deprecated
      url $XORG_URL
   source driver/$SPELL-$VERSION.tar.bz2
     opts --disable-static
 homepage http://www.x.org
 estimate 200
     desc "$SPELL provides video driver for X11R7"
