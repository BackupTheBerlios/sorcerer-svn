   version stable 1.2b
  category archive
 attribute console
    source ftp://metalab.unc.edu/pub/Linux/utils/compress/unace-$VERSION.tar.gz
 freshmeat none
      info good 20120711
      info last 19990513
      desc 'extracts files from ace archives'

build(){
make clean
make "CFLAGS=$CFLAGS -DUNIX -DCASEINSENSE" &&
mkdir -pvm 755 $DESTDIR/usr/bin &&
install -o root -g root -vm 755 unace $DESTDIR/usr/bin/
}
