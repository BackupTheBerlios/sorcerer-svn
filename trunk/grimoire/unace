   VERSION=( "1.2b" )
  CATEGORY="archive"
 ATTRIBUTE="console"
       URL="ftp://metalab.unc.edu/pub/Linux/utils/compress"
      DESC="unace extracts files from ace archives."
 FRESHMEAT="none"
      DESC="unace extracts files from ACE archives.
unace is a freeware program distributed with source code 
and developed for extracting, testing and viewing the 
contents of archives created with the ACE archiver."


build() {
make  clean
make  "CFLAGS=$CFLAGS -DUNIX -DCASEINSENSE"  &&
prepare_install  &&
install  -o root  -g root  -m 755  unace  /usr/bin
}
