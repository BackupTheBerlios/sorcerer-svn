    stable 1.2b
  category archive
 attribute console
    source ftp://metalab.unc.edu/pub/Linux/utils/compress/unace-$VERSION.tar.gz
 freshmeat none
      desc 'unace extracts files from ace archives.'

build(){
make clean
make "CFLAGS=$CFLAGS -DUNIX -DCASEINSENSE" &&
prepare_install &&
install -o root -g root -vm 755 unace /usr/bin
}
