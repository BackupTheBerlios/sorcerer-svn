   VERSION=( "0.61" "0.60" "0.58" "0.57" )
    IGNORE="linux"
  CATEGORY="emulator"
 ATTRIBUTE="broken x11"
# 0.61 does not compile with gcc 3.4.1
    SOURCE="dosbox/dosbox-$VERSION.tar.gz"
       URL="$SOURCEFORGE_URL"
  HOMEPAGE="http://dosbox.sourceforge.net"
       REQ="SDL_net"
  ESTIMATE="201"
      DESC="dosbox emulates a DOS box.
dosbox emulates a PC with DOS.
Good for playing sierra graphical adventure games, 
and other 16 bit software such as Ultima Underworld"


build() {
export  LDFLAGS="$LDFLAGS -lz"
default_build
}
