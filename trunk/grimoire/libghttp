    stable 1.0.9
  category network/web
 attribute library
    source $GNOME_URL sources/libghttp/${VERSION:0:3}/libghttp-$VERSION.tar.gz
  homepage http://www.gnome.org
      opts --enable-static=no
  estimate 150
      desc 'gnome http client library.
gnome-http -- gnome http client library
maybe required by Enlightenment for autoupgrades?'

build(){
libtoolize -i -c -f &&
aclocal &&
autoconf &&
default_build
}
