     PNAME=${SPELL#perl--}
   VERSION=( "0.17004" )
  CATEGORY="perl"
 ATTRIBUTE="perl-module new"
    SOURCE="P/PE/PEVANS/$PNAME-$VERSION.tar.gz"
       URL="http://www.cpan.org/authors/id/"
  HOMEPAGE="http://search.cpan.org/search?dist=$PNAME"
       REQ=( "perl" )
  ESTIMATE="20"
      DESC="$SPELL provides error/exception handling in an OO-ish way"
