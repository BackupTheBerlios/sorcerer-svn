with info    good 20130914
with version stable 1.0
with role    game development/emulator
with trait   data
with source  stable $SOURCEFORGE_URL scummvm/extras/Soltys/soltys-pl-v$VERSION.zip
with info    cite 'the Polish version of Soltys point and click adventure game'

build(){
 cd ..
 chmod 755 *
 chmod -R 644 */*
 install -vm 755 -d "$DESTDIR"/usr/share/games/ &&
 cp    -av *        "$DESTDIR"/usr/share/games/
}
