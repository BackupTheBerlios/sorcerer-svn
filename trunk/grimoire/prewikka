   VERSION=(          0.9.12.1 )
#  VERSION=( 0.9.13 0.9.12.1 )
# 0.9.13 has unknown build requirements
  CATEGORY='security/authentication'
 ATTRIBUTE='console python-module'
    SOURCE="prewikka-$VERSION.tar.gz"
  HOMEPAGE='http://www.prelude-ids.org'
       URL='http://www.prelude-ids.org/download/releases/'
      VURL='http://www.prelude-ids.org/spip.php?rubrique6'
       REQ='libpreludedb'
  ESTIMATE='3500'
      DESC="prewikka provides a prelude management console.
This requires extensive additional configuration by the SA
as documented at:
https://trac.prelude-ids.org/wiki/InstallingPrewikka"

build_new() {
python  setup.py  build  &&
prepare_install          &&
python  setup.py  install
}
