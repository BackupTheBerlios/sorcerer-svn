    source $GNOME_URL sources/gedit/${VERSION%.*}/gedit-$VERSION.tar.bz2
      info home  http://gedit.sourceforge.net/
      info docs  http://live.gnome.org/Gedit
  estimate 2100
      desc 'text editor for the GNOME Desktop'
