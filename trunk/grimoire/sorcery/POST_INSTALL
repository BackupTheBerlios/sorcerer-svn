clean_provide_logs()  {
  grep  -r  ""  $PROVIDE_LOGS  |
  basenames                    |
  tr  ':'  ' '                 |
  while  read  LEFT  RIGHT;  do
    if    [  "$LEFT"  ==       "$RIGHT"  ]
    then  rm  -f  $PROVIDE_LOGS/$RIGHT
    fi
  done
  grep   -rl  ","  $PROVIDE_LOGS  |  xargs  -l64   rm  -f
}


ARCH=$( uname -m )

if    [  -x  $SENTINEL-$ARCH  ]
then  cp     $SENTINEL-$ARCH  $SENTINEL
fi

clean_provide_logs

generate_index
