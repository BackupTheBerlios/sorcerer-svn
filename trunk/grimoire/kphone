    stable 4.2
   require qt-x11-free
  optional alsa-lib '' '' for output using alsa
  category network/chat network/telephone
 attribute kde3
 attribute broke
    source http://www.wirlab.net/kphone/kphone-$VERSION.tar.gz
  homepage http://www.wirlab.net/kphone/
      opts --prefix=/opt/kde3 --with-qt-dir=/opt/kde3 --disable-debug --enable-final
  estimate 150
      desc 'kphone is an Internet telephony program.
KPhone is a SIP (Session Initiation Protocol) user agent to
initiate VoIP (voice over IP) connections over the Internet.
It supports Presence and Instant Messaging, and to some extent
also video calls between two hosts.'

build(){
#export PATH="/opt/kde3/bin:$PATH"
 export QTDIR=/opt/kde3
 default_build
}
