   VERSION=( "0.12" )
    SOURCE="/B/BJ/BJKUIT/${SPELL#perl--}-$VERSION.tar.gz"
  CATEGORY="perl"
 ATTRIBUTE="perl-module new"
  HOMEPAGE="http://search.cpan.org/search?dist=${SPELL#perl--}"
       URL="http://www.cpan.org/authors/id/"
       REQ=( "perl" )
  ESTIMATE="10"
      DESC="$SPELL Perl-only implementation of lanman and nt md4 hash
functions, for use in Samba style smbpasswd entries"

build() {
  true  |  default_pm_build
}
