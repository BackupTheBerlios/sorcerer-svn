   VERSION=( "1.12" "1.10" "1.8" "1.6" "1.4" "1.2" "1.0" )
  CATEGORY="security"
 ATTRIBUTE="library"
    SOURCE="libsepol-$VERSION.tgz"
  HOMEPAGE="http://www.nsa.gov/selinux/"
       URL="http://www.nsa.gov/selinux/archives/"
  ESTIMATE="90"
      DESC="libsepol provides an interface for manipulating binary policies."

build()  {
  make  "CFLAGS=$CFLAGS"  &&
  prepare_install  &&
  make    install
}
