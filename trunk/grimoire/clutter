# If introspection works with clutter version > 1.4.0
# then do an # augur arof clutter
# and check/fix those spells also to enable introspection

    stable 1.4.2 1.4.0
  unstable 1.5.12
case $VERSION in
 1.4.0)
      opts --enable-introspection=no ;;
esac
   require gtk+ json-glib
  category audio/cd
 attribute gnome library makej multilib-concurrent
    source http://source.clutter-project.org/sources/clutter/${VERSION:0:3}/clutter-$VERSION.tar.bz2
#   source $GNOME_URL sources/clutter/${VERSION:0:3}/clutter-$VERSION.tar.bz2
      home http://clutter-project.org/
      docs http://www.clutter-project.org/documentation
  estimate 2800
      desc 'library for creating fast visually rich animated GUIs'
