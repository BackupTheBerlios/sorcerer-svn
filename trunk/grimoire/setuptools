   VERSION=( "0.6c7" "0.6c6" "0.6c5" "0.6c3" )
  CATEGORY="development"
 ATTRIBUTE="python-module"
    SOURCE="setuptools-$VERSION.tar.gz"
  HOMEPAGE="http://cheeseshop.python.org/pypi/setuptools"
       URL="http://cheeseshop.python.org/packages/source/s/setuptools/"
       REQ="Python"
  ESTIMATE="150"
      DESC="setuptools enables easier building and distribution of Python packages."

build() {
  [  -f  setup.py  ]  &&
  prepare_install     &&
  python setup.py install
}
