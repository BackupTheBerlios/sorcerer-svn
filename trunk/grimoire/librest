   version stable 0.6.1 0.6
   require libsoup
case $VERSION in
 0.6.1)
   require autoconf gtk-doc ;;
esac
      opts --enable-static=no
  category network/web
 attribute library makej multilib-dual
#   source http://moblin.org/sites/all/files/rest-$VERSION.tar.gz
    source http://ftp.debian.org/debian/pool/main/libr/librest/librest_$VERSION.orig.tar.gz
      info home http://moblin.org/projects/librest
      info vurl "http://moblin.org/downloads/project/262#downloads"
      desc 'representational state transfer library'

build(){
 case $VERSION in
  0.6.1) gtkdocize --flavour no-tmpl && autoreconf -v -i ;;
 esac
 default_build
}
