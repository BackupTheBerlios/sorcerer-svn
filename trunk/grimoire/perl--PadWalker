     PNAME=${SPELL#perl--}
   VERSION=( "1.0" )
  CATEGORY="perl"
 ATTRIBUTE="perl-module new"
    SOURCE="R/RO/ROBIN/$PNAME-$VERSION.tar.gz"
       URL="http://www.cpan.org/authors/id/"
  HOMEPAGE="http://search.cpan.org/search?dist=$PNAME"
       REQ=( "perl" )
  ESTIMATE="28"
      DESC="$SPELL plays with other peoples' lexical variables"
