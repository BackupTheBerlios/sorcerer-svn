# no successful compilations of this starting with version 0.2.0 nor 0.2.1
   version stable 0.2.1 0.2.0
   require grilo
      opts --enable-static=no
  category utility/search
 attribute gnome library lto makej multilib-dual optimize
#attribute gnome library lto makej multilib-concurrent optimize
attribute broke
    source $GNOME_URL sources/grilo-plugins/${VERSION:0:3}/grilo-plugins-$VERSION.tar.xz
    source git grilo-$VERSION.tar master git://git.gnome.org/grilo-plugins
      info last 20120925
      info home http://www.gnome.org/
      info wiki http://live.gnome.org/Grilo
      info mail http://mail.gnome.org/mailman/listinfo/grilo-list
      info irc  "#grilo on GIMPNet"
      desc 'plugins for grilo'
