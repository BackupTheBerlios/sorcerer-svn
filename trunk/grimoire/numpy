    stable 1.3.0 1.2.1 1.2.0 1.1.1 1.1.0
  unstable 1.4.0rc1 1.3.0rc2 1.3.0rc1
   require Python
  category development
 attribute python-module
    source $SOURCEFORGE_URL numpy/NumPy/$VERSION/numpy-$VERSION.tar.gz
  homepage http://numpy.sourceforge.net/
  estimate 1300
      desc 'numpy adds a fast and sophisticated array facility to Python.'

build(){
[ -f setup.py ] &&
prepare_install &&
python setup.py install
}
