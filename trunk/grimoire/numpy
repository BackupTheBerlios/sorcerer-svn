    stable 1.2.1 1.2.0 1.1.1 1.1.0
   require Python
  category development
 attribute python-module
       url $SOURCEFORGE_URL
    source numpy/numpy-$VERSION.tar.gz
  homepage http://numpy.sourceforge.net/
  estimate 1300
      desc 'numpy adds a fast and sophisticated array facility to Python.'

build(){
[ -f setup.py ] &&
prepare_install &&
python setup.py install
}
