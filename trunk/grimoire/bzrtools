    stable 1.17.0 1.16.0 1.15.0 1.14.0 1.13.0 1.12.0 1.11.0 1.10.0
   require Python
  category development/interpreter/python
 attribute python-module
#   source http://launchpad.net/bzrtools/stable/${VERSION:0:6}/+download/bzrtools-$VERSION.tar.gz
    source http://launchpad.net/bzrtools/stable/${VERSION:0:4}/+download/bzrtools-$VERSION.tar.gz
      vurl http://bazaar-vcs.org/BzrTools
  homepage http://bazaar-vcs.org/BzrTools
  estimate 100
      desc 'extra functionality for bzr (Bazaar-NG)'

build(){
prepare_install &&
python setup.py install
}
