# This should be of the same version as bzr plus a second dot and a digit
   VERSION=( 1.5.0 1.2.0 1.1.0 1.0.0 0.92.0 0.91.0 0.90.0 )
  CATEGORY='development/interpreter/python'
 ATTRIBUTE='python-module new'
    SOURCE="$VERSION/+download/bzrtools-$VERSION.tar.gz"
       URL='http://launchpad.net/bzrtools/stable/'
#      URL='http://panoramicfeedback.com/opensource/'
      VURL='http://bazaar-vcs.org/BzrTools'
  HOMEPAGE='http://bazaar-vcs.org/BzrTools'
#      URL='http://panoramicfeedback.com/'
# HOMEPAGE='http://bazaar-vcs.org'
       REQ='Python'
  ESTIMATE='100'
      DESC='bzrtools provide extra functionality for bzr (Bazaar-NG)'

build(){
 prepare_install &&
 python setup.py install
}
