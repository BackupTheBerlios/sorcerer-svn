# This should be of the same version as bzr plus a second dot and a digit

    stable 2.1.0 2.0.1
   require Python
  category development/interpreter/python
 attribute python-module
#   source http://launchpad.net/bzrtools/stable/$VERSION/+download/bzrtools-$VERSION.tar.gz
    source http://launchpad.net/bzrtools/trunk/$VERSION/+download/bzrtools-$VERSION.tar.gz
      vurl http://bazaar-vcs.org/BzrTools
  homepage http://bazaar-vcs.org/BzrTools
  estimate 100
      desc 'extra functionality for bzr (Bazaar-NG)'

build(){
prepare_install &&
python setup.py install
}
