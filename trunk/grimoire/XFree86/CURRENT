if  [  -z  "$BRANCH"  ];  then
  FCPC="/usr/lib/pkgconfig/fontconfig.pc"
  if    grep  -q    "@VERSION@"          $FCPC
  then  sed   -i  "s:@VERSION@:2.1.92:"  $FCPC
        md5sum  $FCPC  >>  /var/log/sorcery/md5sum/XFree86
  fi

  case  $VERSION  in
    4.3.0)  bzcat  $COMPILE_LOGS/XFree86.bz2  |
            grep  -q  -x  "Adding fontconfig-2.1.92"  ;;
  esac

fi
