    version stable 1.1
   category game development/emulator
  attribute archive_off data
     source $SOURCEFORGE_URL scummvm/extras/Drascula_%20The%20Vampire%20Strikes%20Back/drascula-int-$VERSION.zip
       info good 20120716
       info last 20101005
       desc 'Drascula: The Vampire Strikes Back, Spanish German French Italian Addon'

build(){
 chmod -R 644 */*
 mkdir -pvm 755 $DESTDIR/usr/share/games/drascula &&
 cp    -av *    $DESTDIR/usr/share/games/drascula/
}
