#!/bin/bash

export  PATH="/sbin:/bin:/usr/sbin:/usr/bin"

export  PATH
umask   022
trap    ":" INT QUIT TSTP

for ((SEQ=0;SEQ<10;SEQ++));  do
  for  SCRIPT  in  /etc/rc.d/rcS.d/S${SEQ}*;  do
    if    [  -f  "$SCRIPT"  ]  &&
          [  -x  "$SCRIPT"  ]
    then          $SCRIPT  start
    fi
  done
done
