    stable 3.5.0 3.4.2-2
  category network
 attribute library
       url $SOURCEFORGE_URL
    source libassa/libassa-$VERSION.tar.gz
      opts --enable-doxygen=no --enable-dot=no --enable-static=no
  homepage http://libassa.sourceforge.net
  estimate 450
      desc 'libassa is a networking library.
libassa is a networking library based on some of the design
patterns such as Configurator, Reactor, Acceptor, and Connector
collectively known as Adaptive Communication Patterns described
in various papers published by Dr. D. C. Schmidt.'
