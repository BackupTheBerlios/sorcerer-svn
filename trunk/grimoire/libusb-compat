    stable 0.1.2 0.1.1 0.1.0
   require libusb
  category utility
 attribute library
       url $SOURCEFORGE_URL
    source libusb/libusb-compat-$VERSION.tar.bz2
      opts --disable-build-docs --enable-static=no
