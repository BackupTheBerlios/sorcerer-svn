    stable 0.1.3 0.1.2 0.1.1 0.1.0
   require libusb
  category utility
 attribute library
    source $SOURCEFORGE_URL libusb/libusb-compat-${VERSION:0:3}/libusb-compat-$VERSION/libusb-compat-$VERSION.tar.bz2
      opts --disable-build-docs --enable-static=no
  homepage http://libusb.sourceforge.net
  estimate 100
      desc 'version 0.1.0 compatibility layer for libusb 1.0.x'
