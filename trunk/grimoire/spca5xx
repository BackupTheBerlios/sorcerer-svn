   VERSION=( "20060301" "20060202" "20060101" )
  CATEGORY="video/webcam"
 ATTRIBUTE="linux-26-module new"
    SOURCE="spca5xx-$VERSION.tar.gz"
       URL="http://mxhaard.free.fr/spca50x/Download/"
  HOMEPAGE="http://mxhaard.free.fr/index.html"
  ESTIMATE="29"
      DESC="spca5xx provides driver for spca5xx based chipset webcam.
For supported devices please see
http://mxhaard.free.fr/spca5xx.html
Many webcams are supported by this driver."

build()  {
  make    clean    &&
  make             &&
  prepare_install  &&
  make    install
}


