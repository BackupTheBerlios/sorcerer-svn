   version stable 0.5.4 0.5.3 0.5.2 0.5.1
   require jpeg libXft libXpm libpng
  category utility/terminal-emulator
 attribute x11
    source $SOURCEFORGE_URL materm/mrxvt%20source/$VERSION/mrxvt-$VERSION.tar.gz
      info home http://materm.sourceforge.net/
      opts --disable-debug --enable-everything --with-save-lines=256
      desc 'mrxvt is a feature filled tabbed terminal emulator based on rxvt and aterm.'
