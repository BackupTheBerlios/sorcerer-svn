   VERSION=( "0.8.5" "0.8.4" "0.8.3" "0.8.2" )
  CATEGORY="audio"
 ATTRIBUTE="library"
  HOMEPAGE="http://www.xiph.org/ogg/vorbis"
       URL="http://www.xiph.org/ao/src"
       REQ="audiofile"
    OPT[0]="esound:::for output through esound mixer"
    OPT[1]="arts:::for output through arts sound mixer"
    OPT[2]="alsa,lib:::for output through alsa"
 FRESHMEAT="libao"
  ESTIMATE="137"
     SHORT="libao is a cross-platform audio library."
stream << EOF
libao is a way to easily add cross-platform 
audio output to your program.
Output devices are implemented as dynamic plugins, 
and  support for OSS, ESD, ALSA, aRTs, Solaris, 
and Irix outputs are currently supported.
EOF
