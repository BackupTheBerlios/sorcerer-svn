   VERSION=( "1.5.2.5" "1.5.2.4" "1.5.2.2" )
    IGNORE="rc"
  CATEGORY="development documentation"
 ATTRIBUTE="console"
    SOURCE="/pub/software/scm/git/git-manpages-$VERSION.tar.bz2"
       URL="$KERNEL_URL"
  HOMEPAGE="http://git.or.cz/"
  ESTIMATE="140"
      DESC="git-manpages provides man pages for git"

build() {
  MAN_DIR=/usr/share/man

  prepare_install		&&
  install -v -d  $MAN_DIR	&&
  cp -v -r *     $MAN_DIR
}
