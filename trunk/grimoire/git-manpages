   VERSION=( 1.5.4 1.5.3.8 1.5.3.7 1.5.3.6 1.5.3.5 1.5.3.4 1.5.3.3 1.5.3.2 1.5.2.5 1.5.2.4 1.5.2.2 )
    IGNORE='rc'
  CATEGORY='development documentation'
 ATTRIBUTE='console'
    SOURCE="/pub/software/scm/git/git-manpages-$VERSION.tar.bz2"
       URL="$KERNEL_URL"
  HOMEPAGE='http://git.or.cz/'
  ESTIMATE='10'
      DESC='git-manpages provides man pages for git'

build(){
 MAN_DIR=/usr/share/man
 prepare_install &&
 install -vd $MAN_DIR &&
 cp -vr * $MAN_DIR
}
