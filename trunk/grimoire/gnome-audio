   version stable 2.22.2 2.22.1 2.0.0 1.4.0
  category audio/sounds
 attribute gnome
    source $GNOME_URL sources/gnome-audio/${VERSION:0:4}/gnome-audio-$VERSION.tar.gz
      info home http://www.gnome.org
      desc 'collection of sounds for use with Gnome'

build(){ make install prefix=$DESTDIR/usr; }
