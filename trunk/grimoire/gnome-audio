    stable 2.22.2 2.22.1 2.0.0 1.4.0
  category audio/sounds
 attribute gnome
    source sources/gnome-audio/${VERSION:0:3}/gnome-audio-$VERSION.tar.gz
  homepage http://www.gnome.org
       url "$GNOME_URL"
  estimate 10
      desc 'gnome-audio contains a collection of sounds for use with Gnome.'
build(){
 prepare_install &&
 make    install prefix=/usr
}
