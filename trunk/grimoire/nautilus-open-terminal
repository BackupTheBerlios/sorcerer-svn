   version stable 0.18 0.9
   require nautilus
  category utility/file-manager
 attribute gnome
    source $GNOME_URL sources/nautilus-open-terminal/${VERSION:0:4}/nautilus-open-terminal-$VERSION.tar.bz2
  homepage http://www.gnome.org
      opts --enable-static=no
      desc 'a nautilus extension for opening a terminal in arbitrary local folders'
