     PNAME=${SPELL#perl--}
   VERSION=( "2.30" )
  CATEGORY="perl"
 ATTRIBUTE="perl-module new"
    SOURCE="J/JS/JSTOWE/$PNAME-$VERSION.tar.gz"
       URL="http://www.cpan.org/authors/id/"
  HOMEPAGE="http://search.cpan.org/search?dist=$PNAME"
       REQ=( "perl" )
  ESTIMATE="15"
      DESC="$SPELL is a module for simple terminal control"

