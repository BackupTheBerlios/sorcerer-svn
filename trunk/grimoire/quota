   VERSION=( 3.16 3.15 3.14 3.13 3.12 )
  CATEGORY='disk'
 ATTRIBUTE='console'
    SOURCE="linuxquota/quota-$VERSION.tar.gz"
       URL="$SOURCEFORGE_URL"
       REQ=( 'e2fsprogs'
             ':--enable-rpc=yes:--enable-rpc=no:Enable RPC support'
             ':--enable-rpcsetquota=yes:--enable-rpcsetquota=no:Use RPC for setting quotas' )
  HOMEPAGE='http://sourceforge.net/projects/linuxquota'
  ESTIMATE='230'
      DESC='quota provides tools for adjusting disk quotas.'
