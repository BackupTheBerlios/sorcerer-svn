    stable 3.17 3.16 3.15 3.14 3.13 3.12
   require e2fsprogs
  switch --enable-rpc=yes         --enable-rpc=no         to enable RPC support
  switch --enable-rpcsetquota=yes --enable-rpcsetquota=no to use RPC for setting quotas
  category disk
 attribute console
       url $SOURCEFORGE_URL
    source linuxquota/quota-$VERSION.tar.gz
  homepage http://sourceforge.net/projects/linuxquota
  estimate 200
      desc 'quota provides tools for adjusting disk quotas.'
