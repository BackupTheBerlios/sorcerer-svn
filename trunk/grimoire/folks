# folks 0.3.0 will not compile even with introspection disabled
# multilib-fail because telepathy-glib is also multilib-fail

    stable 0.2.0
  unstable 0.3.4 0.3.3
case $VERSION in
 0.3.0)
      opts --enable-introspection=no ;;
esac
   require libgee libxml2 telepathy-glib
  category development
 attribute gnome library multilib-fail
    source $GNOME_URL sources/folks/${VERSION:0:3}/folks-$VERSION.tar.bz2
      home http://telepathy.freedesktop.org/wiki/Folks
  estimate 450
      desc 'library that aggregates people from multiple sources
(eg, Telepathy connection managers) to create metacontacts.'
