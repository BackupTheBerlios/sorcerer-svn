# Not multilib while evolution-data-server is not multilib.

   version   stable 0.6.9 0.6.8 0.6.7 0.6.6 0.6.3.2
   version unstable 0.6.9 0.6.8
   require dconf evolution-data-server libgee libxml2 perl--PathTools perl--Scalar-List-Utils telepathy-glib
  category development
#attribute gnome library makej multilib-fail
 attribute gnome library makej multilib-concurrent
    source $GNOME_URL sources/folks/${VERSION:0:3}/folks-$VERSION.tar.xz
      info last 20120416
      info home http://telepathy.freedesktop.org/wiki/Folks
      desc 'library that aggregates people from multiple sources
(eg, Telepathy connection managers) to create metacontacts.'

build(){
 case $VERSION in
  0.6.6) sed -i 's:1\.30:0.10.8:' configure ;;
  0.6.7) sed -i 's:1\.30:0.10.8:' configure ;;
 esac
 default_build
}
