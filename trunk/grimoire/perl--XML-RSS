     PNAME=${SPELL#perl--}
   VERSION=( "1.10" )
  CATEGORY="perl"
 ATTRIBUTE="perl-module new"
    SOURCE="A/AB/ABH/$PNAME-$VERSION.tar.gz"
       URL="http://www.cpan.org/authors/id/"
  HOMEPAGE="http://search.cpan.org/search?dist=$PNAME"
       REQ=( "perl perl--XML-Parser perl--Test-Manifest" )
  ESTIMATE="15"
      DESC="$SPELL module creates and updates RSS files"
