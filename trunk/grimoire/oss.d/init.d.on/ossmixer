#!/bin/bash

### BEGIN INIT INFO
# Required-Start: $local_fs
# Should-Start: osssound
# Default-Start: 1 2 3 4 5
# Short-Description: ossmixer loads previously saved mixer settings
### END INIT INFO

. /lib/lsb/init-functions

NAME=savemixer
SERV=/usr/sbin/savemixer
OPTS=-L

start(){
 log_warning_msg "About to run $SERV $OPTS"
 if $SERV $OPTS
 then log_success_msg "$SERV exited successfully"
 else log_failure_msg "$SERV exited with an error"
 fi
}

    restart(){ stop; start; }
try_restart(){ restart; }
parse "$@"
