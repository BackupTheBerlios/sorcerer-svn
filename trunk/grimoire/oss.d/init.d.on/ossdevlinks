#!/bin/bash

### BEGIN INIT INFO
# Default-Mode: 710
# Required-Start: $local_fs
# Should-Start: osssound
# Default-Start: 1 2 3 4 5
# Short-Description: ossdevlinks creates legacy symbolic links for OSS device nodes
### END INIT INFO

. /lib/lsb/init-functions

NAME=ossdevlinks
SERV=/usr/sbin/ossdevlinks

start(){
 log_warning_msg "About to run $SERV"
 if $SERV
 then log_success_msg "$SERV exited successfully"
 else log_failure_msg "$SERV exited with an error"
 fi
}
       stop(){ :; }
    restart(){ start; }
try_restart(){ start; }
parse "$@"
