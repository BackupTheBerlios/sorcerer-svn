    OPT[0]="x11:--enable-graphics:--without-x:for x11 graphics"
    OPT[1]="svgalib:--enable-graphics:--without-svgalib:for svga graphics"
    OPT[2]="libpng:::for displaying portable network graphics"
    OPT[3]="jpeg:::for displaying jpeg graphics format"
    OPT[4]="tiff:::for displaying TIFF images"
    OPT[5]="flex:--enable-javascript --with-libfl::for javascript support"
    OPT[6]="gpm:::to use the mouse when in text mode"
