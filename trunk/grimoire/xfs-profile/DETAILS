   VERSION=( "1.2" "1.0" )
  CATEGORY="profile"
 ATTRIBUTE="console library"
  HOMEPAGE="http://oss.sgi.com/projects/xfs"
      REQ="attr acl xfsprogs dmapi xfsdump"
    OPT[0]="x11:::for x11 desktop"
    ASSUME="off"
     SHORT="xfs-profile installs spells useful for the XFS enabled boxes."
cat << EOF
xfs-profile contains:

attr		Extended Attribute (EA) support for XFS
acl		Access Control lists for XFS
xfsprogs	XFS file system utilities and librarise
dmapi		Data Management API for XFS
xfsdump		dump and restore utilities for XFS
EOF
