   VERSION=( "2.6.3.3" "2.6.3.2" "2.6.3.0" "2.6.2.1" "2.6.1.0" "2.6.0.1" "2.6.0.0" "2.5.5.1" "2.5.4.1" "2.5.3.1" )
# Who can possibily use this?
# They keep releasing versions that will not compile.
  CATEGORY="graphic"
 ATTRIBUTE="library python-module x11 new broken"
# Contains depreciated c++ syntax and does not compile wxPython module.
    SOURCE="wxpython/wxPython-src-$VERSION.tar.bz2"
  HOMEPAGE="http://wxpython.org"
       URL="$SOURCEFORGE_URL"
case  $VERSION  in
  2.6.3.0)  USE_GCC="gcc-v3.4"  ;;
  2.6.2.*)  USE_GCC="gcc-v3.4"  ;;
esac
       REQ="Python gtk+"
  ESTIMATE="413"
      DESC="wxPython is Python extention module that wraps wxGTK library."

build() {
default_build  &&
cd  wxPython   &&
python setup.py build_ext install
}
