# solo becuase cmake requires libidn
# and libidn libraries may be unavailable for an instant
# when libidn is upgrading.

    stable 1.19 1.18 1.16
      opts --enable-static=no
  category network/dns
 attribute library makej solo
    source $GNU_URL libidn/libidn-$VERSION.tar.gz
  homepage http://www.gnu.org/software/libidn/
  estimate 900
      desc 'internationalized domain names library'
