    stable 0.9.8
   require gtk+

  category network/chat
 attribute x11
    source gyach-$VERSION.tar.gz
       url http://www4.infi.net/~cpinkham/gyach/code/
# homepage http://www4.infi.net/~cpinkham/gyach/
  homepage http://gyach.bc2va.org/
      vurl http://gyach.bc2va.org/
  estimate 400
      desc 'gyach is a yahoo instant messenger chat client.
It supports colors, emotes/aliases, PMs, set status,
remote user status display,  ignore lists (local, non-Yahoo! based),
a regex list of triggers to cause users to be automatically ignored,
command-line recall via arrow keys, and tab completion of usernames.'
