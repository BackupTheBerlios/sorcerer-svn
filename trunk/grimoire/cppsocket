   VERSION=( "0.8.4" "0.8.3" )
  CATEGORY="development"
 ATTRIBUTE="library x11"
    SOURCE="cppsocket/cppsocket-$VERSION"
       URL="$SOURCEFORGE_URL"
  HOMEPAGE="http://cppsocket.sourceforge.net"
 FRESHMEAT="cppsocket"
     SHORT="cppsocket is a library for easy socket programming."
stream << EOF
CPPSocket is a small Classlibrary for C++ that supports 
a easy usage of socket-programming for networking-software.
It provides a simple object-oriented 
interface to the classic C library calls.
EOF
