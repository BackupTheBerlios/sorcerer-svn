   VERSION=( 0.8.4 0.8.3 )
    IGNORE="api"
  CATEGORY='development'
 ATTRIBUTE='library x11'
    SOURCE="cppsocket/cppsocket-$VERSION.tar.gz"
       URL="$SOURCEFORGE_URL"
