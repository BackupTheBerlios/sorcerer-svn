     PNAME=${SPELL#perl--}
   VERSION=( "2.01" )
  CATEGORY="perl"
 ATTRIBUTE="perl-module new"
    SOURCE="C/CH/CHAMAS/$PNAME-$VERSION.tar.gz"
       URL="http://www.cpan.org/authors/id/"
  HOMEPAGE="http://search.cpan.org/search?dist=$PNAME"
       REQ=( "perl perl--Data-Dumper perl--Storable perl--FreezeThaw" )
  ESTIMATE="20"
      DESC="$SPELL stores multidimensional hash structures in tied hashes
(including DBM) files.
MLDBM
MLDBM::Serializer::Data::Dumper
MLDBM::Serializer::FreezeThaw
MLDBM::Serializer::Storable
