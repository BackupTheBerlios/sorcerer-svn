    stable 20001218
   require chromium
 attribute data
       url $SOURCEFORGE_URL
    source chromium-bsu/IncArnedine.zip
  homepage http://chromium-bsu.sourceforge.net/
      desc 'IncArnedine provides alternate graphics for chromium.'

build(){
prepare_install &&
mkdir   -pvm 755   /usr/share/games/chromium/png &&
install  -vm 644 * /usr/share/games/chromium/png/
}
