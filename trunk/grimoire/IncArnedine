# Version never changes

   version stable 20001218
   require chromium
 attribute data
    source $SOURCEFORGE_URL chromium-bsu/IncArnedine.zip
      info good 20120528
      info home http://chromium-bsu.sourceforge.net/
      desc 'alternate graphics for chromium'

build(){
 mkdir   -pvm 755   $DESTDIR/usr/share/games/chromium/png
 install  -vm 644 * $DESTDIR/usr/share/games/chromium/png/
}
