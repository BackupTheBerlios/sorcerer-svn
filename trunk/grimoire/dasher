# 4.10.1 requires at-spi and not at-spi2-atk

   version stable 4.11 4.10.1 4.10.0
   require at-spi
#  require at-spi libgnomeui
#  require at-spi gnome-speech libgnomeui libwnck
  category input utility/accessibility
 attribute gnome
 attribute broke
    source $GNOME_URL sources/dasher/${VERSION:0:4}/dasher-$VERSION.tar.bz2
      info home  http://www.gnome.org/
      desc 'zooming predictive text entry system
It is designed for situations where keyboard input is
impractical (for instance, accessibility or PDAs).
It is usable with highly limited amounts of physical
input while still allowing high rates of text entry.'

build(){
 case $VERSION in
  4.11) sed -i 's: Doc : :' Makefile.in ;;
 esac
 default_build
}
