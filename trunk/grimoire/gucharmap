   require libgnomeui xcb-util
  optional gnome-doc-utils '' '' for documentation generation
  category font/editor
 attribute gnome
    source $GNOME_URL sources/gucharmap/${VERSION:0:4}/gucharmap-$VERSION.tar.bz2
  homepage http://gucharmap.sourceforge.net
  estimate 1000
      desc 'character map and font viewer
gucharmap is a Unicode/ISO 10646 character map and font viewer.
It supports anti-aliased, scalable truetype fonts in X, using Xft, 
and works on Unix and Windows platforms.'
