   VERSION=( "0.6.3" "0.6.2" )
  CATEGORY="audio-mixer"
 ATTRIBUTE="kde new"
    SOURCE="kamix/kamix-$VERSION.tar.bz2"
#    SOURCE="kamix/kamix-$VERSION.tar.gz"
       URL="$SOURCEFORGE_URL"
  HOMEPAGE="http://kamix.sourceforge.net"
       REQ="alsa-lib kdelibs"
  ESTIMATE="144"
      DESC="kamix is a mixer for KDE and ALSA, with more features than kmix
(or at least with more features than kmix had when kamix was started).
It supports channel splitting, levels storing/restoring,
sync with external ALSA events, correct handling of "enumerated" elements,
and selective item hiding/showing."
