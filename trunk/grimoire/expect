# expect 5.44.1 nor 5.43.0 is compatible with tcl 8.5.1
# 5.44.0 has bugs
# 5.44.1 has compile error

    stable 5.44.1 5.43.0 5.42.1
   require tcl
  optional tk '' '' for wrapping applications in x11 GUIs

    ignore '5\.44\.[0-1]'
    TCLVER='8.5.3'
  category utility/editor
 attribute console x11 broke
    source expect-$VERSION.tar.gz
    source tcl/tcl${TCLVER}-src.tar.gz
       url http://expect.nist.gov/src/
       url "$SOURCEFORGE_URL"
  homepage http://expect.nist.gov
      opts --with-tcl=/usr/lib --with-tclinclude=tcl$TCLVER/generic
  estimate 100
      desc 'expect is a tool for automating interactive applications.
expect is a tool for automating interactive applications 
such as telnet, ftp, passwd, fsck, rlogin, tip, etc.'

build(){
 # Version 5.44.1 has a messed up configure script
 case $VERSION in
  5.44.1) unset CFLAGS CXXFLAGS LDFLAGS ;;
 esac
 default_build
}
