    stable 2.7 2.6.3 2.6.1 2.6
   require gettext pcre
      opts --bindir=/bin
  category utility
 attribute console solo
    source $GNU_URL grep/grep-$VERSION.tar.xz
  homepage http://www.gnu.org/software/grep/grep.html
   protect /bin/grep
  estimate 800
      desc 'finds lines that match specified patterns'
