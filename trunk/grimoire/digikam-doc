    stable 0.9.5 0.9.4 0.9.3 0.9.1 0.8.2
  category graphic/viewer graphic/editor
 attribute x11
   require doxygen
    source $SOURCEFORGE_URL digikam/digikam-doc/$VERSION/digikam-doc-$VERSION.tar.bz2
  homepage http://www.digikam.org
      opts --prefix=/opt/kde3 --with-qt-dir=/opt/kde3 --disable-debug --enable-final
  estimate 500
      desc 'digikam-doc provides documentation for digikam'

build() {
 case $VERSION in 
    0.9.5) patch -p1 < $SCRIPT_DIR/pt_digikam.patch
           autoconf
 esac

 default_build
}
