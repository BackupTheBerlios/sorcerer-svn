    stable 0.7.2
   require kiwi
  category graphic
 attribute gnome python-module
       url $GNOME_URL
    source sources/gazpacho/${VERSION:0:3}/gazpacho-$VERSION.tar.bz2
  homepage http://gazpacho.sicem.biz
  estimate 300
      desc 'gazpacho is a GUI creator which started as a clone of glade3.'

build(){
 prepare_install &&
 python  setup.py install
}
