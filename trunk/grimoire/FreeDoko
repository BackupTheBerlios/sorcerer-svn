    stable 0.7.6.src 0.7.5.src 0.7.4.src 0.7.3.src
   require gnet gtkmm-v2.2
  category game
 attribute x11
 attribute broke
    source $SOURCEFORGE_URL free-doko/FreeDoko%20source/0.7.6/FreeDoko_$VERSION.zip
  homepage http://free-doko.sourceforge.net
  estimate 300
      desc 'Doppelkopf card game
The game Doppelkopf is a card game for four
or more players similar to Skat or xskat.'

build(){
[ -d  FreeDoko_$VERSION ] &&
cd    FreeDoko_$VERSION
cd    src                 &&
#sed  -i  "s:-2.0:-2.4:g" \
#     Makefile.os         &&
make OSTYPE=linux-gnu     \
     CXXFLAGS="$CXXFLAGS" &&
cd   ..                   &&
prepare_install           &&
make    install
}
