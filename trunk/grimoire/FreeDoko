    stable 0.7.4 0.7.3
   require gnet gtkmm-v2.2

    ignore 'linux\|exe\|Linux\|hpux\|manual\|windows\|pre'
  category game
 attribute broke x11 new
# Compile fine, however no method for installation
    source free-doko/FreeDoko_$VERSION.src.zip
       url "$SOURCEFORGE_URL"
  homepage http://free-doko.sourceforge.net
  estimate 300
      desc 'FreeDoko is a Doppelkopf card game.
The game Doppelkopf is a card game for four
or more players similar to Skat or xskat.'

build(){
 [ -d  FreeDoko_$VERSION ] &&
 cd    FreeDoko_$VERSION
 cd    src                 &&
 #sed  -i  "s:-2.0:-2.4:g" \
 #     Makefile.os         &&
 make OSTYPE=linux-gnu     \
      CXXFLAGS="$CXXFLAGS" &&
 cd   ..                   &&
 prepare_install           &&
 make    install
}
