   VERSION=( "1.0.11" "1.0.7" )
  CATEGORY="geneology"
 ATTRIBUTE="gnome new"
    SOURCE="gramps/gramps-$VERSION.tar.gz"
       URL="$SOURCEFORGE_URL"
  HOMEPAGE="http://gramps.sourceforge.net"
       REQ="gnome-python"
  ESTIMATE="163"
      DESC="gramps is a genealogical application.
The name is an acronym for Genealogical Research
and Analysis Management Programming System."

build(){
if    [  -x  autogen.sh  ]
then       ./autogen.sh
fi  &&
default_build
}
