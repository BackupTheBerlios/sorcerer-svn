    stable 3.0.1 3.0.0 2.2.10 2.2.9 2.2.8
   require gnome-common gnome-python

    ignore 'rc\|RC\|beta'
  category utility/geneology
 attribute gnome
    source gramps/gramps-$VERSION.tar.gz
       url "$SOURCEFORGE_URL"
  homepage http://gramps.sourceforge.net
  estimate 2000
      desc 'gramps is a genealogical application.
The name is an acronym for Genealogical Research
and Analysis Management Programming System.'

build(){
if   [ -x autogen.sh  ]
then    ./autogen.sh
fi &&
default_build
}
