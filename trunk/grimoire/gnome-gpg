   version stable 0.6.0
   require gnupg libgnomeui
  category development
 attribute gnome library
    source $GNOME_URL sources/gnome-gpg/${VERSION:0:3}/gnome-gpg-$VERSION.tar.bz2
  homepage http://www.gnome.org
      opts --enable-static=no
  estimate 60
      desc 'command line wrapper for gpg
that stores passphrases in gnome-keyring.'
