   VERSION=( 0.6.0 )
  CATEGORY='development'
 ATTRIBUTE='gnome library'
    SOURCE="sources/gnome-gpg/${VERSION:0:3}/gnome-gpg-$VERSION.tar.bz2"
  HOMEPAGE='http://www.gnome.org'
       URL="$GNOME_URL"
       REQ=( 'gnupg libgnomeui' )
      OPTS='--enable-static=no'
  ESTIMATE='60'
      DESC='gnome-gpg provides a command line wrapper for gpg
that stores passphrases in gnome-keyring.'
