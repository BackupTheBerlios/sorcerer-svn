    stable 0.6.0
   require gnupg libgnomeui
  category development
 attribute gnome library
       url $GNOME_URL
    source sources/gnome-gpg/${VERSION:0:3}/gnome-gpg-$VERSION.tar.bz2
  homepage http://www.gnome.org
      opts --enable-static=no
  estimate 60
      desc 'gnome-gpg provides a command line wrapper for gpg
