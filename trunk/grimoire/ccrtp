   version stable 1.8.0 1.7.2 1.7.1 1.7.0
   require commoncpp2
  category network/telephone
 attribute library makej multilib-concurrent
    source $GNU_URL ccrtp/ccrtp-$VERSION.tar.gz
      info last  20110522
      info home  http://www.twinklephone.com/
      desc 'RTP protocol stack'

build(){
 case $VERSION in
  1.8.0) sed -i 's:-L$COMMON_LIBPATH::' configure ;;
  1.7.2) sed -i 's:-L$COMMON_LIBPATH::' configure ;;
 esac
 default_build
}
