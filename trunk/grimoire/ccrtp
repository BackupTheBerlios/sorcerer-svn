  stable 1.6.1 1.6.0 1.5.2 1.5.1 1.5.0 1.4.1 1.4.0
  CATEGORY='network/telephone'
 ATTRIBUTE='library'
    SOURCE="ccrtp/ccrtp-$VERSION.tar.gz"
       URL="$GNU_URL"
  HOMEPAGE='http://www.twinklephone.com/'
       REQ='commoncpp2'
  ESTIMATE='500'
      DESC='ccrtp provides a RTP protocol stack'

build(){
 case $VERSION in
  1.6.0) sed -i '1i#include <cstdlib>' src/control.cpp
         ;;
 esac
 default_build
}
