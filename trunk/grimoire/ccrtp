    stable 1.7.2 1.7.1 1.7.0
   require commoncpp2
  category network/telephone
 attribute library
    source $GNU_URL ccrtp/ccrtp-$VERSION.tar.gz
      home http://www.twinklephone.com/
  estimate 500
      desc 'RTP protocol stack'

build(){
 case $VERSION in
  1.7.2) sed -i 's:-L$COMMON_LIBPATH::' configure ;;
  1.6.0) sed -i '1i#include <cstdlib>' src/control.cpp ;;
 esac
 default_build
}
