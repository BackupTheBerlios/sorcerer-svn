    stable 1.34.7 1.34.5 1.34.3 1.34.1 1.34.0 1.32 1.30
   require bison flex libselinux
  category security
 attribute console
    source http://www.nsa.gov/selinux/archives/checkpolicy-$VERSION.tgz
  homepage http://www.nsa.gov/selinux/
      vurl http://www.nsa.gov/selinux/code/download-stable.cfm
  estimate 30
      desc 'checkpolicy contains the policy compiler needed to build policies.'

build(){
make "CFLAGS=$CFLAGS" &&
prepare_install &&
make    install
}
