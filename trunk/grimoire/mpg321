   version stable 0.2.12-1 0.2.11-3 0.2.11-2 0.2.11 0.2.10
   require libao libid3tag libmad
  category audio/player
 attribute console
    source $SOURCEFORGE_URL mpg321/mpg321/${VERSION:0:6}/mpg321-$VERSION.tar.gz
  homepage http://mpg321.sourceforge.net
      opts --with-default-audio=alsa
  estimate 150
      desc 'free replacement for mpg123, a command-line mp3 player
mpg321 is a free replacement for mpg123,
a very popular command-line mp3 player.'
