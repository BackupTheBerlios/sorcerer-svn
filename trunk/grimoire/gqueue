   VERSION=( "0.99.1" )
  CATEGORY="printer"
 ATTRIBUTE="x11 new broken"
# Compiles fine.  Crashes when run
    SOURCE="gqueue-$VERSION.tar.bz2"
  HOMEPAGE="http://web.tiscali.it/diegobazzanella/gqueue.html"
       URL="http://web.tiscali.it/diegobazzanella/"
       REQ="libgnomeui"
  ESTIMATE="200"
      DESC="gqueue provides a gtk+ frontend for lpq and lprm for working with cups queues."
build()  {
  case  $VERSION  in
    0.99.1)  rm  -f                   mkinstalldirs
             cp  /usr/share/automake*/mkinstalldirs .
             aclocal
             automake -af
             grep  -r -l            "@mkdir_p@" *  2>/dev/null |
             xargs -r -l64 sed -i "s:@mkdir_p@:mkdir -p:"  ;;
  esac
  default_build
}
