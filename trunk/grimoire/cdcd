# 0.6.6 no longer compiles

   version stable 0.6.6
   require libcdaudio ncurses readline
  category audio/cd
 attribute console
 attribute broke
    source $SOURCEFORGE_URL libcdaudio/cdcd/$VERSION/cdcd-$VERSION.tar.gz
      info omit 20130101
      info last 20040404
      info home http://libcdaudio.sourceforge.net/
      desc 'command line CD player'

build(){
 case $VERSION in
  0.6.6) sed -i 's:exit 1:exit 0:' config.sub ;;
 esac
 default_build
}
