   version stable 0.3.1
   require SDL_image SDL_mixer automake libtool
  category game
 attribute x11
    source $SOURCEFORGE_URL pinball/pinball/pinball-$VERSION/pinball-$VERSION.tar.gz
  homepage http://pinball.sourceforge.net
  estimate 400
      desc 'emillia pinball project'

build(){
sed -i "s:// using:using:"      base/Private.h
sed -i "s:<vector>:<vector.h>:" base/Group.h
default_build
}
