    stable 0.3.1
   require SDL_image SDL_mixer automake libtool
    ignore '^1\.0\|030'
  category game
 attribute x11
       url $SOURCEFORGE_URL
    source pinball/pinball-$VERSION.tar.gz
  homepage http://pinball.sourceforge.net
  estimate 400
      desc 'pinball is the emillia pinball project.'

build(){
sed -i "s:// using:using:"      base/Private.h
sed -i "s:<vector>:<vector.h>:" base/Group.h
default_build
}
