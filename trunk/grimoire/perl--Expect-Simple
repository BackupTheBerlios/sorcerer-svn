     PNAME=${SPELL#perl--}
   VERSION=( "0.02" )
  CATEGORY="perl"
 ATTRIBUTE="perl-module new"
    SOURCE="D/DJ/DJERIUS/$PNAME-$VERSION.tar.gz"
       URL="http://www.cpan.org/authors/id/"
  HOMEPAGE="http://search.cpan.org/search?dist=$PNAME"
       REQ=( "perl perl--Expect" )
  ESTIMATE="15"
      DESC="$SPELL is a wrapper around the Expect module"

