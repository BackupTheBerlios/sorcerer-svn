# now provided by coreutils
    stable 1.6 1.5 1.4
      opts --prefix=$DEST_DIR/$SPELL/usr
  category utility
 attribute console deprecated
  homepage http://www.mktemp.org/
    source ftp://ftp.mktemp.org/pub/ \
           ftp://ftp.cs.colorado.edu/pub/ \
           ftp://ftp.usa.openbsd.org/ \
           ftp://mirage.informationwave.net/ \
           ftp://ftp.wiretapped.net/pub/security/host-security/ \
           mktemp/mktemp-$VERSION.tar.gz
   estimate 50
      desc 'utility for safely creating temporary file in shells scripts.
mktemp is a simple utility designed to make temporary
file handling in shells scripts be safe and simple.'
