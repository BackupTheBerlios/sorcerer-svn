   version   stable 3.2.3 3.0.0
   version unstable 3.3.4
   require evolution
      opts --enable-static=no
  category utility/office
 attribute gnome library
 attribute broke
    source $GNOME_URL sources/evolution-exchange/${VERSION:0:3}/evolution-exchange-$VERSION.tar.xz
      info home http://gnome.org/projects/evolution/stable.shtml
      desc 'Microsoft Exchange 2000 and 2003 support for evolution'

build_old(){
export LDFLAGS=' -Wl,-rpath,/usr/lib/mozilla -Wl,-rpath-link,/usr/lib/mozilla'
default_build
}
