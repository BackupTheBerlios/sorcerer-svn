     PNAME=${SPELL#perl--}
   VERSION=( "1.13" )
  CATEGORY="perl"
 ATTRIBUTE="perl-module new"
    SOURCE="M/MS/MSERGEANT/$PNAME-$VERSION.tar.gz"
       URL="http://www.cpan.org/authors/id/"
  HOMEPAGE="http://search.cpan.org/search?dist=$PNAME"
       REQ=( "perl perl--DBI sqlite" )
  ESTIMATE="33"
 EPROVIDES="DBD-SQLite"
      DESC="$SPELL is perl module for a small fast embedded SQL database engine"

