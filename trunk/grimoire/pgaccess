    stable 0_99_0_20040219 0.98.8
   require tk

  category db
 attribute x11
    source pgaccess/pgaccess-$VERSION.tgz
       url "$SOURCEFORGE_URL"
  homepage http://www.pgaccess.org/
  estimate 5
      desc 'pgaccess is TCL/TK database management tool for PostgreSQL.'

build(){
[ -f Makefile ] &&
prepare_install &&
make    install
}
