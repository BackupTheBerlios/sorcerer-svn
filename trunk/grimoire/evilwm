   VERSION=( "0.99.17.orig" )
  CATEGORY="window-manager"
 ATTRIBUTE="x11"
    SOURCE="evilwm/evilwm_$VERSION.tar.gz"
       URL="$SOURCEFORGE_URL"
  HOMEPAGE="http://evilwm.sourceforge.net"
       REQ="x11"
  ESTIMATE="9"
      DESC="evilwm is a light weight window manager."


build()  {
  make  allinone   &&
  prepare_install  &&
  make    install
}
