# granted trait library because it installes a pkgconfig file makej multilib-concurrent

with version   stable 7.2.1 7.2.0
#case $VERSION in
# 7.2.0)
#      with also --enable-specs=no ;;
#esac
with role    xorg/proto
with trait   X11R7 archive_off library makej multilib-concurrent
with base    X11R6-symlinks
with source  $XORG_URL proto/xextproto-$VERSION.tar.bz2
with info    good 20120713
with info    last 20120323
