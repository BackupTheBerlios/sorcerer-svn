   VERSION=( "0.4" )
    IGNORE="x86"
  CATEGORY="archive"
 ATTRIBUTE="console new"
    SOURCE="parchive/par2cmdline-$VERSION.tar.gz"
       URL="$SOURCEFORGE_URL"
  HOMEPAGE="http://www.fpx.de/fp/Software/UUDeview/"
  ESTIMATE="130"
      DESC="par2cmdline is for posting and recovery of multi-part archives on usenet."

case  $VERSION  in
  0.4) REQ="$REQ gcc-v3.4"  ;;
esac

build() {
case  $VERSION  in
  0.4)  GCC3_VER=$( installed_version gcc-v3.4 )
        export  PATH="${PATH/gcc\/current/gcc/$GCC3_VER}"
        export  CCACHE_PATH="/opt/gcc/$GCC3_VER/bin"  ;;
esac
default_build
}

