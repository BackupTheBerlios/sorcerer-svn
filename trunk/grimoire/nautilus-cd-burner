# Version 2.24.0 definitely requires eel and eel is not compiling.
# broke because eel is broke

   version stable 2.24.0 2.22.1 2.22.0
   version unstable 2.25.3
   require cdrtools eel nautilus
  category disk/cd
 attribute gnome
 attribute broke
    source $GNOME_URL sources/nautilus-cd-burner/${VERSION:0:4}/nautilus-cd-burner-$VERSION.tar.bz2
  homepage http://www.gnome.org/projects/nautilus
  estimate 1100
      desc 'program for writing CDs'
