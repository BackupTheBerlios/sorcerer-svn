   VERSION=( "1.0.40" )
  CATEGORY="compiler"
 ATTRIBUTE="broken depreciated gnome"
#ATTRIBUTE="gnome1 x11 broken depreciated"
    SOURCE="sources/gnome-objc/1.0/gnome-objc-$VERSION"
  HOMEPAGE="http://www.gnome.org"
       URL="$GNOME_URL"
       REQ="gnome-libs"
      DESC="Gnome Objective-C Compiler

Gnome Objective-C Compiler
"


build() {
./configure  --prefix=/usr            \
             --localstatedir=/var     \
             --sysconfdir=/etc        &&
make                                  &&
prepare_install                       &&
make    install
}
