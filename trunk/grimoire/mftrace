    stable 1.2.14 1.2.5 1.2.4 1.2.3 1.2.0
   require Python potrace t1utils

  category font/editor
 attribute console
    source mftrace-$VERSION.tar.gz
#       url http://www.xs4all.nl/~hanwen/mftrace/
#  homepage http://www.xs4all.nl/~hanwen/mftrace/
       url http://lilypond.org/download/sources/mftrace/
  homepage http://lilypond.org/mftrace/
# can use either autotrace or potrace
  estimate 50
      desc 'mftrace converts TeX bitmap fonts into PFA or PFB fonts.
mftrace is a small Python program that lets you
trace a TeX bitmap font into a PFA or PFB font
(A PostScript Type1 Scalable Font) or TTF (TrueType) font.'
