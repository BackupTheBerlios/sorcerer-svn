   VERSION=( 1.10.0 )
  CATEGORY='development/interpreter/ruby'
 ATTRIBUTE='library new'
    SOURCE="ruby-gettext-package-$VERSION.tar.gz"
      VURL='http://rubyforge.org/frs/?group_id=855'
       URL='http://rubyforge.org/frs/download.php/22478/'
#      URL='http://rubyforge.org/frs/download.php/5885/'
  HOMEPAGE='http://ponx.s5.xrea.com/hiki/ruby-gettext.html'
       REQ='gettext ruby-gnome2'
  ESTIMATE='5'
      DESC='ruby-gettext-package provides native language support for ruby.'

build()  {
[  -f  setup.rb  ]  &&  prepare_install  &&
ruby   setup.rb
}
