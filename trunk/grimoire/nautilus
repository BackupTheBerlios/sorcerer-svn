   version   stable 3.6.0 3.4.2 3.4.1
#  version unstable 3.6.0
   require exempi gnome-desktop libexif libnotify shared-mime-info tracker
  category utility/file-manager
 attribute gnome library makej multilib-concurrent
    source   stable $GNOME_URL sources/nautilus/${VERSION:0:3}/nautilus-$VERSION.tar.xz
    source unstable $GNOME_URL sources/nautilus/${VERSION:0:3}/nautilus-$VERSION.tar.xz
      info last 20120925
      info home http://www.gnome.org/projects/nautilus
 freshmeat eazelnautilus
      desc 'file manager for the GNOME desktop
It also can be used to draw your desktop.'

build(){
 sed -i 's:hardcode_into_libs=yes:hardcode_into_libs=no:' configure
 default_build
}
