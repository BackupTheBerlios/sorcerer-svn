# Version 6.14.6 is broke if using the unstable version of xorg-server

   version   stable 6.14.6 6.14.4
   version unstable 6.14.6
   require xorg-server
      opts --disable-static
  category xorg/driver
 attribute x11 X11R7 library makej multilib-concurrent optimize
#attribute broke
    source   stable $XORG_URL driver/$SPELL-$VERSION.tar.bz2
    source unstable $XORG_URL driver/$SPELL-$VERSION.tar.bz2
      info good 20120713
      info last 20120329
      info home http://www.x.org

