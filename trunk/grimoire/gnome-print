   version stable 0.37 0.36 0.35
   require freetype gdk-pixbuf-v0.22 ghostscript gnome-libs libxml
  category printer
 attribute gnome
    source $GNOME_URL sources/gnome-print/${VERSION:0:4}/gnome-print-$VERSION.tar.bz2
      info home http://www.gnome.org
      desc 'implementation of the Gnome Printing Architecture
gnome-print is an implementation of the Gnome Printing Architecture,
as described in:  http://www.levien.com/gnome/print-arch.html'

build(){
case $VERSION in
 0.37) patch -p1 < $SCRIPT_DIR/gnome-print-0.37.patch ;;
esac
default_build
}
