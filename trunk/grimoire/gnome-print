   VERSION=( "0.37" "0.36" "0.35" )
  CATEGORY="printer"
 ATTRIBUTE="gnome"
    SOURCE="sources/gnome-print/${VERSION:0:4}/gnome-print-$VERSION.tar.bz2"
  HOMEPAGE="http://www.gnome.org"
       URL="$GNOME_URL"
       REQ="freetype gdk-pixbuf ghostscript gnome-libs libxml"
  ESTIMATE="379"
      DESC="gnome-print is an implementation of the Gnome Printing Architecture
gnome-print is an implementation of the Gnome Printing Architecture,
as described in:  http://www.levien.com/gnome/print-arch.html"

build() {
 case  $VERSION  in
  0.37)  patch  -p1  <  $SCRIPT_DIR/gnome-print-0.37.patch  ;;
 esac

 default_build
}

