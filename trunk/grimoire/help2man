with version stable 1.40.13 1.40.12 1.40.11
with base    perl--gettext
with role    documentation
with trait   console
with source  $GNU_URL help2man/help2man-$VERSION.tar.gz
with info    last 20121204
with info    home http://www.gnu.org/software/help2man/
with info    cite 'generates simple manual pages from program output'

build(){
 strip_destdir(){
  local  LC_ALL
  export LC_ALL=C

  find $DESTDIR -type f     |
  xargs --max-lines=64 file |
  grep ', not stripped'     |
  cut -d : -f1              |
  xargs --max-lines=64 strip --strip-unneeded
 }

 default_build &&
 strip_destdir
}
