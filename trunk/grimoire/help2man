   version stable 1.40.12 1.40.11 1.40.10
   require perl--gettext
  category documentation
 attribute console
    source $GNU_URL help2man/help2man-$VERSION.tar.gz
      info last 20120824
      info home http://www.gnu.org/software/help2man/
      desc 'generates simple manual pages from program output'

build(){
 strip_destdir(){
  local  LC_ALL
  export LC_ALL=C

  find $DESTDIR -type f     |
  xargs --max-lines=64 file |
  grep ', not stripped'     |
  cut -d : -f1              |
  xargs --max-lines=64 strip --strip-unneeded
 }

 default_build &&
 strip_destdir
}
