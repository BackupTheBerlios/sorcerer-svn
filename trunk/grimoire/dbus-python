   VERSION="0.71"
  CATEGORY="disk"
 ATTRIBUTE="console"
    SOURCE="$SPELL/$SPELL-$VERSION.tar.gz"
  HOMEPAGE="http://www.freedesktop.org/wiki/Software_2fDBusBindings"
       URL="http://dbus.freedesktop.org/releases/"
       REQ="dbus-glib"
  ESTIMATE="1148"
      DESC="$SPELL provides Python bindings for dbus"

build() {
  prepare_install	&&
  python setup.py install
}
