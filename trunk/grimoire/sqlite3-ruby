  ESTIMATE='37'
      DESC="sqlite3-ruby provides ruby bindings for sqlite."


build()  {
ruby  setup.rb  config   &&
ruby  setup.rb  setup    &&  prepare_install  &&
ruby  setup.rb  install
}
