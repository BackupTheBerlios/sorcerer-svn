version_year_month 12
  category network
 attribute client server stale
    source http://sorcerer.silverice.org/download/iana/protocols-$VERSION.tar.xz
      info home http://www.iana.org/
      info good 20120630
   protect /etc/protocols
   exclude /etc/protocols.old
      desc 'translates the protocol numbers assigned by IANA to /etc/protocols'

build(){
 [ -s protocols ] || return 1
 mkdir -pvm 755 $DESTDIR/etc
 cat protocols | column -s '	' -t > $DESTDIR/etc/protocols
}
