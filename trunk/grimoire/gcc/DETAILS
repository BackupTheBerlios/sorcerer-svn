   VERSION=( "3.3.2" "3.2.3" "3.2.2" "3.2.1" "3.2" "3.1" )
      SAFE=( "3.3.2" "3.2.3" )
    IGNORE="ada\|core\|g++\|g77\|java\|objc\|testsuite"
  CATEGORY="compiler"
 ATTRIBUTE="concurrent console core library solo"
 SOURCE[0]="gcc-$VERSION"
 SOURCE[1]="protector-3.2.2-10.tar.gz"
    URL[0]="ftp://sources.redhat.com/pub/gcc/releases/gcc-$VERSION"
#   URL[1]="http://www.research.ibm.com/trl/projects/security/ssp/gcc3_2_2"
#   SOURCE="gcc/gcc-$VERSION"
#      URL="$GNU_URL"
       REQ="automake coreutils libtool"
  HOMEPAGE="http://gcc.gnu.org"
# http://www.research.ibm.com/trl/projects/security/ssp/
# website above for stack smashing protection patch.
# with the above patch applied gcc accepts -fstack-protector 
# to prevent stack smashing techniques.
 FRESHMEAT="gcc"
  ESTIMATE="6016"
   FEATURE="$FEATURE libar"
     SHORT="gcc is the GNU compiler collection."
cat << EOF
gcc - GNU Compiler Collection.
The GNU Compiler Collection is a suite of compilers
that compile C, C++, Objective C, Fortran, Java,
and Ada for a wide variety of architectures.
EOF
