   VERSION=( "3.2.1" "3.2" "3.1" )
    IGNORE="ada\|core\|g++\|g77\|java\|objc\|testsuite"
  CATEGORY="compiler"
 ATTRIBUTE="concurrent console core library solo"
    SOURCE="gcc/gcc-$VERSION"
       URL="$GNU_URL"
       OPT="gcc,v3.2:::for casting of sources not ported to gcc $VERSION"
  HOMEPAGE="http://gcc.gnu.org"
     SHORT="gcc is the GNU compiler collection."
cat << EOF
gcc - GNU Compiler Collection.
The GNU Compiler Collection is a suite of compilers
that compile C, C++, Objective C, Fortran, Java,
and Ada for a wide variety of architectures.
