         TEMP_LD_SO_CONF="/tmp/ld.so.conf.$$.$RANDOM"
grep  -v  "gcc"           /etc/ld.so.conf  >  $TEMP_LD_SO_CONF
cp      $TEMP_LD_SO_CONF  /etc/ld.so.conf
rm  -f  $TEMP_LD_SO_CONF

for           GCC_DIR  in  /opt/gcc/*/lib
do    echo  "$GCC_DIR"
done  >>  /etc/ld.so.conf


rm    -f  $CCACHE_CACHE/*
unlock_cast
cast  -c  libtool
true
