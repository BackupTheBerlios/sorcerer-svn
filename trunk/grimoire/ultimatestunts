    stable 0751 0741 0731 0721 0701 0631 0611 0601
   require MesaLib SDL_image freealut

  category game
 attribute x11
    source ultimatestunts/ultimatestunts-srcdata-$VERSION.tar.gz
       url "$SOURCEFORGE_URL"
  homepage http://www.ultimatestunts.nl
  estimate 600
      desc 'ultimatestunts is a stunt car driving simulation.'

build(){
 find data -type d | xargs -r --max-lines=64 chmod 755
 find data -type f | xargs -r --max-lines=64 chmod 644
 default_build
}
