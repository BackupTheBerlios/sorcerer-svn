# Where did 4.3.1 go?
# 4.3.0 prefers to link with xine-lib instead of gst-plugins-base.
# How come kde's ftp is not hosting phonon version 4.3.80?

#   stable 4.3.1 4.3.0
    stable 4.4.0 4.3.80 4.3.0
   inherit kde-v4
   require automoc4 gst-plugins-base xine-lib
    source $KDE_URL stable/phonon/$VERSION/phonon-$VERSION.tgz
#   source stable/phonon/4.3.0/phonon-4.3.0.tar.bz2
  homepage http://phonon.kde.org/
  category utility/file-manager utility/terminal-emulator
  estimate 900
      desc 'audio server/mixer for kde-v4'
