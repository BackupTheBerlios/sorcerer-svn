    stable 0.9.12
   require libXpm
  category game
 attribute x11
    source $SOURCEFORGE_URL loser/loser/loser-$VERSION/loser-$VERSION.tar.gz
  homepage http://loser.sourceforge.net
case $VERSION in
 0.9.12) use_gcc gcc-v3.3 ;;
esac
  estimate 1200
      desc 'loser is a side scrolling game.'

build(){
case $VERSION in
 0.9.12) patch -p1 < $SCRIPT_DIR/loser-0.9.12.patch ;;
esac
default_build
}
