   version stable 1.2 1.1 0.9 0.8
#  version stable 1.3 1.2 1.1 0.9 0.8
  category audio/cd
 attribute console
    source http://linukz.org/download/cd-discid-$VERSION.tar.gz
      info last 20111123
      info home http://linukz.org/cd-discid.shtml
 freshmeat cddiscid
      desc 'returns the disc id for a cd
cd-discid is a backend utility to get CDDB
discid information from a CD-ROM disc.
It was originally designed for cdgrab (now abcde),
but can be used for any purpose requiring CDDB data.'


build() {
sed -i "s:-g -O2:$CFLAGS:" Makefile
make &&
make install "DESTDIR=$DESTDIR"
}
