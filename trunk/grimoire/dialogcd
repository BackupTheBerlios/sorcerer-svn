   VERSION=( "0.1.8" "0.1.7-noarch" "0.1.6" "0.1.5" )
  CATEGORY="cd"
 ATTRIBUTE="console"
    SOURCE="dialogcd/dialogcd-$VERSION.tgz"
       URL="$SOURCEFORGE_URL"
  HOMEPAGE="http://dialogcd.sourceforge.net"
       REQ="bash dialog cdrtools"
  ESTIMATE="5"
      DESC="dialogcd is a a bash/dialog front end to cdrtools.

dialogcd is a dialog based frontend to cdrtools.
"


build() {
[  -f  dialogcd  ]  &&
prepare_install     &&
set      -x         &&
install  -m 755  dialogcd  /usr/bin/
}
