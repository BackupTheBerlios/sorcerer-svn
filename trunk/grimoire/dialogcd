    stable 0.1.8 0.1.7-noarch 0.1.6 0.1.5
   require bash dialog cdrtools
  category disk/cd
 attribute console
    source dialogcd/dialogcd-$VERSION.tgz
       url "$SOURCEFORGE_URL"
  homepage http://dialogcd.sourceforge.net
  estimate 5
      desc 'dialogcd is a a bash/dialog front end to cdrtools.'

build(){
[ -f dialogcd ] &&
prepare_install &&
install -vm 755 dialogcd /usr/bin/
}
