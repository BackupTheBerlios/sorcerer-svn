   disable scavenge
   version stable 20091017
  category game
 attribute archive_off data
    source $SOURCEFORGE_URL warzone2100/warzone2100/Videos/standard-quality-en/sequences.wz
      info good 20120713
      info last 20100405
      info home http://wz2100.net/
      desc '572M of standard quality video for warzone2100'

build(){
mkdir -pvm 755 $DESTDIR/usr/share/warzone2100 &&
cp -av $SOURCE $DESTDIR/usr/share/warzone2100/
}
