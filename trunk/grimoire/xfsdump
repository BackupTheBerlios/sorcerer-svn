 ATTRIBUTE="console"
#   SOURCE="xfsdump-$VERSION.src.tar.gz"
    SOURCE="xfsdump_$VERSION.tar.gz"
DICTIONARY="xfsdump-2.2.33.src.tar"
       URL="ftp://oss.sgi.com/projects/xfs/cmd_tars/"
  HOMEPAGE="http://oss.sgi.com/projects/xfs/"
       REQ="attr dmapi ncurses"
  ESTIMATE="385"
      DESC="xfsdump provide dump and restore utilities for XFS filesystems.
xfsdump provide dump and restore utilities for XFS,
including extended attribute and DMAPI support."

build() {
build_old()  {
  ./configure  --prefix=/usr    \
               $OPTS            &&
  make  "DEBUG="                \
        "LDFLAGS=$LDFLAGS"      &&
  prepare_install               &&
  make    install  install-dev
}

  export  OPTIMIZER=-O
  export  DEBUG=-DNDEBUG

  make  configure  &&
  make             &&
  prepare_install  &&
  make    install  install-dev
# make    install  install-lib  install-dev
}
