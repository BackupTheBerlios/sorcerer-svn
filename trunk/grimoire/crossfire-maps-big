inherit crossfire
unset  REQ
 ATTRIBUTE="data x11"
    IGNORE="\arch\|maps\|client"
    SOURCE="crossfire/crossfire-${VERSION}.maps-big.tar.bz2"
  EPROVIDE="crossfire,maps crossfire,maps-big"
     SHORT="crossfire-maps-big contains data files for crossfire."


build()  {  data_install  /usr/share/crossfire/maps;  }
