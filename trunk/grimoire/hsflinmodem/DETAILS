   VERSION=( "5.03.27lnxtbeta03042700" "5.03.27lnxtbeta03041600" )
  CATEGORY="modem kernel"
 ATTRIBUTE="broken depreciated x86"
  HOMEPAGE="http://www.linuxant.com/drivers/hsf/index.html"
       URL="http://www.linuxant.com/drivers/hsf/archive/hsflinmodem-$VERSION"
    SOURCE="hsflinmodem-$VERSION"
 FRESHMEAT="hsflinmodem"
       REQ="pciutils"
     SHORT="hsflinmodem is no longer used, use hsfmodem instead."
cat << EOF
hsflinmodem is no longer used.
Use hsfmodem instead.

provides Linux drivers for the Conexant 
(formerly Rockwell) Softmodem HSF modem family.
---
Make sure to run "hsfconfig" to complete the 
installation and configure your modem.
Also run it every time you update/recompile your kernel.
EOF
