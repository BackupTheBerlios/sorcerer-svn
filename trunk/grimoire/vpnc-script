with version stable 20121210
with role    development
with trait   data
with source  "http://git.infradead.org/users/dwmw2/vpnc-scripts.git/blob_plain/HEAD:/vpnc-script"
with info    last 20121210
with info    home "http://git.infradead.org/users/dwmw2/vpnc-scripts.git/blob_plain/HEAD:/vpnc-script"
with info    cite 'script required by openconnect'

build(){
 mkdir   -pvm 755         $DESTDIR/etc/vpnc
 install  -vm 755 $SOURCE $DESTDIR/etc/vpnc/vpnc-script
}
