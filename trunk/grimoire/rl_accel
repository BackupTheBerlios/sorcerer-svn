   VERSION=( "0.52" )
    IGNORE="daily"
  CATEGORY="development"
 ATTRIBUTE="library new"
    SOURCE="rl_accel-$VERSION-daily-unix.tgz"
       URL="http://www.reportlab.org/daily/"
  HOMEPAGE="http://www.reportlab.org/rl_toolkit.html"
       REQ=""
  ESTIMATE="5"
      DESC="rl_accel is a C accelerator extension for ReportLab
available by subversion at URL
http://www.reportlab.co.uk/svn/public/reportlab/trunk/rl_addons/rl_accel"

build(){
cd      rl_accel  &&
prepare_install   &&
python  setup.py install
}
