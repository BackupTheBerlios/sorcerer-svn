   VERSION=( "1.0.5" )
  CATEGORY="development"
 ATTRIBUTE="gnome library x11"
    SOURCE="sources/gnome-vfs/1.0/gnome-vfs-$VERSION"
  HOMEPAGE="http://www.gnome.org"
       URL="$GNOME_URL"
       REQ="gnome-libs oaf GConf.1 gnome-mime-data"
    OPT[0]="openssl:::for secure socket layer support"
    OPT[1]="cdparanoia:::for browsing CDs"
     SHORT="gnome-vfs provides the Gnome Virtual File System."
cat << EOF
gnome-vfs
This is the GNOME Virtual File System.
GNOME VFS is currently used as one of the 
foundations of the Nautilus file manager.
EOF
