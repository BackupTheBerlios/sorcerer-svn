   VERSION=( "0.7.0" "0.6.5" "0.6.4" "0.6.3" "0.6.2" )
  CATEGORY="file-share"
 ATTRIBUTE="kde"
    SOURCE="qtella/qtella-$VERSION.tar.gz"
       URL="$SOURCEFORGE_URL"
  HOMEPAGE="http://www.qtella.net"
       REQ=( "automake id3lib libtool qt-x11-free"
             "kdelibs::--with-kde=no:for kde integration" )
  ESTIMATE="317"
      DESC="qtella is a gnutealla client using qt libraries.

qtella is a Gnutella client for Linux.
It is written in C++ using the Qt libraries.
KDE is supported but not necessary."


build() {
aclocal
automake    -afc
autoconf
libtoolize  --force
default_build
}
