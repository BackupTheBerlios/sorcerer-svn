      info home http://gnu.ethz.ch/jumpbump.mine.nu/
      desc 'violent multiplayer game with cute bunnies

It is both fun and addictive for the whole family.
It features AI, multi-player local and network games.'

build(){
 install_levels(){
            USJ=$DESTDIR/usr/share/jumpnbump
  mkdir -p $USJ
  CX=1
  while [[ -n ${SOURCE[$CX]} ]]; do
   DEST=${SOURCE[$CX]##*/}
   DEST=${DEST%.bz2}
   decompress ${SOURCE[$CX]} > $USJ/$DEST
   (( CX++ ))
  done
 }

 default_build &&
 install_levels
}
