unset  VERSION  SOURCE  URL
   VERSION=( "1.41" )
 ATTRIBUTE="concurrent data"
        CX=0
for  LINE  in  $(<  $SCRIPT_DIR/levels  );  do
  SOURCE[$CX]="$LINE.dat.bz2"
     URL[$CX]="http://www.jumpbump.mine.nu/levels.bz2"
      ((  CX++  ))
done
     SHORT="jumpnbump,levels provides additional levels for the game."
stream << EOF
jumpnbump,levels provides additional levels for the game.
EOF
