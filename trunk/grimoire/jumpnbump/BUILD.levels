USJ=/usr/share/jumpnbump
CX=0

prepare_install  &&
mkdir  -p  $USJ  &&

while  [   -n           "${SOURCE[$CX]}"  ];  do
  echo      "Installing  ${SOURCE[$CX]/.bz2/}  in  $USJ"
  decompress                                          \
    $(  guess_filename  "${SOURCE[$CX]}"              \
                        "${SOURCE_CACHE}/$BSPELL"  )  \
                >  "$USJ/${SOURCE[$CX]/.bz2/}"
  touch            "$USJ/${SOURCE[$CX]/.bz2/}"
  ((  CX++  ))
done
