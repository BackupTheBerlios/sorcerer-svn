# does not install library but does install pkgconfig file.

with info    good   20130618
with info    last   20120614
with version stable 20120614
with base    pkgconfig
with role    development
with trait   data gnome library multilib-concurrent
with source    stable $GNOME_URL sources/mobile-broadband-provider-info/$VERSION/mobile-broadband-provider-info-$VERSION.tar.xz
with source  unstable $GNOME_URL sources/mobile-broadband-provider-info/$VERSION/mobile-broadband-provider-info-$VERSION.tar.xz
with info    home http://live.gnome.org/NetworkManager/MobileBroadband/ServiceProviders
with info    cite 'mobile broadband settings for different service providers in different countries'
