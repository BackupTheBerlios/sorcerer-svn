    stable 0.8.10 0.8.8 0.8.6 0.8.5 0.8.3 0.8.0
   require libgnomeprint libgnomeui libgsf pcre
  category development
 attribute library gnome
    source $GNOME_URL sources/goffice/${VERSION:0:3}/goffice-$VERSION.tar.bz2
  homepage http://www.gnumeric.org
  estimate 1300
      desc 'library for common document centric functions'

