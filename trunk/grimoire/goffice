    stable 0.8.10 0.8.8
   require libgnomeprint libgnomeui libgsf pcre
  category development
 attribute library gnome
    source $GNOME_URL sources/goffice/${VERSION:0:3}/goffice-$VERSION.tar.bz2
      home http://www.gnumeric.org/
      docs http://projects.gnome.org/gnumeric/doc/gnumeric.shtml
#     mail http://mail.gnome.org/mailman/listinfo/gnumeric-list
#      irc http://projects.gnome.org/gnumeric/contact-info.shtml
  estimate 1300
      desc 'library for common document centric functions'

build(){
 export   CFLAGS="${CFLAGS/-ffast-math/}"
 export CXXFLAGS="${CXXFLAGS/-ffast-math/}"
 default_build
}
