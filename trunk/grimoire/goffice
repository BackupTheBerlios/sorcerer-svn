    stable 0.6.5 0.6.4 0.6.3 0.6.2 0.6.1
  unstable 0.7.0
   require libgnomeprint libgnomeui libgsf pcre
  category development
 attribute library gnome
       url $GNOME_URL
    source sources/goffice/${VERSION:0:3}/goffice-$VERSION.tar.bz2
  homepage http://www.gnumeric.org
  estimate 2300
      desc 'goffice provides a library for common document centric functions.'

build(){
export   CFLAGS="${CFLAGS/-ffast-math/}"
export CXXFLAGS="${CXXFLAGS/-ffast-math/}"
default_build
}
