# Disable introspection since introspection is broke with clutter

    stable 0.9.6
case $VERSION in
 0.9.6)
      opts --enable-introspection=no ;;
esac
   require cluttermm clutter-gtk
  category audio/cd
 attribute gnome
    source $GNOME_URL sources/clutter-gtkmm/${VERSION:0:3}/clutter-gtkmm-$VERSION.tar.bz2
  homepage http://www.gnome.org
  estimate 570
      desc 'c++ bindings for clutter'
