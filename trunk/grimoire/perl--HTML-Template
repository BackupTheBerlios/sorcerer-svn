   VERSION=( "2.8" "2.7" )
  CATEGORY="perl"
 ATTRIBUTE="perl-module new"
  CATEGORY="$CATEGORY web"
       URL="http://www.cpan.org/authors/id/"
  HOMEPAGE="http://search.cpan.org/search?dist=HTML-Template.tar.gz"
       REQ="perl"
    SOURCE="S/SA/SAMTREGAR/${SPELL#perl--}-$VERSION.tar.gz"
  ESTIMATE="10"
  EPROVIDE="HTML-Template"
      DESC="$SPELL is perl module for using HTML templates from CGI scripts.
Allows for a clean separation of Perl code and HTML markup.
"
