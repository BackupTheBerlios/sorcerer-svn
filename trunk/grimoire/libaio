    stable 0.3.107-2
  category library
 attribute library new
    source libaio_${VERSION%-*}.orig.tar.gz
    source libaio_${VERSION}.diff.gz
       url http://ftp.us.debian.org/debian/pool/main/liba/libaio/
  homepage http://packages.debian.org/unstable/libdevel/libaio-dev/
  estimate 50
      desc 'libaio library enables userspace use of kernel asynchronous I/O system calls'

build(){
zcat $SOURCE_CACHE/$SPELL/$VERSION/${SOURCE[1]} | patch -p1 &&
make &&
prepare_install &&
make prefix=/usr install
}
