    stable 0.3.107-2
  CATEGORY='library'
 ATTRIBUTE='library new'
 SOURCE[0]="libaio_${VERSION%-*}.orig.tar.gz"
 SOURCE[1]="libaio_${VERSION}.diff.gz"
       URL='http://ftp.us.debian.org/debian/pool/main/liba/libaio/'
  HOMEPAGE='http://packages.debian.org/unstable/libdevel/libaio-dev'
  ESTIMATE='59'
      DESC='libaio library enables userspace use of kernel asynchronous
I/O system calls'

build() {
 zcat $SOURCE_CACHE/$SPELL/$VERSION/${SOURCE[1]} | patch -p1	&&
 make             &&
 prepare_install  &&
 make prefix=/usr install
}
