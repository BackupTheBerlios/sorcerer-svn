    IGNORE="pre"
  CATEGORY="db"
 ATTRIBUTE="gui kde"
    SOURCE="tellico-$VERSION.tar.gz"
       URL="http://www.periapsis.org/tellico/download/"
  HOMEPAGE="http://www.periapsis.org/tellico/"
       REQ=( "kdelibs"
             "kdemultimedia:::for CDDA support"
             "kdepim:::for calendar support (kcal)"
             "taglib:::for reading meta data from audio files" )
  ESTIMATE="1000"
      OPTS="--disable-debug --enable-final"
      DESC="tellico is a collection manager for KDE.
It includes default collections for books, bibliographies,
comic books, videos, music, video games, coins, stamps,
trading cards, and wines, and also allows custom collections."
