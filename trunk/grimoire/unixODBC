# no qt-x11-free optional gui for the moment since it creates
# a recursive requriement loop
# optional qt-x11-free --enable-gui --disable-gui for various GUIs

    stable 2.2.12 2.2.11 2.2.10 2.2.9 2.2.8

  category db
 attribute console x11
    source unixODBC-$VERSION.tar.gz
  homepage http://www.unixodbc.org/
       url http://www.unixodbc.org/
      opts --disable-gui
  estimate 5920
 freshmeat unixodbc
      desc 'unixODBC is a free database access system compatible with windows.
The unixODBC project provides Unix applications with the 
same ODBC 3.51 API and facilities available under Windows.'

build(){
case $VERSION in
 2.2.11) export ac_cv_have_x=have_x=yes ;;
esac
default_build
}
