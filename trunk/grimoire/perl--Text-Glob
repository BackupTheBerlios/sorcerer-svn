     PNAME=${SPELL#perl--}
   VERSION=( "0.07" )
  CATEGORY="perl"
 ATTRIBUTE="perl-module new"
    SOURCE="R/RC/RCLAMP/$PNAME-$VERSION.tar.gz"
       URL="http://www.cpan.org/authors/id/"
  HOMEPAGE="http://search.cpan.org/search?dist=$PNAME"
       REQ=( "perl" )
  ESTIMATE="15"
      DESC="$SPELL module matches globbing patterns against text"

