     PNAME=${SPELL#perl--}
   VERSION=( "0.2805" )
  CATEGORY="perl"
 ATTRIBUTE="perl-module new"
    SOURCE="K/KW/KWILLIAMS/$PNAME-$VERSION.tar.gz"
       URL="http://www.cpan.org/authors/id/"
  HOMEPAGE="http://search.cpan.org/search?dist=$PNAME"
       REQ="perl perl--version perl--Module-Signature perl--ExtUtils-ParseXS perl--YAML perl--Archive-Tar perl--Pod-Readme"
  ESTIMATE="132"
      DESC="$SPELL module builds and installs perl modules"

