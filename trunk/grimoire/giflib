# Sun Nov 11 01:39:14 UTC 2007
# libungif and giflib dissapeared from sourceforge, 404
## stable 4.1.5 4.1.4 4.1.3
# stable         4.1.4 4.1.3

   version stable 4.1.6
  optional libSM  '' --without-x for gif conversion programs
  optional libX11 '' --without-x for gif conversion programs
      opts --enable-static=no
  category graphic
 attribute library makej multilib-concurrent x11
    source http://ftp.us.debian.org/debian/pool/main/g/giflib/giflib_$VERSION.orig.tar.gz
      vurl http://ftp.us.debian.org/debian/pool/main/g/giflib/
      info last 20080117
      info home http://libungif.sf.net/
      desc 'library for LZW compressed GIF images'

case $VERSION in
 4.1.6)
   require automake libtool ;;
esac

build(){
 case $VERSION in
  4.1.6) aclocal; automake -acf; libtoolize -cf ;;
 esac
 default_build
}
