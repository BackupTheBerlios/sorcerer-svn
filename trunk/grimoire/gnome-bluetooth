# no versions known to compile at the moment.

   version   stable 3.2.2 3.2.1 3.2.0
   version unstable 3.3.4
   require gnome-control-center libnotify
  category documentation
 attribute gnome library makej multilib-concurrent
#attribute broke
    source $GNOME_URL sources/gnome-bluetooth/${VERSION:0:3}/gnome-bluetooth-$VERSION.tar.xz
      info last 20120207
      info home http://www.gnome.org/softwaremap/projects/gnome-bluetooth
      desc 'GNOME Bluetooth Subsystem'

build(){
 sed -i 's:hardcode_into_libs=yes:hardcode_into_libs=no:' configure
 default_build
}
