# /usr/bin/bcc required to compile VirtualBox

    stable 0.16.18 0.16.17
#     last 20101023
  category development/assembler
 attribute console
    source http://www.debath.co.uk/dev86/Dev86src-$VERSION.tar.gz
      home http://www.debath.co.uk/
  eprovide bin86
  estimate 3000
      desc 'Bruce Evans C Compiler'

build(){ make install "PREFIX=/usr" "DIST=$DESTDIR"; }
