# multilib-fail while systemd is multilib-fail

with version stable 0.6.21 0.6.14 0.6.8
with base    glib polkit
case $VERSION in
 0.6.14)
with base    automake libtool ;;
 0.6.8)
with base    automake libtool ;;
esac
with also    --disable-static
with role    security/authentication
with trait   gnome hidden library makej multilib-dual
with source  http://cgit.freedesktop.org/accountsservice/snapshot/accountsservice-$VERSION.tar.gz
with info    last 20120517
with info    home unknown
with info    cite 'set of D-Bus interfaces for querying and manipulating user account information'

build(){
if   [ -f autogen.sh ]
then sh ./autogen.sh
fi
default_build
}
