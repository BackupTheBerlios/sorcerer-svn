   version stable 0.6.8
   require polkit
case $VERSION in
 0.6.8)
   require automake libtool ;;
esac
  category security/authentication
 attribute gnome library makej multilib-dual
    source http://cgit.freedesktop.org/accountsservice/snapshot/accountsservice-$VERSION.tar.bz2
      info home  unknown
      desc 'set of D-Bus interfaces for querying and manipulating user account information'

build(){
if   [ -f autogen.sh ]
then sh ./autogen.sh
fi
default_build
}
