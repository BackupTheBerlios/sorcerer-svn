   version stable 0.14.1
   require telepathy-qt
      opts -DCMAKE_INSTALL_PREFIX:PATH=/usr
      opts -DCMAKE_INSTALL_RPATH:PATH=/usr/lib64:/usr/lib
      opts -DCMAKE_SKIP_BUILD_RPATH:BOOL=TRUE
      opts -DCMAKE_BUILD_WITH_INSTALL_RPATH:BOOL=TRUE
      opts -DCMAKE_INSTALL_RPATH_USE_LINK_PATH:BOOL=TRUE
      opts -DCMAKE_BUILD_TYPE=Release
  category network/chat
 attribute library
    source $SOURCEFORGE_URL tapioca-voip/tapioca-qt/$VERSION/tapioca-qt-$VERSION.tar.gz
      info good 20120710
      info last 20070523
      info home http://tapioca-voip.sourceforge.net
      desc 'tapioca-qt provides a library for ...'

build(){
 export PATH="/usr/bin/qt4:$PATH"
 default_build
}
