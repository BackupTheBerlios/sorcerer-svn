    stable 3.7 3.6 3.5 3.4 3.3
   require libX11 libXinerama
  category window-manager
 attribute console
    source dmenu-$VERSION.tar.gz
       url http://suckless.org/download/
  homepage http://www.suckless.org/wiki/tools/xlib
  estimate 10
      desc 'dmenu is a dynamic menu generator used by wmii.'

build(){
 make            PREFIX=/usr &&
 prepare_install             &&
 make    install PREFIX=/usr
}
