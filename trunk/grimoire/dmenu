    stable 4.2.1 4.0 3.9
#     last 20101120
   require libX11 libXinerama
  category window-manager
 attribute console
    source http://dl.suckless.org/tools/dmenu-$VERSION.tar.gz
      home http://www.suckless.org/
      vurl http://suckless.org/
      desc 'dynamic menu generator used by wmii'

build(){
make         PREFIX=/usr &&
make install PREFIX=$DESTDIR/usr
}
