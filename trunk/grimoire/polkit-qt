   version stable 1-0.103.0 0.9.4
   require automoc4 cmake glib polkit qt-everywhere-opensource
      opts -DCMAKE_INSTALL_PREFIX:PATH=/usr
      opts -DCMAKE_INSTALL_RPATH:PATH=/usr/lib64:/usr/lib
      opts -DCMAKE_SKIP_BUILD_RPATH:BOOL=TRUE
      opts -DCMAKE_BUILD_WITH_INSTALL_RPATH:BOOL=TRUE
      opts -DCMAKE_INSTALL_RPATH_USE_LINK_PATH:BOOL=TRUE
      opts -DCMAKE_BUILD_TYPE=Release
  category security
 attribute kde4
    source $KDE_URL stable/apps/KDE4.x/admin/polkit-qt-$VERSION.tar.bz2
      desc 'Authentication Agent for PolicyKit that integrates with qt'
