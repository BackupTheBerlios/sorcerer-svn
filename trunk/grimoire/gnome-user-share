with version   stable 3.8.0 3.0.4 3.0.3
with version unstable 3.8.0
with base    GConf gnome-bluetooth libcanberra libunique nautilus
with role    network/file-share
with trait   gnome library makej multilib-concurrent
with source    stable $GNOME_URL sources/gnome-user-share/${VERSION:0:3}/gnome-user-share-$VERSION.tar.xz
with source  unstable $GNOME_URL sources/gnome-user-share/${VERSION:0:3}/gnome-user-share-$VERSION.tar.xz
with info    last 20130326
with info    home http://www.gnome.org/softwaremap/projects/gnome-user-share
with info    cite 'interface to various user-level file sharing programs'

build(){
 sed -i 's:hardcode_into_libs=yes:hardcode_into_libs=no:' configure
 default_build
}
