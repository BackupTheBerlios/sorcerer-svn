# requires ancient version of ClanLib

   version stable 2.0.2 2.0.0 1.1.7 1.1.6 1.1.5 1.1.3
case $VERSION in
 1.1.5) use_gcc gcc-v4.1 ;;
esac
   require ClanLib
  category game
 attribute new x11
 attribute broke
    source $SOURCEFORGE_URL trophy/trophy/$VERSION/trophy-$VERSION.tar.gz
      info last 20120216
      info home http://trophy.sourceforge.net
      desc 'top down racing game'
