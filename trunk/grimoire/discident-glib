# http://discident.com/
# What is it?

# disabling introspection since it's introspection file format is too old.

   version stable 0.9.9
case $VERSION in
 0.9.9)
      opts --enable-introspection=no ;;
esac
   require json-glib
  category development
 attribute gnome new
    source $GNOME_URL sources/discident-glib/${VERSION:0:3}/discident-glib-$VERSION.tar.bz2
  homepage http://www.gnome.org
      desc 'convenience library for the DiscIdent Web Service'
