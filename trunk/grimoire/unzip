   VERSION=( "552" "551" "550" "542" )
      SAFE=( "552" )
  CATEGORY="archive"
 ATTRIBUTE="core console solo"
    SOURCE="unzip${VERSION}.tar.gz"
  HOMEPAGE="http://www.ctan.org/tex-archive/tools/zip/info-zip/UnZip.html"
       URL="http://ftp.info-zip.org/pub/infozip/src
             ftp://ftp.info-zip.org/pub/infozip/src
            http://sunsite.cnlab-switch.ch/ftp/mirror/infozip/src
             ftp://sunsite.cnlab-switch.ch/mirror/infozip/src
            http://www.mirror.ac.uk/sites/ftp.info-zip.org/pub/infozip/src
             ftp://sunsite.icm.edu.pl/pub/unix/archiving/info-zip/src"
  ESTIMATE="72"
      DESC="unzip is a PKWARE compatible decompressor.
Info-ZIP's purpose is to provide free, portable, high-quality versions
of the Zip and UnZip compressor-archiver utilities that are compatible
with the DOS-based PKZIP by PKWARE, Inc."


build() {
sed    -i  "s/-O /$CFLAGS /"  \
           unix/Makefile
make   -f  unix/Makefile  generic  &&
prepare_install                    &&
make   -f  unix/Makefile  install  prefix=/usr
}
