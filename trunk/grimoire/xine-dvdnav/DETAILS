  VERSION="0.9.10"
# Version  0.9.12 compiles, but causes xine 0.9.12 to crash when
# attempting to play DVDs
 CATEGORY="video"
ATTRIBUTE="library x11"
   SOURCE="dvd/xine-dvdnav-$VERSION"
      URL="$SOURCEFORGE_URL"
 HOMEPAGE="http://dvd.sourceforge.net"
      REQ="xine-ui libdvdnav"
    SHORT="xine-dvdnav is a plugin for xine to give DVD support."
cat << EOF
plugin for xine to give DVD support
EOF
