    stable 1.1.3 1.1.2 1.1 1.0 1.0rc3
  category audio/codec audio/library
 attribute library
    source libogg-$VERSION.tar.gz
  homepage http://www.xiph.org/ogg/
       url http://downloads.xiph.org/releases/ogg/
      opts --enable-static=no
  estimate 62
      desc 'libogg is the bitstream and framing library for the Ogg project
It provides functions which are necessary to codec libraries like libvorbis.'

build(){
 sed -i 's:-O20 -ffast-math::' configure{,.in}
 default_build
}
