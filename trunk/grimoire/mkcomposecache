   VERSION=( 1.2 1.1 1.0 )
    SOURCE="app/mkcomposecache-$VERSION.tar.bz2"
inherit xorg
       REQ='libX11'
  ESTIMATE='80'
      DESC="mkcomposecache is used for creating global (system-wide) compose cache files.

Compose cache files help with application startup times and memory usage,
especially in locales with large compose tables (e.g. all UTF-8 locales)."
