   VERSION=( 1.5.3.6 1.5.3.5 1.5.3.4 1.5.3.3 1.5.3.2 1.5.2.2 )
    IGNORE='rc'
  CATEGORY='development documentation'
 ATTRIBUTE='console'
    SOURCE="/pub/software/scm/git/git-htmldocs-$VERSION.tar.bz2"
       URL="$KERNEL_URL"
  HOMEPAGE='http://git.or.cz/'
  ESTIMATE='352'
      DESC='git-htmldocs provides HTML documentaion for git'

build() {
  DOC_DIR=/usr/doc/git/html
  prepare_install		&&
  install -v -d  $DOC_DIR	&&
  cp -v -r *     $DOC_DIR
}
