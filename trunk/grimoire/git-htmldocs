inherit git
  CATEGORY='development documentation'
 ATTRIBUTE='console'
    SOURCE="/pub/software/scm/git/git-htmldocs-$VERSION.tar.bz2"
      DESC='git-htmldocs provides HTML documentaion for git'

build(){
 DOC_DIR=/usr/doc/git/html
 prepare_install &&
 install -vd $DOC_DIR &&
 cp -vr * $DOC_DIR
}
