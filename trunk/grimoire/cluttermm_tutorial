# Disable introspection since introspection is broke with clutter

    stable 1.3.1
case $VERSION in
 1.3.1)
      opts --enable-introspection=no ;;
esac
   require clutter-gtkmm
  category audio/cd
 attribute gnome
    source $GNOME_URL sources/cluttermm_tutorial/${VERSION:0:3}/cluttermm_tutorial-$VERSION.tar.bz2
  homepage http://www.gnome.org
  estimate 120
      desc 'tutorial for clutermm'
