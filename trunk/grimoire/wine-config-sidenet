   VERSION=( "1.9.1" "1.9.0" )
    IGNORE="test"
  CATEGORY="development/emulator"
 ATTRIBUTE="x11 new"
    SOURCE="wine-config-sidenet-$VERSION.tgz"
       URL="http://sidenet.ddo.jp/winetips/files/"
  HOMEPAGE="http://sidenet.ddo.jp/winetips/config.html"
       REQ="wget wine"
  ESTIMATE="5"
      DESC="wine-config-sidenet helps set up a Wine environment.
It includes some base tools like the file explorer,
and sets up menu entries for KDE and GNOME.
It can automatically install Internet Explorer and Media Player."

build()  {
  [  -f  setup  ]  &&
  prepare_install  &&
  mkdir  -p      /opt/wine-config-sidenet  &&
  cp     -av  *  /opt/wine-config-sidenet
}
