   version stable 1.2.30
   require httpd
  category network/web
 attribute console server new
      opts --with-apxs=/usr/sbin/apxs
    source $APACHE_URL tomcat/tomcat-connectors/jk/source/jk-$VERSION/tomcat-connectors-$VERSION-src.tar.gz
      info home http://tomcat.apache.org/connectors-doc
      desc 'apache module for connecting to tomcat'

build(){
 cd native/ &&
 default_build
}
