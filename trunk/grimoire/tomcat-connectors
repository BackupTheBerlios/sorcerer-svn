    stable 1.2.30
   require httpd
  category network/web
 attribute console server new
  optional jdk '--with-java-home=/opt/jdk --enable-jni' '' to enable JNI support 
      opts --with-apxs=/usr/sbin/apxs
       url $APACHE_URL
    source tomcat/tomcat-connectors/jk/source/jk-$VERSION/tomcat-connectors-$VERSION-src.tar.gz
  homepage http://tomcat.apache.org/connectors-doc
  estimate 274
      desc 'tomcat-connectors is an apache module for connecting to tomcat'

build(){
 cd native/ &&
 default_build
}
