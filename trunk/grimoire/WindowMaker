   version stable 0.95.3
case $VERSION in
 0.95.3)
      opts --disable-gif ;;
 0.92.0)
   use_gcc gcc-v3.4 ;;
esac
   require giflib imake libXft libXinerama libXpm libpng tiff xorg-cf-files
  optional libXinerama --enable-xinerama '' for xinerama support
      opts --with-appspath=/usr/share/GNUstep --enable-static=no
  category window-manager
 attribute x11
    source http://windowmaker.info/pub/source/release/WindowMaker-$VERSION.tar.gz
      info good 20120716
      info last 20120516
      info home http://www.windowmaker.info/
      info docs http://www.windowmaker.info/documentation.php
     wmbin /usr/bin/wmaker
 freshmeat windowmaker
      desc 'X11 windows manager, similar to NeXTSTEP
WindowMaker is an X11 window manager
designed to give additional integration
support to the GNUstep Desktop Environment.
In every way possible, it reproduces the
elegant look and feel of the NeXTSTEP[tm] GUI.
It is fast, feature rich, easy to configure,
and easy to use.
In addition, Window Maker works with
GNOME and KDE, making it one of the most
useful and universal window managers available.'
