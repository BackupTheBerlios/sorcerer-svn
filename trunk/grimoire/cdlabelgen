    stable 4.0.0 3.6.0 3.5.0 3.1.0 3.0.0 1.5.0
   require perl
    ignore '213'
 category audio/cd video/cd
attribute console
   source cdlabelgen-$VERSION.tgz
      url http://www.aczoom.com/pub/tools/
 homepage http://www.aczoom.com/tools/cdinsert/
 estimate 20
freshmeat cdinsert
     desc 'cdlabelgen generates frontcards and traycards for CDs.'

build(){
prepare_install &&
make    install BASE_DIR=/usr
}
