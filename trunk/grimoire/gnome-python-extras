# Switching to 2.19.1 to have bindings pygda for libgda 3.0.2
  stable 2.19.1 2.14.3 2.14.2 2.14.1 2.14.0
    IGNORE='^2\.19\.'
  CATEGORY='development/binding'
 ATTRIBUTE='gnome'
    SOURCE="sources/gnome-python-extras/${VERSION%.*}/gnome-python-extras-$VERSION.tar.bz2"
  HOMEPAGE='http://www.gnome.org'
       URL="$GNOME_URL"
# libgda required for creating pygda which is required for glom
       REQ=( 'gdl gnome-python libgda'
             'gtkspell:::for pygtkspell' )
  ESTIMATE='1000'
      DESC='gnome-python-extras provides additional python bindings for GNOME.
gnome-python-extras is a set of bindings for
the GNOME libraries for use with python.'
