   VERSION=( "1.2.4" "1.2.3" "1.2" )
  CATEGORY="audio/codec audio/library"
 ATTRIBUTE="library"
    SOURCE="libmpcdec-$VERSION.tar.bz2"
       REQ="automake libtool"
       URL="http://files.musepack.net/source/"
      VURL="http://www.musepack.net/index.php?pg=src"
  HOMEPAGE="http://www.musepack.net/"
  ESTIMATE="280"
      DESC="libmpcdec provides the musepack decoder library"

build()  {
  [  -f  autogen.sh  ]  &&
  sh    ./autogen.sh
  default_build
}
