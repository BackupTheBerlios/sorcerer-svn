   version stable 1.2.6 1.2.5 1.2.4 1.2.3 1.2
   require automake libtool
      opts --enable-static=no
  category audio/codec audio/library
 attribute library makej multilib-dual
    source http://files.musepack.net/source/libmpcdec-$VERSION.tar.bz2
      vurl http://www.musepack.net/index.php?pg=src
      info home http://www.musepack.net/
      desc 'musepack decoder library'

build(){
 [ -f autogen.sh ] &&
 sh ./autogen.sh
 default_build
}
