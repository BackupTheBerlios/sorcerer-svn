version_year_month 
   require GeoIP
  category data
 attribute data archive_off
 attribute new
       url http://geolite.maxmind.com/download/geoip/database/
    source GeoLiteCity.dat.gz
  homepage http://www.maxmind.com/app/geolitecity
  estimate 5
      desc 'GeoLiteCity IP database used by GeoIP'

build() {
 zcat ${SOURCE} > GeoIPCity.dat	&&
 prepare_install    		&&
 install -m 0644 -v GeoIPCity.dat /usr/share/GeoIP/
}

