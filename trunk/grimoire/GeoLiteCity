version_year_month 
   require GeoIP
  category data
 attribute data archive_off
    source http://geolite.maxmind.com/download/geoip/database/GeoLiteCity.dat.gz
      info good 20120528
      info home http://www.maxmind.com/app/geolitecity
      desc 'IP database used by GeoIP'

build() {
 zcat ${SOURCE} > GeoIPCity.dat	&&
 mkdir   -p                       $DESTDIR/usr/share/GeoIP/ &&
 install -m 0644 -v GeoIPCity.dat $DESTDIR/usr/share/GeoIP/
}

