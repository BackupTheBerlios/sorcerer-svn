      opts --enable-static=no
  category graphic/library graphic/toolkit
 attribute gnome library makej multilib-concurrent
    source $GNOME_URL sources/libwnck/${VERSION:0:3}/libwnck-$VERSION.tar.xz
      info last 20120131
