# version 3.2.1 and 3.3.5 fail to compile

   version   stable 3.2.1 3.2.0 3.0.0
   version unstable 3.3.5
   require gtk+-v3 imake libXres startup-notification xorg-cf-files
#  require gtk+ libXres startup-notification
      opts --enable-static=no
  category graphic/library graphic/toolkit
 attribute gnome library makej multilib-concurrent
#attribute broke
    source $GNOME_URL sources/libwnck/${VERSION:0:3}/libwnck-$VERSION.tar.xz
      info last 20111016
      info home http://www.gnome.org
      desc 'window navigator construction kit'

build(){
 sed -i 's:hardcode_into_libs=yes:hardcode_into_libs=no:' configure
 default_build
}
