     PNAME=${SPELL#perl--}
   VERSION=( "3.21" )
  CATEGORY="perl"
 ATTRIBUTE="perl-module new"
    SOURCE="P/PE/PETEK/$PNAME-$VERSION.tar.gz"
       URL="http://www.cpan.org/authors/id/"
  HOMEPAGE="http://search.cpan.org/search?dist=$PNAME"
       REQ=( "perl perl--HTML-Parser perl--HTML-Tagset" )
  ESTIMATE="55"
      DESC="$SPELL creates and extracts information from HTML syntax trees
HTML::AsSubs
HTML::Element
HTML::Element::traverse
HTML::Parse 
