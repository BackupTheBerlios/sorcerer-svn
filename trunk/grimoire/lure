    version stable 1.1
   category game development/emulator
  attribute archive_off data solo
     source $SOURCEFORGE_URL scummvm/extras/Lure%20of%20the%20Temptress/lure-$VERSION.zip
       info good 20120625
       desc 'Lure of the Temptress (English)'

build(){
 chmod -R 644 */*
 mkdir -pvm 755 $DESTDIR/usr/share/games/lure &&
 cp    -av  *   $DESTDIR/usr/share/games/lure/
}
