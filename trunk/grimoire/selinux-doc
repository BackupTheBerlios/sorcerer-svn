   VERSION=( 1.26 1.24 1.22 1.20 )
  CATEGORY='security'
 ATTRIBUTE='console'
    SOURCE="selinux-doc-$VERSION.tgz"
       URL='http://www.nsa.gov/selinux/archives/'
      VURL='http://www.nsa.gov/selinux/code/download-stable.cfm'
  HOMEPAGE='http://www.nsa.gov/selinux/'
  ESTIMATE='5'
      DESC="selinux-doc provides documentation for SELinux."

build(){
 prepare_install &&
 mkdir -p   /usr/share/SELinux
 cp    -r * /usr/share/SELinux
}
