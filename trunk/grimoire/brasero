   version stable 3.2.0 3.0.0 2.91.93 2.32.1 2.32.0
   require gst-plugins-base libarchive libcanberra nautilus totem-pl-parser
  category disk/cd
 attribute gnome library makej multilib-concurrent
    source $GNOME_URL sources/brasero/${VERSION:0:3}/brasero-$VERSION.tar.bz2
      info last 20111006
  homepage http://www.gnome.org
      desc 'CD/DVD mastering tool'

build(){
 sed -i 's:hardcode_into_libs=yes:hardcode_into_libs=no:' configure
 default_build
}
