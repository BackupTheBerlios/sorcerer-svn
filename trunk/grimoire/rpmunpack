   version stable 0
   require cpio
  category archive
 attribute archive_off console
    source http://www.ibiblio.org/pub/Linux/utils/package/rpmunpack.tar.gz
      info good 20120703
      info last 19980628
      desc 'converts RPMs to cpio archives'

build(){
 make &&
 mkdir -pvm 755 $DESTDIR/usr/bin &&
 install -vs rpmunpack $DESTDIR/usr/bin
}
