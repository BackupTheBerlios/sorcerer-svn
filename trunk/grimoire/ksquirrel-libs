inherit ksquirrel
 ATTRIBUTE='kde3 library'
    SOURCE="ksquirrel/ksquirrel-libs-$VERSION.tar.bz2"
       REQ=( 'OpenEXR giflib'
             'jasper:::for viewing jpeg2000 files'
             'libmng:::for viewing mng files'
             'libwmf:::for viewing wmf files' )
  ESTIMATE='1500'
      DESC='ksquirrel-libs provides image codecs for ksquirrel.'
