   VERSION=( 1.10.2 1.10.1 1.10 1.9.1 1.9 1.8.2 1.8.1 )
      SAFE=( "1.10.2" )
# 1.10.3 will probably be a security release
  CATEGORY='network/ftp network/web'
 ATTRIBUTE='console'
    SOURCE="wget/wget-$VERSION.tar.gz"
  HOMEPAGE='http://sunsite.dk/wget'
       URL="$GNU_URL"
       REQ=( "" "openssl:--with-ssl::for https and ftps" )
  ESTIMATE='103'
      DESC="wget retreives files from web and ftp sites."
