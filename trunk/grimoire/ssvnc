   version stable 1.0.29 1.0.28 1.0.27 1.0.25 1.0.24 1.0.23 1.0.22
   require imake jdk jpeg libXaw openssl xorg-cf-files
  category utility/terminal-emulator
 attribute x11
    source $SOURCEFORGE_URL ssvnc/ssvnc/$VERSION/ssvnc-$VERSION.src.tar.gz
      info last 20111123
      info home http://www.karlrunge.com/x11vnc/ssvnc.html
      desc 'SSL/SSH VNC viewer'

build(){
 make &&
 make install PREFIX=/usr ROOT=$DESTDIR
}

