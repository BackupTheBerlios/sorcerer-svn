# Disable introspection since introspection is broke with clutter

    stable 1.3.1 0.9.4 0.7.2 0.7.1 0.7.0 0.5.1 0.5.0
case $VERSION in
 1.3.1)
      opts --enable-introspection=no ;;
esac
   require clutter gtkmm
  category audio/cd
 attribute gnome
    source $GNOME_URL sources/cluttermm/${VERSION:0:3}/cluttermm-$VERSION.tar.bz2
  homepage http://www.gnome.org
  estimate 570
      desc 'c++ bindings for clutter'
