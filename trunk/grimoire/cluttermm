# Disable introspection since introspection is broke with clutter

   version stable 1.3.1 0.9.4 0.7.2
   require clutter gtkmm
case $VERSION in
 1.3.1)
      opts --enable-introspection=no ;;
esac
  category audio/cd
 attribute gnome library makej multilib-concurrent
    source $GNOME_URL sources/cluttermm/${VERSION:0:3}/cluttermm-$VERSION.tar.bz2
      info home  http://clutter-project.org/
      info docs  http://www.clutter-project.org/documentation
  estimate 570
      desc 'c++ bindings for clutter'
