   VERSION=( "1.1.10" "1.1.8" "1.1.7" "1.1.6" "1.0.4" "1.0.1" )
      SAFE=( "1.1.10" )
  CATEGORY="web"
 ATTRIBUTE="gnome library x11"
    SOURCE="sources/gtkhtml/${VERSION:0:3}/gtkhtml-$VERSION"
       URL="$GNOME_URL"
  HOMEPAGE="http://www.gnome.org"
       REQ="bonobo gal control-center-v1.4 libghttp"
# gtkhtml can be compiled without soup, but evolution requires soup.
 FRESHMEAT="gtkhtml"
  EPROVIDE="gtkhtml,v1.1"
  ESTIMATE="800"
     SHORT="gtkhtml-v1.1 is a light weight html rendering/editing library."
