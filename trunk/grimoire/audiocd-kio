# Nothing in the compile log suggests
# that cdparanoia is only optionally required

   inherit kde-v4
   require cdparanoia libkcddb libkcompactdisc
  category audio audio/cd
 attribute library makej multilib-concurrent
    source   stable $KDE_URL   stable/$VERSION/src/$SPELL-$VERSION.tar.xz
    source unstable $KDE_URL unstable/$VERSION/src/$SPELL-$VERSION.tar.xz
      desc 'audio cd brower application and library for KDE'
