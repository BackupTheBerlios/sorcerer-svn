inherit SearchAndRescue
unset  REQ  SOURCE
 ATTRIBUTE="data"
    SOURCE="SearchAndRescue-data-$VERSION.tar.bz2"
  EPROVIDE="SearchAndRescue,data"
  ESTIMATE="5"

build()  {
  prepare_install  &&
  mkdir  -p      /usr/share/games/SearchAndRescue  &&
  cp     -av  *  /usr/share/games/SearchAndRescue
}
