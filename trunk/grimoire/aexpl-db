   VERSION=( "$( date  -u  +%Y%U  )" )
  CATEGORY="security"
 ATTRIBUTE="console solo depreciated"
# originating project dropped
# database appears to no longer be updating
    SOURCE="exploit.db";  disable  scavenge
#      URL="http://www.h07.org/projects/aexpl/"
# HOMEPAGE="http://www.h07.org/projects/aexpl"
       URL="http://www.hzeroseven.org/projects/aexpl/"
  HOMEPAGE="http://www.hzeroseven.org/projects/aexpl"
       REQ=""
  ESTIMATE="5"
#FRESHMEAT="aexpl"
      DESC="aexpl-db provided a weekly updated database of md5sum identified exploit files.
To make full use of this you should also enable the
aexpl feature within the sorcery options feature menu.
The project providing this service was discontinued.
Therefore, this spell is useless.
Support for aexpl checking was removed from sorcery.
If installed this spell will be automatically added to
the dispel queue following an augur newer or augur newer_ia"


build()  {
  make_install()  {
    mkdir    -p   $(  dirname   $AEXPL_DB  )
    install  -m 0644  $SOURCE   $AEXPL_DB
    echo  "$SOURCE installed to $AEXPL_DB"
  }

  cp  $SOURCE_CACHE/$SPELL/$VERSION/$SOURCE  .  &&
  prepare_install  &&
   make_install
}


current()  {
  echo  "aexpl-db"  |  pipe_queue  "$DISPEL_QUEUE"
}
