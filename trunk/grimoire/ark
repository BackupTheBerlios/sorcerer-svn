   inherit kde-v4
   require kde-baseapps kde-runtime libarchive
  category archive
 attribute library makej multilib-concurrent
    source   stable $KDE_URL   stable/$VERSION/src/${SPELL%-v4}-$VERSION.tar.xz
    source unstable $KDE_URL unstable/$VERSION/src/${SPELL%-v4}-$VERSION.tar.xz
      desc 'kde archiving tool'
