with base    kde-baseapps kde-runtime libarchive
with role    archive
with trait   library makej multilib-concurrent
with source    stable $KDE_URL   stable/$VERSION/src/${SPELL%-v4}-$VERSION.tar.xz
with source  unstable $KDE_URL unstable/$VERSION/src/${SPELL%-v4}-$VERSION.tar.xz
with info    cite 'kde archiving tool'
