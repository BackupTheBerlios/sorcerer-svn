   version stable 1.10
   require libX11
  category utility/time utility/monitor
 attribute x11
    source http://www.pogo.org.uk/~mark/xosview/releases/xosview-$VERSION.tar.gz
      info last 20120922
      info home http://www.pogo.org.uk/~mark/xosview/
      desc 'system monitoring tool'

build(){
 make PLATFORM=linux PREFIX=/usr &&
 make PLATFORM=linux PREFIX=/usr DESTDIR="$DESTDIR" install
}
