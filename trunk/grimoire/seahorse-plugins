# broke because seahorse is broke
   version stable 2.30.1 2.30.0 2.28.1 2.28.0 2.26.2 2.26.1 2.26.0
   require seahorse
  category security/encryption
 attribute x11 gnome
 attribute broke
    source $GNOME_URL sources/seahorse-plugins/${VERSION:0:4}/seahorse-plugins-$VERSION.tar.bz2
      info home http://seahorse.sourceforge.net
      opts --disable-gpg-check
      desc 'plugins for seahorse'
