    stable 0.9.3
   require fftw
  category audio/library
 attribute library
    source http://www.musicip.com/dns/files/libofa-$VERSION.tar.gz
      vurl http://www.musicip.com/dns/downloads.jsp
      opts --enable-static=no
case $VERSION in
 0.9.3) use_gcc gcc-v4.0 ;;
esac
