    stable 0.9.3
   require fftw

  category audio/library
 attribute library new
    source libofa-$VERSION.tar.gz
       url http://www.musicip.com/dns/files/
      vurl http://www.musicip.com/dns/downloads.jsp
      opts --enable-static=no
case $VERSION in
 0.9.3) use_gcc gcc-v4.0 ;;
esac
  homepage http://www.musicip.com/dns/downloads.jsp
  estimate 310
      desc 'libofa provides open fingerprint architecture library.'
