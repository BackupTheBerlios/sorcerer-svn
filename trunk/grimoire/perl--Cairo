# 1.090 is listed as unstable.
# that also complicates downloading.

   version stable 1.082
   require cairo perl--ExtUtils-Depends perl--ExtUtils-ParseXS perl--ExtUtils-PkgConfig perl--Test-Number-Delta
  category graphic
 attribute perl-module library x11
 attribute multilib-fail
    source $SOURCEFORGE_URL gtk2-perl/Cairo/$VERSION/Cairo-$VERSION.tar.gz
      info good 20120629
      info last 20111226
      info home http://gtk2-perl.sourceforge.net
      desc 'perl module wrapper for cairo'
