# 1.090 is listed as unstable.
# that also complicates downloading.

with info    last 20131007
with version stable 1.104 1.103
with base    cairo perl--ExtUtils-Depends perl--ExtUtils-ParseXS perl--ExtUtils-PkgConfig perl--Test-Number-Delta
with role    graphic
with trait   perl-module library x11
with trait   multilib-fail
with source  stable $SOURCEFORGE_URL gtk2-perl/Cairo/$VERSION/Cairo-$VERSION.tar.gz
with info    home http://gtk2-perl.sourceforge.net
with info    cite 'perl module wrapper for cairo'
