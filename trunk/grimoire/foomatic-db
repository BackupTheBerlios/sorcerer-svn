       NOW="$( date           -u +%Y%m01 )"; (( OLD = NOW - 100 ))
       OLD="$( date -d "$OLD" -u +%Y%m01 )"
   VERSION=( "$NOW" "$OLD" )
    IGNORE="engine\|^200"
  CATEGORY="printer"
 ATTRIBUTE="console"
