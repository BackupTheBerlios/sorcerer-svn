  stable 0.26 0.16 0.13 0.11 0.10 0.9 0.8
  CATEGORY='disk/file-system'
 ATTRIBUTE='console new'
    SOURCE="cowdancer_$VERSION.tar.gz"
       URL='http://www.netfort.gr.jp/~dancer/software/downloads/'
  HOMEPAGE='http://www.netfort.gr.jp/~dancer/software/cowdancer.html'
       REQ=''
  ESTIMATE='20'
      DESC="cowdancer implements a copy-on-write overlay filesystem"

build()  {
  sed  -i  "s:-Wall -O2 -g:$CFLAGS:"  Makefile
  make             &&
  prepare_install  &&
  make    install
}
