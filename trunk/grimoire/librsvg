   VERSION=( "2.16.1" "2.16.0" "2.14.4" "2.14.3" "2.14.2" "2.14.1" "2.14.0" )
    IGNORE="2\.17\."
  CATEGORY="graphic"
 ATTRIBUTE="library gnome"
    SOURCE="sources/librsvg/${VERSION%.*}/librsvg-$VERSION.tar.bz2"
       URL="$GNOME_URL"
  HOMEPAGE="http://librsvg.sourceforge.net/"
# libgsf and libgnomeprintui
# removed as requirement to avoid recursive requirement loop
#      REQ="libcroco libgnomeprintui libgsf"
#      REQ="libcroco"
       REQ="gtk+ libcroco"
  ESTIMATE="600"
      DESC="librsvg is a library for rendering vector-graphic files.
librsvg library is a lightweight library for
parsing and rendering vector-graphic files in
SVG format (like the ones made by sodipodi).
It also includes functions that render
anti-aliased fonts using freetype,
including caching of glyphs.
It's used by Nautilus for drawing
vector icons and anti-aliased type."
