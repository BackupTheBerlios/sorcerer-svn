   VERSION=( "0.2" )
  CATEGORY="window-manager"
 ATTRIBUTE="x11"
    SOURCE="enlightenment/e16keyedit-$VERSION.tar.gz"
       URL="$SOURCEFORGE_URL"
  HOMEPAGE="http://enlightenment.sourceforge.net"
       REQ="imlib"
  ESTIMATE="166"
      DESC="e16keyedit is a keybindings editor for e16"

build()  {
  make             &&
  prepare_install  &&
  install  -m 0755  e16keyedit  /usr/bin
}
