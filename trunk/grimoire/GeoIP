   version stable 1.4.8 1.4.7 1.4.6
   require libtool zlib
      opts --enable-shared --enable-static=no
  category network/library
 attribute console library makej multilib-concurrent
    source http://geolite.maxmind.com/download/geoip/api/c/GeoIP-$VERSION.tar.gz
      info last 20110724
      info home http://www.maxmind.com/app/ip-location
      info docs http://www.maxmind.com/app/c
      desc 'GeoIP API, library and database to geolocate IP addresse'

build(){
 case $VERSION in
  1.4.8) libtoolize -cf ;;
 esac
 default_build
}
