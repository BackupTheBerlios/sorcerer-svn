   version stable 4.1.3 4.1.2 0.1.10
   require automake libdvdread
  category video
 attribute library x11
    source http://www1.mplayerhq.hu/MPlayer/releases/dvdnav-old/libdvdnav-$VERSION.tar.bz2
      info good 20120619
      info last 20080907
      info home http://dvd.sourceforge.net/
      desc 'library navigating DVDs'

# build(){
# ./autogen.sh
# default_build
# }

build(){
 mkdir bin
 ln -s /usr/bin/aclocal bin/aclocal-1.11
 ln -s /usr/bin/automake bin/automake-1.11
 local  PATH="$PWD/bin:$PATH"
 export PATH
 autoreconf -if
 default_build
}
