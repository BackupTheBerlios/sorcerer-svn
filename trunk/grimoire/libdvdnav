with version stable 4.1.3 4.1.2 0.1.10
with base    automake libdvdread
with role    video
with trait   library x11
with source  http://www1.mplayerhq.hu/MPlayer/releases/dvdnav-old/libdvdnav-$VERSION.tar.bz2
with info    good 20120619
with info    last 20080907
with info    home http://dvd.sourceforge.net/
with info    cite 'library navigating DVDs'

# build(){
# ./autogen.sh
# default_build
# }

build(){
 mkdir bin
 ln -s /usr/bin/aclocal bin/aclocal-1.11
 ln -s /usr/bin/automake bin/automake-1.11
 local  PATH="$PWD/bin:$PATH"
 export PATH
 autoreconf -if
 default_build
}
