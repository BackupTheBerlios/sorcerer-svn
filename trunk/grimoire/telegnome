   version stable 0.1.0
   require libgnomeui
  category network
 attribute gnome
    source $GNOME_URL sources/telegnome/${VERSION:0:3}/telegnome-$VERSION.tar.bz2
  homepage http://www.gnome.org
      opts --enable-static=no
  estimate 100
      desc 'telegnome displays teletext pages over the Internet.'
