    stable 1.0.3       1.0.1 X11R7.1-1.0.1 X11R7.0-1.0.0
  unstable 1.0.3 1.0.2 1.0.1
    legacy 1.0.1
   require libX11
  category xorg/app
 attribute X11R7 x11
       url $XORG_URL
    source app/rgb-$VERSION.tar.bz2
  estimate 50
      desc 'showrgb uncompiles rgb color-name databases'

build(){
default_build &&
ln -fsv /usr/share/X11/rgb.txt /usr/X11R6/lib/X11/
}
