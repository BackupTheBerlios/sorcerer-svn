    stable 1.0.3       1.0.1 X11R7.1-1.0.1 X11R7.0-1.0.0
  unstable 1.0.4 1.0.3 1.0.2 1.0.1
    legacy 1.0.1
   require libX11
  category xorg/app
 attribute X11R7 x11
    source $XORG_URL app/rgb-$VERSION.tar.bz2
  estimate 50
      desc 'showrgb uncompiles rgb color-name databases'

build(){
mkdir -p /usr/lib/X11 &&
ln -fs /usr/share/X11/rgb.txt $DESTDIR/usr/lib/X11/
}
