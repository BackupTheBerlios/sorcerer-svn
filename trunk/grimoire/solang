   version stable 0.4.1
   require gdl tracker
  category development
 attribute gnome new
    source $GNOME_URL sources/solang/${VERSION:0:3}/solang-$VERSION.tar.bz2
      info home http://projects.gnome.org/solang
      desc 'photo manager that uses tracker as a meta-data store
and acts as a front-end to directory based local storage
as well as remote photo-storage systems'
