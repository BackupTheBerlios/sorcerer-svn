    stable 3.9.8 3.9.7 3.9.6
    secure 3.9.8 3.9.7 3.9.6
   require libpcap
  optional openssl      ''            --without-ssl for ftps and https
  switch               --enable-smb --disable-smb to enable possibly-buggy SMB printer

    ignore '\.pgp'
  category network
 attribute console
    source tcpdump-$VERSION.tar.gz
       url http://www.tcpdump.org/release/
  homepage http://www.tcpdump.org/
  estimate 200
      desc 'tcpdump is CLI packet sniffer network debugging tool'
