# version 2.19.6 requires gtk-engines-2 >= 2.11.0
# neither are stable.
# therefore gnome-themes-extras does not yet compile

    stable 2.22.0 2.20 2.19.6
   require gnome-themes

  category development
#attribute gnome library broke
 attribute gnome library
    source sources/gnome-themes-extras/${VERSION:0:4}/gnome-themes-extras-$VERSION.tar.bz2
  homepage http://www.gnome.org
       url "$GNOME_URL"
  estimate 3000
      desc 'gnome-themes-extras provides extra themes for gnome'
