    stable 3.3.2.p11
    ignore 'p4'
  category network
 attribute console
    source wanpipe-$VERSION.tgz
  homepage http://wiki.sangoma.com
       url ftp://ftp.sangoma.com/
  estimate 200
      desc 'wanpipe provides software to create a Linux based WAN Router'

build(){
[ -x Setup ] &&
prepare_install &&
./Setup install --silent
}
