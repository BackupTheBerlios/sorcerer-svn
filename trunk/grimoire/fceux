   version stable 2.1.5
   require SDL gtk+ scons
  category game development/emulator
 attribute x11
 attribute broke
    source $SOURCEFORGE_URL fceultra/Source%20Code/$VERSION%20src/fceux-$VERSION.src.tar.bz2
#     info good 20120713
      info omit 20130604
      info last 20110604
      info home http://fceux.com/web/home.html
      desc 'Nintendo 8 bit and famicom emulator'

build(){
 scons &&
 mkdir   -pvm 755           $DESTDIR/usr/bin &&
 install  -vm 755 src/fceux $DESTDIR/usr/bin
}
