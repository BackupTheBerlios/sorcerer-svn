    stable 0.8.0
   require gtk+

  category game
 attribute client x11
    source ggmud/GGMud-${VERSION}-src.tar.gz
       url "$SOURCEFORGE_URL"
  homepage http://www.ggsoft.org/ggmud/
  estimate 130
      desc 'GGMud is a gtk+ mud client.'

build(){
 make &&
 prepare_install &&
 install -m 755 ggmud /usr/bin
}
