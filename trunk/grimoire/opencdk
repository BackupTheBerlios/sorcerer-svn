    stable 0.6.6 0.6.5 0.6.4
   require libgcrypt zlib
  category security/encryption
 attribute library
 attribute broke
#   source ftp://ftp.gnutls.org/pub/gnutls/opencdk/opencdk-$VERSION.tar.bz2
       url $GNU_URL
    source gnutls/opencdk-$VERSION.tar.bz2
  homepage http://www.gnupg.org/
      opts --enable-static=no
  estimate 300
      desc 'opencdk provides basic parts of the OpenPGP message format.'

current(){ echo opencdk | pipe_queue $DISPEL_QUEUE; }
