   VERSION=( "1.0.0" )
  CATEGORY="audio-player audio-mixer"
 ATTRIBUTE="library"
    SOURCE="freealut-$VERSION.tar.gz"
  HOMEPAGE="http://www.openal.org/"
       URL="http://www.openal.org/openal_webstf/downloads/"
       REQ=( "openal")
REPOSITORY=( "cvs -d:pserver:guest:guest@opensource.creative.com:/usr/local/cvs-repository login"
            "cvs -d:pserver:guest:guest@opensource.creative.com:/usr/local/cvs-repository co -r HEAD freealut"
            "cvs -d:pserver:guest:guest@opensource.creative.com:/usr/local/cvs-repository update -Pd -r HEAD freealut" )
  ESTIMATE="93"
      DESC="alut library for openal"

build () {
	export LDFLAGS="$LDFLAGS -lopenal"
	default_build
}
