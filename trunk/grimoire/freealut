   version stable 1.1.0 1.0.1
   require openal
      opts --enable-static=no
  category audio/library audio/mixer
 attribute library makej multilib-concurrent x11
    source http://connect.creativelabs.com/openal/Downloads/ALUT/freealut-$VERSION.tar.gz
  homepage http://connect.creativelabs.com/openal/default.aspx
      vurl 'http://connect.creativelabs.com/openal/Downloads/Forms/AllItems.aspx?RootFolder=%2fopenal%2fDownloads%2fALUT'
  estimate 300
      desc 'alut library for openal'

build(){
 rm -f CMakeLists.txt
 default_build
}
