service comsat
{
	socket_type	= dgram
	protocol	= udp
	wait		= no
	user		= nobody
	group		= tty
	server		= /usr/sbin/in.comsat
	server_args	= 
	only_from	= 127.0.0.1
	per_source	= 16
	log_type	= FILE /var/log/xinetd/comsat
	log_on_success	= PID HOST USERID EXIT DURATION
	log_on_failure	= USERID ATTEMPT
}
